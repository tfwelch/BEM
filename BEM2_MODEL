####################################
### BUILDING EMISSIONS MODEL v2.101###
####################################

#Version Date: 12.18.2015
# Coded by: Tim Welch, PEER Group, LLC.


#!!!!!!!!USER INPUT NEEDED!!!!!!!!#
#It is recommended that you place BEM2 in a root directory - if yoy have permission

#USER: change model directory changing D: to C:, F:, E:, etc.
MAINPATH <- file.path("D:", "BEM2.1")

#GET EXTERNAL COMMANDS
options(echo=TRUE) 
args <- commandArgs(trailingOnly = TRUE)
print(args)
BASEYEAR <- as.numeric(args[1])
PROJYEAR <- as.numeric(args[2])

#Set input/output folders - No User Input Needed
INPATH <- file.path(MAINPATH,"INPUT","" )
OUTPATH <- file.path(MAINPATH,"OUTPUT","" )
TEMPATH <- file.path(MAINPATH,"TEMP","" )

#Set parameters
Parameters <- read.csv(paste(MAINPATH,"/PARAMETERS/Parameters.csv",sep="")) #Model Parameters
DDCCRAW  <- read.csv(paste(MAINPATH,"/PARAMETERS/DDCC.csv",sep="")) #Climate Change Parameters
SCENARIO <- Parameters$Scenario
#BASEYEAR <- Parameters$BaseYear
#PROJYEAR <- Parameters$ProjectionYear
TIME <- PROJYEAR-BASEYEAR #Calculate Time Span
DDCCTARGET <- Parameters$TempratureChange
DDCCRESULT <- subset(DDCCRAW,Degrees==DDCCTARGET)
MD_COM_CO2con <- 4.59
PA_COM_CO2con <- 11.78
VA_COM_CO2con <- 5.02
WV_COM_CO2con <- 1.59
DE_COM_CO2con <- 0.73
DC_COM_CO2con <- 1.10
 
MD_RES_CO2con <- 5.90
PA_RES_CO2con <- 20.26
VA_RES_CO2con <- 6.96
WV_RES_CO2con <- 2.00
DE_RES_CO2con <- 0.99
DC_RES_CO2con <- 0.7
 
# Read in data files
Base_SMZ_Activities <- read.csv(paste(INPATH,sprintf("activitiesBySilo_%s.csv", BASEYEAR), sep="")) #Base Activities File
names(Base_SMZ_Activities) <- c("SMZ_N","ACRES","HH_BASE","ENR_BASE","RE_BASE","OFF_BASE","OTH_BASE","TOTEMP_BASE") #rename headings
Proj_SMZ_Activities <- read.csv(paste(INPATH,sprintf("activitiesBySilo_%s.csv", PROJYEAR), sep="")) #Forecast Activities File
names(Proj_SMZ_Activities) <- c("SMZ_N","ACRES","HH_PROJ","ENR_PROJ","RE_PROJ","OFF_PROJ","OTH_PROJ","TOTEMP_PROJ") #rename headings
SMZ_HDD <- read.csv(paste(INPATH,"SMZ_climate_lookup.csv",sep="")) #base Housing Parcel data
CBECS <- read.csv(paste(INPATH,"cbecs_data.csv",sep="")) #Observed Commercial Emissions
RECS  <- read.csv(paste(INPATH,"recs_data.csv",sep="")) #Observed Residential Emissions
MD_Com_Base  <- read.csv(paste(INPATH,"MD_Base_Com_Data.csv",sep="")) #Base Commercial Parcel data
VA_Com_Base  <- read.csv(paste(INPATH,"VA_Base_Com_Data.csv",sep="")) #Base Commercial Parcel data
PA_Com_Base  <- read.csv(paste(INPATH,"PA_Base_Com_Data.csv",sep="")) #Base Commercial Parcel data
DC_Com_Base  <- read.csv(paste(INPATH,"DC_Base_Com_Data.csv",sep="")) #Base Commercial Parcel data
DE_Com_Base  <- read.csv(paste(INPATH,"DE_Base_Com_Data.csv",sep="")) #Base Commercial Parcel data
WV_Com_Base  <- read.csv(paste(INPATH,"WV_Base_Com_Data.csv",sep="")) #Base Commercial Parcel data
MD_Res_Base  <- read.csv(paste(INPATH,"MD_Base_Res_Data.csv",sep="")) #base Housing Parcel data
VA_Res_Base  <- read.csv(paste(INPATH,"VA_Base_Res_Data.csv",sep="")) #base Housing Parcel data
PA_Res_Base  <- read.csv(paste(INPATH,"PA_Base_Res_Data.csv",sep="")) #base Housing Parcel data
DC_Res_Base  <- read.csv(paste(INPATH,"DC_Base_Res_Data.csv",sep="")) #base Housing Parcel data
DE_Res_Base  <- read.csv(paste(INPATH,"DE_Base_Res_Data.csv",sep="")) #base Housing Parcel data
WV_Res_Base  <- read.csv(paste(INPATH,"WV_Base_Res_Data.csv",sep="")) #base Housing Parcel data
BlankSMZ <- read.csv(paste(INPATH,"SMZ_Numbers.csv",sep="")) #Blank SMZ Number File 

###Take out the trash
file.remove(paste(TEMPATH,"RES_CO2_SAMPLE.csv",sep=""))
file.remove(paste(TEMPATH,"COM_CO2_SAMPLE.csv",sep=""))

##############
###Begin BUILDING CO2 MODEL
##############

####Prep Varible for CO2 Binary
CBECS$CO2_PROD <- NA
CBECS$CO2_PROD[CBECS$TCO2>0] <- 1
CBECS$CO2_PROD[CBECS$TCO2==0] <- 0

###Calculate Building Age
CBECS$Age <- NA
CBECS$Age <-2006-CBECS$YRCON8

#Convert from buildings to workers
CBECS$TCO2Work <- CBECS$TCO2/CBECS$Workers

###Replace Errors with Zero
CBECS[is.na(CBECS)] <- 0
CBECS[CBECS==-Inf] <- 0
CBECS[CBECS==Inf] <- 0

##### Select observations based on CO2 Emissions Greater than zero
CBECS2 <- CBECS[which(CBECS$TCO2>0),]

#####Fit Com_ols
Com_ols <- lm(TCO2Work ~ HDD658 + Age + SQFT8, data = CBECS2)
summary(Com_ols)

#####START MD COM CO2####

##### Estimate Commercial Emissions
MD_Com_Base$CO2_EST <- Com_ols$coef[1]+
					  (Com_ols$coef[2])*MD_Com_Base$HDD658 +
					  (Com_ols$coef[3])*MD_Com_Base$Age +
					  (Com_ols$coef[4])*MD_Com_Base$SQFTSTRC					  

###Calculate Emissions per Worker
MD_Com_Base$CO2_RE<- MD_Com_Base$CO2_EST*MD_Com_Base$RETAIL
MD_Com_Base$CO2_OFF<- MD_Com_Base$CO2_EST*MD_Com_Base$Office
MD_Com_Base$CO2_OTH<- MD_Com_Base$CO2_EST*MD_Com_Base$Other

###Calculate Number of Workers BY Sector
MD_Com_Base$Workers_RE<- MD_Com_Base$Workers*MD_Com_Base$RETAIL
MD_Com_Base$Workers_OFF<- MD_Com_Base$Workers*MD_Com_Base$Office
MD_Com_Base$Workers_OTH<- MD_Com_Base$Workers*MD_Com_Base$Other
				
####Aggregate data By SMZ by sector
MD_Com_Base_SMZ <-aggregate(cbind(MD_Com_Base$CO2_EST, MD_Com_Base$CO2_RE,MD_Com_Base$CO2_OFF, MD_Com_Base$CO2_OTH, MD_Com_Base$Workers_RE,MD_Com_Base$Workers_OFF, MD_Com_Base$Workers_OTH)~MD_Com_Base$SMZ_N, data=MD_Com_Base, sum, na.rm=TRUE)
names(MD_Com_Base_SMZ)[1] <- "SMZ_N" 
names(MD_Com_Base_SMZ)[2] <- "CO2_EST" 
names(MD_Com_Base_SMZ)[3] <- "CO2_EST_RE" 
names(MD_Com_Base_SMZ)[4] <- "CO2_EST_OFF"
names(MD_Com_Base_SMZ)[5] <- "CO2_EST_OTH"
names(MD_Com_Base_SMZ)[6] <- "CO2_Workers_RE" 
names(MD_Com_Base_SMZ)[7] <- "CO2_Workers_OFF"
names(MD_Com_Base_SMZ)[8] <- "CO2_Workers_OTH"

MD_Com_Base_SMZ_SQFT <-aggregate(cbind(MD_Com_Base$SQFTSTRC)~MD_Com_Base$SMZ_N, data=MD_Com_Base, mean, na.rm=TRUE) 
names(MD_Com_Base_SMZ_SQFT)[1] <- "SMZ_N" 
names(MD_Com_Base_SMZ_SQFT)[2] <- "SQFT" 

MD_Com_Base_SMZ$SQFT = MD_Com_Base_SMZ_SQFT[match(MD_Com_Base_SMZ$SMZ_N, MD_Com_Base_SMZ_SQFT$SMZ_N),"SQFT"] 

###Rate Per Worker Per Sector
MD_Com_Base_SMZ$CO2_Rate_RE <- MD_Com_Base_SMZ$CO2_EST_RE/MD_Com_Base_SMZ$CO2_Workers_RE
MD_Com_Base_SMZ$CO2_Rate_OFF <- MD_Com_Base_SMZ$CO2_EST_OFF/MD_Com_Base_SMZ$CO2_Workers_OFF
MD_Com_Base_SMZ$CO2_Rate_OTH <- MD_Com_Base_SMZ$CO2_EST_OTH/MD_Com_Base_SMZ$CO2_Workers_OTH

# merge Rates and Activities
MD_Com_Base_Activities_SMZ = merge(MD_Com_Base_SMZ, Base_SMZ_Activities, by="SMZ_N")

###Apply Rates to Activities
MD_Com_Base_Activities_SMZ$CO2_base_RE <- MD_Com_Base_Activities_SMZ$CO2_Rate_RE*MD_Com_Base_Activities_SMZ$RE_BASE
MD_Com_Base_Activities_SMZ$CO2_base_OFF <- MD_Com_Base_Activities_SMZ$CO2_Rate_OFF*MD_Com_Base_Activities_SMZ$OFF_BASE
MD_Com_Base_Activities_SMZ$CO2_base_OTH <- MD_Com_Base_Activities_SMZ$CO2_Rate_OTH*MD_Com_Base_Activities_SMZ$OTH_BASE

###Replace Errors with Zero
MD_Com_Base_Activities_SMZ[is.na(MD_Com_Base_Activities_SMZ)] <- 0
MD_Com_Base_Activities_SMZ[MD_Com_Base_Activities_SMZ==-Inf] <- 0
MD_Com_Base_Activities_SMZ[MD_Com_Base_Activities_SMZ==Inf] <- 0

###Emission Rate Adjustment Factor
MD_Com_Base_Activities_SMZ$Tot_SEC_CO2 <- MD_Com_Base_Activities_SMZ$CO2_base_RE + MD_Com_Base_Activities_SMZ$CO2_base_OFF + MD_Com_Base_Activities_SMZ$CO2_base_OTH
Tot_BASE_Com_CO2<- sum(MD_Com_Base_Activities_SMZ$Tot_SEC_CO2, na.rm = TRUE)*0.000001
CO2_Com_Rate_Adj<-(MD_COM_CO2con/Tot_BASE_Com_CO2)

###Adjusted Rate Per Worker Per Sector
MD_Com_Base_Activities_SMZ$CO2_Rate_RE_ADJ <- MD_Com_Base_Activities_SMZ$CO2_Rate_RE*CO2_Com_Rate_Adj
MD_Com_Base_Activities_SMZ$CO2_Rate_OFF_ADJ <- MD_Com_Base_Activities_SMZ$CO2_Rate_OFF*CO2_Com_Rate_Adj
MD_Com_Base_Activities_SMZ$CO2_Rate_OTH_ADJ <- MD_Com_Base_Activities_SMZ$CO2_Rate_OTH*CO2_Com_Rate_Adj

###Apply Adjusted Rates to Activities
MD_Com_Base_Activities_SMZ$CO2_base_RE <- MD_Com_Base_Activities_SMZ$CO2_Rate_RE_ADJ*MD_Com_Base_Activities_SMZ$RE_BASE
MD_Com_Base_Activities_SMZ$CO2_base_OFF <- MD_Com_Base_Activities_SMZ$CO2_Rate_OFF_ADJ*MD_Com_Base_Activities_SMZ$OFF_BASE
MD_Com_Base_Activities_SMZ$CO2_base_OTH <- MD_Com_Base_Activities_SMZ$CO2_Rate_OTH_ADJ*MD_Com_Base_Activities_SMZ$OTH_BASE

###Sum Total Base Emissions
MD_Com_Base_Activities_SMZ$CO2_base_Com_Total <- MD_Com_Base_Activities_SMZ$CO2_base_RE + MD_Com_Base_Activities_SMZ$CO2_base_OFF + MD_Com_Base_Activities_SMZ$CO2_base_OTH

#Merge Base Data, Rates and Projected Activities
MD_Com_Base_Activities_SMZ_PROJ = merge(MD_Com_Base_Activities_SMZ, Proj_SMZ_Activities, by="SMZ_N")

###Calculate Projected Growth
PROJ_GROWTH_RE <- data.frame(MD_Com_Base_Activities_SMZ_PROJ$SMZ_N, (MD_Com_Base_Activities_SMZ_PROJ$RE_PROJ-MD_Com_Base_Activities_SMZ_PROJ$RE_BASE))
names(PROJ_GROWTH_RE) <- c("SMZ_N","PROJ_GROWTH_RE")
PROJ_GROWTH_OFF <- data.frame(MD_Com_Base_Activities_SMZ_PROJ$SMZ_N, (MD_Com_Base_Activities_SMZ_PROJ$OFF_PROJ-MD_Com_Base_Activities_SMZ_PROJ$OFF_BASE))
names(PROJ_GROWTH_OFF) <- c("SMZ_N","PROJ_GROWTH_OFF")
PROJ_GROWTH_OTH <- data.frame(MD_Com_Base_Activities_SMZ_PROJ$SMZ_N, (MD_Com_Base_Activities_SMZ_PROJ$OTH_PROJ-MD_Com_Base_Activities_SMZ_PROJ$OTH_BASE))
names(PROJ_GROWTH_OTH) <- c("SMZ_N","PROJ_GROWTH_OTH")
PROJ_GROWTH_EMP <- data.frame(MD_Com_Base_Activities_SMZ_PROJ$SMZ_N, (PROJ_GROWTH_RE$PROJ_GROWTH_RE + PROJ_GROWTH_OFF$PROJ_GROWTH_OFF + PROJ_GROWTH_OTH$PROJ_GROWTH_OTH))
names(PROJ_GROWTH_EMP) <- c("SMZ_N","PROJ_GROWTH_EMP")

############
###Simulate New Commercial Stock
############
MD_Com_SMZ_SIM <- PROJ_GROWTH_EMP

#Attach HDD Data
MD_Com_SMZ_SIM$HDD = SMZ_HDD[match(MD_Com_SMZ_SIM$SMZ_N, SMZ_HDD$SMZ_N),"HDD"] 

#Sampling Function
sampEMP<-function(x){

sample(PROJ_GROWTH_EMP + 1, 1)

#Sim commercial Data
MD_Com_SMZ_SIM$Age <- sapply(MD_Com_SMZ_SIM$PROJ_GROWTH_EMP*TIME + 1, sample, 1) - 1
MD_Com_SMZ_SIM$SQFTSTRC <- sapply(MD_Com_SMZ_SIM$PROJ_GROWTH_EMP*MD_Com_Base_SMZ$SQFT + 1, sample, 1) - 1


##### Estimate New Stock Commercial Emissions
MD_Com_SMZ_SIM$CO2_EST <- Com_ols$coef[1]+
					  (Com_ols$coef[2])*(MD_Com_SMZ_SIM$HDD*DDCCRESULT$HDD) +
					  (Com_ols$coef[3])*(MD_Com_SMZ_SIM$Age) +
					  (Com_ols$coef[4])*MD_Com_SMZ_SIM$SQFTSTRC
					  
###Apply Emissions Adjustment to Projected New Stock
MD_Com_SMZ_SIM$CO2_EST_ADJ <- MD_Com_SMZ_SIM$CO2_EST*CO2_Com_Rate_Adj
write.table(MD_Com_SMZ_SIM[c("SMZ_N","CO2_EST_ADJ")],file=(paste(TEMPATH,"COM_CO2_SAMPLE.csv",sep="")), sep=",", row.names=FALSE, col.names=FALSE, append=TRUE)
}

replicate(n = 10000, expr = sampEMP())

MD_Com_SMZ_SIM_RESULT<- read.csv(paste(TEMPATH,"COM_CO2_SAMPLE.csv",sep="")) #Read back in simulated housing stock
names(MD_Com_SMZ_SIM_RESULT) <- c("SMZ_N","CO2_EST_ADJ")

####Aggregate CO2 data in simulated commercial stock By SMZ
MD_Com_SMZ_SIM_RESULT_AGG <-aggregate(cbind(MD_Com_SMZ_SIM_RESULT$CO2_EST_ADJ)~MD_Com_SMZ_SIM_RESULT$SMZ_N, data=MD_Com_SMZ_SIM_RESULT, mean, na.rm=FALSE)
names(MD_Com_SMZ_SIM_RESULT_AGG)[1] <- "SMZ_N" 
names(MD_Com_SMZ_SIM_RESULT_AGG)[2] <- "CO2_EST_ADJ" 

###########
###Age Base Commercial Stock
############
MD_Com_Base_AGED <- MD_Com_Base

MD_Com_Base_AGED$CO2_EST <- Com_ols$coef[1]+
					  (Com_ols$coef[2])*(MD_Com_Base$HDD*DDCCRESULT$HDD) +
					  (Com_ols$coef[3])*(MD_Com_Base$Age + TIME) +
					  (Com_ols$coef[4])*MD_Com_Base$SQFTSTRC

###Calculate Emissions BY Sector per Worker
MD_Com_Base_AGED$CO2_RE <- MD_Com_Base_AGED$CO2_EST*MD_Com_Base_AGED$RETAIL
MD_Com_Base_AGED$CO2_OFF<- MD_Com_Base_AGED$CO2_EST*MD_Com_Base_AGED$Office
MD_Com_Base_AGED$CO2_OTH<- MD_Com_Base_AGED$CO2_EST*MD_Com_Base_AGED$Other

###Calculate Number of Workers BY Sector
MD_Com_Base_AGED$Workers_RE<- MD_Com_Base_AGED$Workers*MD_Com_Base_AGED$RETAIL
MD_Com_Base_AGED$Workers_OFF<- MD_Com_Base_AGED$Workers*MD_Com_Base_AGED$Office
MD_Com_Base_AGED$Workers_OTH<- MD_Com_Base_AGED$Workers*MD_Com_Base_AGED$Other
				
####Aggregate data By SMZ by sector
MD_Com_Base_SMZ_AGED <-aggregate(cbind(MD_Com_Base_AGED$CO2_EST, MD_Com_Base_AGED$CO2_RE,MD_Com_Base_AGED$CO2_OFF, MD_Com_Base_AGED$CO2_OTH, MD_Com_Base_AGED$Workers_RE,MD_Com_Base_AGED$Workers_OFF, MD_Com_Base_AGED$Workers_OTH)~MD_Com_Base_AGED$SMZ_N, data=MD_Com_Base_AGED, sum, na.rm=TRUE)
names(MD_Com_Base_SMZ_AGED)[1] <- "SMZ_N" 
names(MD_Com_Base_SMZ_AGED)[2] <- "CO2_EST" 
names(MD_Com_Base_SMZ_AGED)[3] <- "CO2_EST_RE" 
names(MD_Com_Base_SMZ_AGED)[4] <- "CO2_EST_OFF"
names(MD_Com_Base_SMZ_AGED)[5] <- "CO2_EST_OTH"
names(MD_Com_Base_SMZ_AGED)[6] <- "CO2_Workers_RE" 
names(MD_Com_Base_SMZ_AGED)[7] <- "CO2_Workers_OFF"
names(MD_Com_Base_SMZ_AGED)[8] <- "CO2_Workers_OTH"

###Aged Rate Per Worker Per Sector
MD_Com_Base_SMZ_AGED$CO2_Rate_RE <- MD_Com_Base_SMZ_AGED$CO2_EST_RE/MD_Com_Base_SMZ_AGED$CO2_Workers_RE
MD_Com_Base_SMZ_AGED$CO2_Rate_OFF <- MD_Com_Base_SMZ_AGED$CO2_EST_OFF/MD_Com_Base_SMZ_AGED$CO2_Workers_OFF
MD_Com_Base_SMZ_AGED$CO2_Rate_OTH <- MD_Com_Base_SMZ_AGED$CO2_EST_OTH/MD_Com_Base_SMZ_AGED$CO2_Workers_OTH

# Merge Rates and Activities for Aged Commercial Stock
MD_Com_Base_Activities_SMZ_AGED = merge(MD_Com_Base_SMZ_AGED, Base_SMZ_Activities, by="SMZ_N")

##Apply New Rates to Aged Workers
MD_Com_Base_Activities_SMZ_AGED$CO2_base_RE <- MD_Com_Base_Activities_SMZ_AGED$CO2_Rate_RE*MD_Com_Base_Activities_SMZ_AGED$RE_BASE
MD_Com_Base_Activities_SMZ_AGED$CO2_base_OFF <- MD_Com_Base_Activities_SMZ_AGED$CO2_Rate_OFF*MD_Com_Base_Activities_SMZ_AGED$OFF_BASE
MD_Com_Base_Activities_SMZ_AGED$CO2_base_OTH <- MD_Com_Base_Activities_SMZ_AGED$CO2_Rate_OTH*MD_Com_Base_Activities_SMZ_AGED$OTH_BASE

###Replace Errors with Zero					  
MD_Com_Base_Activities_SMZ_AGED[is.na(MD_Com_Base_Activities_SMZ_AGED)] <- 0
MD_Com_Base_Activities_SMZ_AGED[MD_Com_Base_Activities_SMZ_AGED==-Inf] <- 0
MD_Com_Base_Activities_SMZ_AGED[MD_Com_Base_Activities_SMZ_AGED==Inf] <- 0					  

###Adjusted CO2 Rate Per Aged Worker
MD_Com_Base_Activities_SMZ_AGED$CO2_Rate_RE_ADJ <- MD_Com_Base_Activities_SMZ_AGED$CO2_Rate_RE*CO2_Com_Rate_Adj
MD_Com_Base_Activities_SMZ_AGED$CO2_Rate_OFF_ADJ <- MD_Com_Base_Activities_SMZ_AGED$CO2_Rate_OFF*CO2_Com_Rate_Adj					  
MD_Com_Base_Activities_SMZ_AGED$CO2_Rate_OTH_ADJ <- MD_Com_Base_Activities_SMZ_AGED$CO2_Rate_OTH*CO2_Com_Rate_Adj
					  
###Apply Adjusted Aged Rates to Activities
MD_Com_Base_Activities_SMZ_AGED$CO2_base_RE_AGED <- MD_Com_Base_Activities_SMZ_AGED$CO2_Rate_RE_ADJ*MD_Com_Base_Activities_SMZ_AGED$RE_BASE
MD_Com_Base_Activities_SMZ_AGED$CO2_base_OTH_AGED <- MD_Com_Base_Activities_SMZ_AGED$CO2_Rate_OTH_ADJ*MD_Com_Base_Activities_SMZ_AGED$OTH_BASE
MD_Com_Base_Activities_SMZ_AGED$CO2_base_OFF_AGED <- MD_Com_Base_Activities_SMZ_AGED$CO2_Rate_OFF_ADJ*MD_Com_Base_Activities_SMZ_AGED$OFF_BASE
MD_Com_Base_Activities_SMZ_AGED$CO2_base_Com_AGED <- MD_Com_Base_Activities_SMZ_AGED$CO2_base_RE_AGED + MD_Com_Base_Activities_SMZ_AGED$CO2_base_OFF_AGED + MD_Com_Base_Activities_SMZ_AGED$CO2_base_OTH_AGED
###Sum Total Aged Base Emissions
MD_Com_Base_Activities_SMZ_AGED$CO2_base_Com_Total_AGED <- MD_Com_Base_Activities_SMZ_AGED$CO2_base_RE_AGED + MD_Com_Base_Activities_SMZ_AGED$CO2_base_OFF_AGED + MD_Com_Base_Activities_SMZ_AGED$CO2_base_OTH_AGED
					  
###Add Aged Base Emissions to Projected Emissions Growth
MD_Com_SMZ_SIM_RESULT_AGG$CO2_base_Com_AGED = MD_Com_Base_Activities_SMZ_AGED[match(MD_Com_SMZ_SIM_RESULT_AGG$SMZ_N, MD_Com_Base_Activities_SMZ_AGED$SMZ_N),"CO2_base_Com_AGED"]

###Add Base Emissions to Projected Emissions Growth
MD_Com_SMZ_SIM_RESULT_AGG$CO2_base_Com = MD_Com_Base_Activities_SMZ[match(MD_Com_SMZ_SIM_RESULT_AGG$SMZ_N, MD_Com_Base_Activities_SMZ$SMZ_N),"CO2_base_Com_Total"]

MD_Com_SMZ_SIM_RESULT_AGG$CO2_Proj_Com <- MD_Com_SMZ_SIM_RESULT_AGG$CO2_base_Com_AGED + MD_Com_SMZ_SIM_RESULT_AGG$CO2_EST_ADJ

###Clean Final Data Set
MD_Com_SMZ_SIM_RESULT_AGG[is.na(MD_Com_SMZ_SIM_RESULT_AGG)] <- 0

###Write Final CO2 Output
write.csv(MD_Com_SMZ_SIM_RESULT_AGG[,c("SMZ_N","CO2_base_Com","CO2_Proj_Com")], file=(paste(OUTPATH,"MD_BEM_Com_CO2.csv",sep="")),row.names=FALSE)

###Take out the trash
file.remove(paste(TEMPATH,"COM_CO2_SAMPLE.csv",sep=""))
	
####END MD COM CO2

#####START VA_COM CO2####

##### Estimate Commercial Emissions
VA_Com_Base$CO2_EST <- Com_ols$coef[1]+
					  (Com_ols$coef[2])*VA_Com_Base$HDD658 +
					  (Com_ols$coef[3])*VA_Com_Base$Age +
					  (Com_ols$coef[4])*VA_Com_Base$SQFTSTRC					  

###Calculate Emissions per Worker
VA_Com_Base$CO2_RE<- VA_Com_Base$CO2_EST*VA_Com_Base$RETAIL
VA_Com_Base$CO2_OFF<- VA_Com_Base$CO2_EST*VA_Com_Base$Office
VA_Com_Base$CO2_OTH<- VA_Com_Base$CO2_EST*VA_Com_Base$Other

###Calculate Number of Workers BY Sector
VA_Com_Base$Workers<-1
VA_Com_Base$Workers_RE<- VA_Com_Base$Workers*VA_Com_Base$RETAIL
VA_Com_Base$Workers_OFF<- VA_Com_Base$Workers*VA_Com_Base$Office
VA_Com_Base$Workers_OTH<- VA_Com_Base$Workers*VA_Com_Base$Other
				
####Aggregate data By SMZ by sector
VA_Com_Base_SMZ <-aggregate(cbind(VA_Com_Base$CO2_EST, VA_Com_Base$CO2_RE,VA_Com_Base$CO2_OFF, VA_Com_Base$CO2_OTH, VA_Com_Base$Workers_RE,VA_Com_Base$Workers_OFF, VA_Com_Base$Workers_OTH)~VA_Com_Base$SMZ_N, data=VA_Com_Base, sum, na.rm=TRUE)
names(VA_Com_Base_SMZ)[1] <- "SMZ_N" 
names(VA_Com_Base_SMZ)[2] <- "CO2_EST" 
names(VA_Com_Base_SMZ)[3] <- "CO2_EST_RE" 
names(VA_Com_Base_SMZ)[4] <- "CO2_EST_OFF"
names(VA_Com_Base_SMZ)[5] <- "CO2_EST_OTH"
names(VA_Com_Base_SMZ)[6] <- "CO2_Workers_RE" 
names(VA_Com_Base_SMZ)[7] <- "CO2_Workers_OFF"
names(VA_Com_Base_SMZ)[8] <- "CO2_Workers_OTH"

VA_Com_Base_SMZ_SQFT <-aggregate(cbind(VA_Com_Base$SQFTSTRC)~VA_Com_Base$SMZ_N, data=VA_Com_Base, mean, na.rm=TRUE) 
names(VA_Com_Base_SMZ_SQFT)[1] <- "SMZ_N" 
names(VA_Com_Base_SMZ_SQFT)[2] <- "SQFT" 

VA_Com_Base_SMZ$SQFT = VA_Com_Base_SMZ_SQFT[match(VA_Com_Base_SMZ$SMZ_N, VA_Com_Base_SMZ_SQFT$SMZ_N),"SQFT"] 

###Rate Per Worker Per Sector
VA_Com_Base_SMZ$CO2_Rate_RE <- VA_Com_Base_SMZ$CO2_EST_RE/VA_Com_Base_SMZ$CO2_Workers_RE
VA_Com_Base_SMZ$CO2_Rate_OFF <- VA_Com_Base_SMZ$CO2_EST_OFF/VA_Com_Base_SMZ$CO2_Workers_OFF
VA_Com_Base_SMZ$CO2_Rate_OTH <- VA_Com_Base_SMZ$CO2_EST_OTH/VA_Com_Base_SMZ$CO2_Workers_OTH

# merge Rates and Activities
VA_Com_Base_Activities_SMZ = merge(VA_Com_Base_SMZ, Base_SMZ_Activities, by="SMZ_N")

###Apply Rates to Activities
VA_Com_Base_Activities_SMZ$CO2_base_RE <- VA_Com_Base_Activities_SMZ$CO2_Rate_RE*VA_Com_Base_Activities_SMZ$RE_BASE
VA_Com_Base_Activities_SMZ$CO2_base_OFF <- VA_Com_Base_Activities_SMZ$CO2_Rate_OFF*VA_Com_Base_Activities_SMZ$OFF_BASE
VA_Com_Base_Activities_SMZ$CO2_base_OTH <- VA_Com_Base_Activities_SMZ$CO2_Rate_OTH*VA_Com_Base_Activities_SMZ$OTH_BASE

###Replace Errors with Zero
VA_Com_Base_Activities_SMZ[is.na(VA_Com_Base_Activities_SMZ)] <- 0
VA_Com_Base_Activities_SMZ[VA_Com_Base_Activities_SMZ==-Inf] <- 0
VA_Com_Base_Activities_SMZ[VA_Com_Base_Activities_SMZ==Inf] <- 0

###Emission Rate Adjustment Factor
VA_Com_Base_Activities_SMZ$Tot_SEC_CO2 <- VA_Com_Base_Activities_SMZ$CO2_base_RE + VA_Com_Base_Activities_SMZ$CO2_base_OFF + VA_Com_Base_Activities_SMZ$CO2_base_OTH
Tot_BASE_Com_CO2<- sum(VA_Com_Base_Activities_SMZ$Tot_SEC_CO2, na.rm = TRUE)*0.000001
CO2_Com_Rate_Adj<-(VA_COM_CO2con/Tot_BASE_Com_CO2)

###Adjusted Rate Per Worker Per Sector
VA_Com_Base_Activities_SMZ$CO2_Rate_RE_ADJ <- VA_Com_Base_Activities_SMZ$CO2_Rate_RE*CO2_Com_Rate_Adj
VA_Com_Base_Activities_SMZ$CO2_Rate_OFF_ADJ <- VA_Com_Base_Activities_SMZ$CO2_Rate_OFF*CO2_Com_Rate_Adj
VA_Com_Base_Activities_SMZ$CO2_Rate_OTH_ADJ <- VA_Com_Base_Activities_SMZ$CO2_Rate_OTH*CO2_Com_Rate_Adj

###Apply Adjusted Rates to Activities
VA_Com_Base_Activities_SMZ$CO2_base_RE <- VA_Com_Base_Activities_SMZ$CO2_Rate_RE_ADJ*VA_Com_Base_Activities_SMZ$RE_BASE
VA_Com_Base_Activities_SMZ$CO2_base_OFF <- VA_Com_Base_Activities_SMZ$CO2_Rate_OFF_ADJ*VA_Com_Base_Activities_SMZ$OFF_BASE
VA_Com_Base_Activities_SMZ$CO2_base_OTH <- VA_Com_Base_Activities_SMZ$CO2_Rate_OTH_ADJ*VA_Com_Base_Activities_SMZ$OTH_BASE

###Sum Total Base Emissions
VA_Com_Base_Activities_SMZ$CO2_base_Com_Total <- VA_Com_Base_Activities_SMZ$CO2_base_RE + VA_Com_Base_Activities_SMZ$CO2_base_OFF + VA_Com_Base_Activities_SMZ$CO2_base_OTH

#Merge Base Data, Rates and Projected Activities
VA_Com_Base_Activities_SMZ_PROJ = merge(VA_Com_Base_Activities_SMZ, Proj_SMZ_Activities, by="SMZ_N")

###Calculate Projected Growth
PROJ_GROWTH_RE <- data.frame(VA_Com_Base_Activities_SMZ_PROJ$SMZ_N, (VA_Com_Base_Activities_SMZ_PROJ$RE_PROJ-VA_Com_Base_Activities_SMZ_PROJ$RE_BASE))
names(PROJ_GROWTH_RE) <- c("SMZ_N","PROJ_GROWTH_RE")
PROJ_GROWTH_OFF <- data.frame(VA_Com_Base_Activities_SMZ_PROJ$SMZ_N, (VA_Com_Base_Activities_SMZ_PROJ$OFF_PROJ-VA_Com_Base_Activities_SMZ_PROJ$OFF_BASE))
names(PROJ_GROWTH_OFF) <- c("SMZ_N","PROJ_GROWTH_OFF")
PROJ_GROWTH_OTH <- data.frame(VA_Com_Base_Activities_SMZ_PROJ$SMZ_N, (VA_Com_Base_Activities_SMZ_PROJ$OTH_PROJ-VA_Com_Base_Activities_SMZ_PROJ$OTH_BASE))
names(PROJ_GROWTH_OTH) <- c("SMZ_N","PROJ_GROWTH_OTH")
PROJ_GROWTH_EMP <- data.frame(VA_Com_Base_Activities_SMZ_PROJ$SMZ_N, (PROJ_GROWTH_RE$PROJ_GROWTH_RE + PROJ_GROWTH_OFF$PROJ_GROWTH_OFF + PROJ_GROWTH_OTH$PROJ_GROWTH_OTH))
names(PROJ_GROWTH_EMP) <- c("SMZ_N","PROJ_GROWTH_EMP")

############
###Simulate New Commercial Stock
############
VA_Com_SMZ_SIM <- PROJ_GROWTH_EMP

#Attach HDD Data
VA_Com_SMZ_SIM$HDD = SMZ_HDD[match(VA_Com_SMZ_SIM$SMZ_N, SMZ_HDD$SMZ_N),"HDD"] 

#Sampling Function
sampEMP<-function(x){

sample(PROJ_GROWTH_EMP + 1, 1)

#Sim commercial Data
VA_Com_SMZ_SIM$Age <- sapply(VA_Com_SMZ_SIM$PROJ_GROWTH_EMP*TIME + 1, sample, 1) - 1
VA_Com_SMZ_SIM$SQFTSTRC <- sapply(VA_Com_SMZ_SIM$PROJ_GROWTH_EMP*VA_Com_Base_SMZ$SQFT + 1, sample, 1) - 1


##### Estimate New Stock Commercial Emissions
VA_Com_SMZ_SIM$CO2_EST <- Com_ols$coef[1]+
					  (Com_ols$coef[2])*(VA_Com_SMZ_SIM$HDD*DDCCRESULT$HDD) +
					  (Com_ols$coef[3])*(VA_Com_SMZ_SIM$Age) +
					  (Com_ols$coef[4])*VA_Com_SMZ_SIM$SQFTSTRC
					  
###Apply Emissions Adjustment to Projected New Stock
VA_Com_SMZ_SIM$CO2_EST_ADJ <- VA_Com_SMZ_SIM$CO2_EST*CO2_Com_Rate_Adj
write.table(VA_Com_SMZ_SIM[c("SMZ_N","CO2_EST_ADJ")],file=(paste(TEMPATH,"COM_CO2_SAMPLE.csv",sep="")), sep=",", row.names=FALSE, col.names=FALSE, append=TRUE)
}

replicate(n = 10000, expr = sampEMP())

VA_Com_SMZ_SIM_RESULT<- read.csv(paste(TEMPATH,"COM_CO2_SAMPLE.csv",sep="")) #Read back in simulated housing stock
names(VA_Com_SMZ_SIM_RESULT) <- c("SMZ_N","CO2_EST_ADJ")

####Aggregate CO2 data in simulated commercial stock By SMZ
VA_Com_SMZ_SIM_RESULT_AGG <-aggregate(cbind(VA_Com_SMZ_SIM_RESULT$CO2_EST_ADJ)~VA_Com_SMZ_SIM_RESULT$SMZ_N, data=VA_Com_SMZ_SIM_RESULT, mean, na.rm=FALSE)
names(VA_Com_SMZ_SIM_RESULT_AGG)[1] <- "SMZ_N" 
names(VA_Com_SMZ_SIM_RESULT_AGG)[2] <- "CO2_EST_ADJ" 

###########
###Age Base Commercial Stock
############
VA_Com_Base_AGED <- VA_Com_Base

VA_Com_Base_AGED$CO2_EST <- Com_ols$coef[1]+
					  (Com_ols$coef[2])*(VA_Com_Base$HDD*DDCCRESULT$HDD) +
					  (Com_ols$coef[3])*(VA_Com_Base$Age + TIME) +
					  (Com_ols$coef[4])*VA_Com_Base$SQFTSTRC

###Calculate Emissions BY Sector per Worker
VA_Com_Base_AGED$CO2_RE <- VA_Com_Base_AGED$CO2_EST*VA_Com_Base_AGED$RETAIL
VA_Com_Base_AGED$CO2_OFF<- VA_Com_Base_AGED$CO2_EST*VA_Com_Base_AGED$Office
VA_Com_Base_AGED$CO2_OTH<- VA_Com_Base_AGED$CO2_EST*VA_Com_Base_AGED$Other

###Calculate Number of Workers BY Sector
VA_Com_Base_AGED$Workers_RE<- VA_Com_Base_AGED$Workers*VA_Com_Base_AGED$RETAIL
VA_Com_Base_AGED$Workers_OFF<- VA_Com_Base_AGED$Workers*VA_Com_Base_AGED$Office
VA_Com_Base_AGED$Workers_OTH<- VA_Com_Base_AGED$Workers*VA_Com_Base_AGED$Other
				
####Aggregate data By SMZ by sector
VA_Com_Base_SMZ_AGED <-aggregate(cbind(VA_Com_Base_AGED$CO2_EST, VA_Com_Base_AGED$CO2_RE,VA_Com_Base_AGED$CO2_OFF, VA_Com_Base_AGED$CO2_OTH, VA_Com_Base_AGED$Workers_RE,VA_Com_Base_AGED$Workers_OFF, VA_Com_Base_AGED$Workers_OTH)~VA_Com_Base_AGED$SMZ_N, data=VA_Com_Base_AGED, sum, na.rm=TRUE)
names(VA_Com_Base_SMZ_AGED)[1] <- "SMZ_N" 
names(VA_Com_Base_SMZ_AGED)[2] <- "CO2_EST" 
names(VA_Com_Base_SMZ_AGED)[3] <- "CO2_EST_RE" 
names(VA_Com_Base_SMZ_AGED)[4] <- "CO2_EST_OFF"
names(VA_Com_Base_SMZ_AGED)[5] <- "CO2_EST_OTH"
names(VA_Com_Base_SMZ_AGED)[6] <- "CO2_Workers_RE" 
names(VA_Com_Base_SMZ_AGED)[7] <- "CO2_Workers_OFF"
names(VA_Com_Base_SMZ_AGED)[8] <- "CO2_Workers_OTH"

###Aged Rate Per Worker Per Sector
VA_Com_Base_SMZ_AGED$CO2_Rate_RE <- VA_Com_Base_SMZ_AGED$CO2_EST_RE/VA_Com_Base_SMZ_AGED$CO2_Workers_RE
VA_Com_Base_SMZ_AGED$CO2_Rate_OFF <- VA_Com_Base_SMZ_AGED$CO2_EST_OFF/VA_Com_Base_SMZ_AGED$CO2_Workers_OFF
VA_Com_Base_SMZ_AGED$CO2_Rate_OTH <- VA_Com_Base_SMZ_AGED$CO2_EST_OTH/VA_Com_Base_SMZ_AGED$CO2_Workers_OTH

# Merge Rates and Activities for Aged Commercial Stock
VA_Com_Base_Activities_SMZ_AGED = merge(VA_Com_Base_SMZ_AGED, Base_SMZ_Activities, by="SMZ_N")

##Apply New Rates to Aged Workers
VA_Com_Base_Activities_SMZ_AGED$CO2_base_RE <- VA_Com_Base_Activities_SMZ_AGED$CO2_Rate_RE*VA_Com_Base_Activities_SMZ_AGED$RE_BASE
VA_Com_Base_Activities_SMZ_AGED$CO2_base_OFF <- VA_Com_Base_Activities_SMZ_AGED$CO2_Rate_OFF*VA_Com_Base_Activities_SMZ_AGED$OFF_BASE
VA_Com_Base_Activities_SMZ_AGED$CO2_base_OTH <- VA_Com_Base_Activities_SMZ_AGED$CO2_Rate_OTH*VA_Com_Base_Activities_SMZ_AGED$OTH_BASE

###Replace Errors with Zero					  
VA_Com_Base_Activities_SMZ_AGED[is.na(VA_Com_Base_Activities_SMZ_AGED)] <- 0
VA_Com_Base_Activities_SMZ_AGED[VA_Com_Base_Activities_SMZ_AGED==-Inf] <- 0
VA_Com_Base_Activities_SMZ_AGED[VA_Com_Base_Activities_SMZ_AGED==Inf] <- 0					  

###Adjusted CO2 Rate Per Aged Worker
VA_Com_Base_Activities_SMZ_AGED$CO2_Rate_RE_ADJ <- VA_Com_Base_Activities_SMZ_AGED$CO2_Rate_RE*CO2_Com_Rate_Adj
VA_Com_Base_Activities_SMZ_AGED$CO2_Rate_OFF_ADJ <- VA_Com_Base_Activities_SMZ_AGED$CO2_Rate_OFF*CO2_Com_Rate_Adj					  
VA_Com_Base_Activities_SMZ_AGED$CO2_Rate_OTH_ADJ <- VA_Com_Base_Activities_SMZ_AGED$CO2_Rate_OTH*CO2_Com_Rate_Adj
					  
###Apply Adjusted Aged Rates to Activities
VA_Com_Base_Activities_SMZ_AGED$CO2_base_RE_AGED <- VA_Com_Base_Activities_SMZ_AGED$CO2_Rate_RE_ADJ*VA_Com_Base_Activities_SMZ_AGED$RE_BASE
VA_Com_Base_Activities_SMZ_AGED$CO2_base_OTH_AGED <- VA_Com_Base_Activities_SMZ_AGED$CO2_Rate_OTH_ADJ*VA_Com_Base_Activities_SMZ_AGED$OTH_BASE
VA_Com_Base_Activities_SMZ_AGED$CO2_base_OFF_AGED <- VA_Com_Base_Activities_SMZ_AGED$CO2_Rate_OFF_ADJ*VA_Com_Base_Activities_SMZ_AGED$OFF_BASE
VA_Com_Base_Activities_SMZ_AGED$CO2_base_Com_AGED <- VA_Com_Base_Activities_SMZ_AGED$CO2_base_RE_AGED + VA_Com_Base_Activities_SMZ_AGED$CO2_base_OFF_AGED + VA_Com_Base_Activities_SMZ_AGED$CO2_base_OTH_AGED
###Sum Total Aged Base Emissions
VA_Com_Base_Activities_SMZ_AGED$CO2_base_Com_Total_AGED <- VA_Com_Base_Activities_SMZ_AGED$CO2_base_RE_AGED + VA_Com_Base_Activities_SMZ_AGED$CO2_base_OFF_AGED + VA_Com_Base_Activities_SMZ_AGED$CO2_base_OTH_AGED
					  
###Add Aged Base Emissions to Projected Emissions Growth
VA_Com_SMZ_SIM_RESULT_AGG$CO2_base_Com_AGED = VA_Com_Base_Activities_SMZ_AGED[match(VA_Com_SMZ_SIM_RESULT_AGG$SMZ_N, VA_Com_Base_Activities_SMZ_AGED$SMZ_N),"CO2_base_Com_AGED"]

###Add Base Emissions to Projected Emissions Growth
VA_Com_SMZ_SIM_RESULT_AGG$CO2_base_Com = VA_Com_Base_Activities_SMZ[match(VA_Com_SMZ_SIM_RESULT_AGG$SMZ_N, VA_Com_Base_Activities_SMZ$SMZ_N),"CO2_base_Com_Total"]

VA_Com_SMZ_SIM_RESULT_AGG$CO2_Proj_Com <- VA_Com_SMZ_SIM_RESULT_AGG$CO2_base_Com_AGED + VA_Com_SMZ_SIM_RESULT_AGG$CO2_EST_ADJ

###Clean Final Data Set
VA_Com_SMZ_SIM_RESULT_AGG[is.na(VA_Com_SMZ_SIM_RESULT_AGG)] <- 0

###Write Final CO2 Output
write.csv(VA_Com_SMZ_SIM_RESULT_AGG[,c("SMZ_N","CO2_base_Com","CO2_Proj_Com")], file=(paste(OUTPATH,"VA_BEM_Com_CO2.csv",sep="")),row.names=FALSE)

###Take out the trash
file.remove(paste(TEMPATH,"COM_CO2_SAMPLE.csv",sep=""))
	
####END VA_COM CO2	
	

#####START PA_COM CO2####

##### Estimate Commercial Emissions
PA_Com_Base$CO2_EST <- Com_ols$coef[1]+
					  (Com_ols$coef[2])*PA_Com_Base$HDD658 +
					  (Com_ols$coef[3])*PA_Com_Base$Age +
					  (Com_ols$coef[4])*PA_Com_Base$SQFTSTRC					  

###Calculate Emissions per Worker
PA_Com_Base$CO2_RE<- PA_Com_Base$CO2_EST*PA_Com_Base$RETAIL
PA_Com_Base$CO2_OFF<- PA_Com_Base$CO2_EST*PA_Com_Base$Office
PA_Com_Base$CO2_OTH<- PA_Com_Base$CO2_EST*PA_Com_Base$Other

###Calculate Number of Workers BY Sector
PA_Com_Base$Workers<-1
PA_Com_Base$Workers_RE<- PA_Com_Base$Workers*PA_Com_Base$RETAIL
PA_Com_Base$Workers_OFF<- PA_Com_Base$Workers*PA_Com_Base$Office
PA_Com_Base$Workers_OTH<- PA_Com_Base$Workers*PA_Com_Base$Other
				
####Aggregate data By SMZ by sector
PA_Com_Base_SMZ <-aggregate(cbind(PA_Com_Base$CO2_EST, PA_Com_Base$CO2_RE,PA_Com_Base$CO2_OFF, PA_Com_Base$CO2_OTH, PA_Com_Base$Workers_RE,PA_Com_Base$Workers_OFF, PA_Com_Base$Workers_OTH)~PA_Com_Base$SMZ_N, data=PA_Com_Base, sum, na.rm=TRUE)
names(PA_Com_Base_SMZ)[1] <- "SMZ_N" 
names(PA_Com_Base_SMZ)[2] <- "CO2_EST" 
names(PA_Com_Base_SMZ)[3] <- "CO2_EST_RE" 
names(PA_Com_Base_SMZ)[4] <- "CO2_EST_OFF"
names(PA_Com_Base_SMZ)[5] <- "CO2_EST_OTH"
names(PA_Com_Base_SMZ)[6] <- "CO2_Workers_RE" 
names(PA_Com_Base_SMZ)[7] <- "CO2_Workers_OFF"
names(PA_Com_Base_SMZ)[8] <- "CO2_Workers_OTH"

PA_Com_Base_SMZ_SQFT <-aggregate(cbind(PA_Com_Base$SQFTSTRC)~PA_Com_Base$SMZ_N, data=PA_Com_Base, mean, na.rm=TRUE) 
names(PA_Com_Base_SMZ_SQFT)[1] <- "SMZ_N" 
names(PA_Com_Base_SMZ_SQFT)[2] <- "SQFT" 

PA_Com_Base_SMZ$SQFT = PA_Com_Base_SMZ_SQFT[match(PA_Com_Base_SMZ$SMZ_N, PA_Com_Base_SMZ_SQFT$SMZ_N),"SQFT"] 

###Rate Per Worker Per Sector
PA_Com_Base_SMZ$CO2_Rate_RE <- PA_Com_Base_SMZ$CO2_EST_RE/PA_Com_Base_SMZ$CO2_Workers_RE
PA_Com_Base_SMZ$CO2_Rate_OFF <- PA_Com_Base_SMZ$CO2_EST_OFF/PA_Com_Base_SMZ$CO2_Workers_OFF
PA_Com_Base_SMZ$CO2_Rate_OTH <- PA_Com_Base_SMZ$CO2_EST_OTH/PA_Com_Base_SMZ$CO2_Workers_OTH

# merge Rates and Activities
PA_Com_Base_Activities_SMZ = merge(PA_Com_Base_SMZ, Base_SMZ_Activities, by="SMZ_N")

###Apply Rates to Activities
PA_Com_Base_Activities_SMZ$CO2_base_RE <- PA_Com_Base_Activities_SMZ$CO2_Rate_RE*PA_Com_Base_Activities_SMZ$RE_BASE
PA_Com_Base_Activities_SMZ$CO2_base_OFF <- PA_Com_Base_Activities_SMZ$CO2_Rate_OFF*PA_Com_Base_Activities_SMZ$OFF_BASE
PA_Com_Base_Activities_SMZ$CO2_base_OTH <- PA_Com_Base_Activities_SMZ$CO2_Rate_OTH*PA_Com_Base_Activities_SMZ$OTH_BASE

###Replace Errors with Zero
PA_Com_Base_Activities_SMZ[is.na(PA_Com_Base_Activities_SMZ)] <- 0
PA_Com_Base_Activities_SMZ[PA_Com_Base_Activities_SMZ==-Inf] <- 0
PA_Com_Base_Activities_SMZ[PA_Com_Base_Activities_SMZ==Inf] <- 0

###Emission Rate Adjustment Factor
PA_Com_Base_Activities_SMZ$Tot_SEC_CO2 <- PA_Com_Base_Activities_SMZ$CO2_base_RE + PA_Com_Base_Activities_SMZ$CO2_base_OFF + PA_Com_Base_Activities_SMZ$CO2_base_OTH
Tot_BASE_Com_CO2<- sum(PA_Com_Base_Activities_SMZ$Tot_SEC_CO2, na.rm = TRUE)*0.000001
CO2_Com_Rate_Adj<-(PA_COM_CO2con/Tot_BASE_Com_CO2)

###Adjusted Rate Per Worker Per Sector
PA_Com_Base_Activities_SMZ$CO2_Rate_RE_ADJ <- PA_Com_Base_Activities_SMZ$CO2_Rate_RE*CO2_Com_Rate_Adj
PA_Com_Base_Activities_SMZ$CO2_Rate_OFF_ADJ <- PA_Com_Base_Activities_SMZ$CO2_Rate_OFF*CO2_Com_Rate_Adj
PA_Com_Base_Activities_SMZ$CO2_Rate_OTH_ADJ <- PA_Com_Base_Activities_SMZ$CO2_Rate_OTH*CO2_Com_Rate_Adj

###Apply Adjusted Rates to Activities
PA_Com_Base_Activities_SMZ$CO2_base_RE <- PA_Com_Base_Activities_SMZ$CO2_Rate_RE_ADJ*PA_Com_Base_Activities_SMZ$RE_BASE
PA_Com_Base_Activities_SMZ$CO2_base_OFF <- PA_Com_Base_Activities_SMZ$CO2_Rate_OFF_ADJ*PA_Com_Base_Activities_SMZ$OFF_BASE
PA_Com_Base_Activities_SMZ$CO2_base_OTH <- PA_Com_Base_Activities_SMZ$CO2_Rate_OTH_ADJ*PA_Com_Base_Activities_SMZ$OTH_BASE

###Sum Total Base Emissions
PA_Com_Base_Activities_SMZ$CO2_base_Com_Total <- PA_Com_Base_Activities_SMZ$CO2_base_RE + PA_Com_Base_Activities_SMZ$CO2_base_OFF + PA_Com_Base_Activities_SMZ$CO2_base_OTH

#Merge Base Data, Rates and Projected Activities
PA_Com_Base_Activities_SMZ_PROJ = merge(PA_Com_Base_Activities_SMZ, Proj_SMZ_Activities, by="SMZ_N")

###Calculate Projected Growth
PROJ_GROWTH_RE <- data.frame(PA_Com_Base_Activities_SMZ_PROJ$SMZ_N, (PA_Com_Base_Activities_SMZ_PROJ$RE_PROJ-PA_Com_Base_Activities_SMZ_PROJ$RE_BASE))
names(PROJ_GROWTH_RE) <- c("SMZ_N","PROJ_GROWTH_RE")
PROJ_GROWTH_OFF <- data.frame(PA_Com_Base_Activities_SMZ_PROJ$SMZ_N, (PA_Com_Base_Activities_SMZ_PROJ$OFF_PROJ-PA_Com_Base_Activities_SMZ_PROJ$OFF_BASE))
names(PROJ_GROWTH_OFF) <- c("SMZ_N","PROJ_GROWTH_OFF")
PROJ_GROWTH_OTH <- data.frame(PA_Com_Base_Activities_SMZ_PROJ$SMZ_N, (PA_Com_Base_Activities_SMZ_PROJ$OTH_PROJ-PA_Com_Base_Activities_SMZ_PROJ$OTH_BASE))
names(PROJ_GROWTH_OTH) <- c("SMZ_N","PROJ_GROWTH_OTH")
PROJ_GROWTH_EMP <- data.frame(PA_Com_Base_Activities_SMZ_PROJ$SMZ_N, (PROJ_GROWTH_RE$PROJ_GROWTH_RE + PROJ_GROWTH_OFF$PROJ_GROWTH_OFF + PROJ_GROWTH_OTH$PROJ_GROWTH_OTH))
names(PROJ_GROWTH_EMP) <- c("SMZ_N","PROJ_GROWTH_EMP")

############
###Simulate New Commercial Stock
############
PA_Com_SMZ_SIM <- PROJ_GROWTH_EMP

#Attach HDD Data
PA_Com_SMZ_SIM$HDD = SMZ_HDD[match(PA_Com_SMZ_SIM$SMZ_N, SMZ_HDD$SMZ_N),"HDD"] 

#Sampling Function
sampEMP<-function(x){

sample(PROJ_GROWTH_EMP + 1, 1)

#Sim commercial Data
PA_Com_SMZ_SIM$Age <- sapply(PA_Com_SMZ_SIM$PROJ_GROWTH_EMP*TIME + 1, sample, 1) - 1
PA_Com_SMZ_SIM$SQFTSTRC <- sapply(PA_Com_SMZ_SIM$PROJ_GROWTH_EMP*PA_Com_Base_SMZ$SQFT + 1, sample, 1) - 1


##### Estimate New Stock Commercial Emissions
PA_Com_SMZ_SIM$CO2_EST <- Com_ols$coef[1]+
					  (Com_ols$coef[2])*(PA_Com_SMZ_SIM$HDD*DDCCRESULT$HDD) +
					  (Com_ols$coef[3])*(PA_Com_SMZ_SIM$Age) +
					  (Com_ols$coef[4])*PA_Com_SMZ_SIM$SQFTSTRC
					  
###Apply Emissions Adjustment to Projected New Stock
PA_Com_SMZ_SIM$CO2_EST_ADJ <- PA_Com_SMZ_SIM$CO2_EST*CO2_Com_Rate_Adj
write.table(PA_Com_SMZ_SIM[c("SMZ_N","CO2_EST_ADJ")],file=(paste(TEMPATH,"COM_CO2_SAMPLE.csv",sep="")), sep=",", row.names=FALSE, col.names=FALSE, append=TRUE)
}

replicate(n = 10000, expr = sampEMP())

PA_Com_SMZ_SIM_RESULT<- read.csv(paste(TEMPATH,"COM_CO2_SAMPLE.csv",sep="")) #Read back in simulated housing stock
names(PA_Com_SMZ_SIM_RESULT) <- c("SMZ_N","CO2_EST_ADJ")

####Aggregate CO2 data in simulated commercial stock By SMZ
PA_Com_SMZ_SIM_RESULT_AGG <-aggregate(cbind(PA_Com_SMZ_SIM_RESULT$CO2_EST_ADJ)~PA_Com_SMZ_SIM_RESULT$SMZ_N, data=PA_Com_SMZ_SIM_RESULT, mean, na.rm=FALSE)
names(PA_Com_SMZ_SIM_RESULT_AGG)[1] <- "SMZ_N" 
names(PA_Com_SMZ_SIM_RESULT_AGG)[2] <- "CO2_EST_ADJ" 

###########
###Age Base Commercial Stock
############
PA_Com_Base_AGED <- PA_Com_Base

PA_Com_Base_AGED$CO2_EST <- Com_ols$coef[1]+
					  (Com_ols$coef[2])*(PA_Com_Base$HDD*DDCCRESULT$HDD) +
					  (Com_ols$coef[3])*(PA_Com_Base$Age + TIME) +
					  (Com_ols$coef[4])*PA_Com_Base$SQFTSTRC

###Calculate Emissions BY Sector per Worker
PA_Com_Base_AGED$CO2_RE <- PA_Com_Base_AGED$CO2_EST*PA_Com_Base_AGED$RETAIL
PA_Com_Base_AGED$CO2_OFF<- PA_Com_Base_AGED$CO2_EST*PA_Com_Base_AGED$Office
PA_Com_Base_AGED$CO2_OTH<- PA_Com_Base_AGED$CO2_EST*PA_Com_Base_AGED$Other

###Calculate Number of Workers BY Sector
PA_Com_Base_AGED$Workers_RE<- PA_Com_Base_AGED$Workers*PA_Com_Base_AGED$RETAIL
PA_Com_Base_AGED$Workers_OFF<- PA_Com_Base_AGED$Workers*PA_Com_Base_AGED$Office
PA_Com_Base_AGED$Workers_OTH<- PA_Com_Base_AGED$Workers*PA_Com_Base_AGED$Other
				
####Aggregate data By SMZ by sector
PA_Com_Base_SMZ_AGED <-aggregate(cbind(PA_Com_Base_AGED$CO2_EST, PA_Com_Base_AGED$CO2_RE,PA_Com_Base_AGED$CO2_OFF, PA_Com_Base_AGED$CO2_OTH, PA_Com_Base_AGED$Workers_RE,PA_Com_Base_AGED$Workers_OFF, PA_Com_Base_AGED$Workers_OTH)~PA_Com_Base_AGED$SMZ_N, data=PA_Com_Base_AGED, sum, na.rm=TRUE)
names(PA_Com_Base_SMZ_AGED)[1] <- "SMZ_N" 
names(PA_Com_Base_SMZ_AGED)[2] <- "CO2_EST" 
names(PA_Com_Base_SMZ_AGED)[3] <- "CO2_EST_RE" 
names(PA_Com_Base_SMZ_AGED)[4] <- "CO2_EST_OFF"
names(PA_Com_Base_SMZ_AGED)[5] <- "CO2_EST_OTH"
names(PA_Com_Base_SMZ_AGED)[6] <- "CO2_Workers_RE" 
names(PA_Com_Base_SMZ_AGED)[7] <- "CO2_Workers_OFF"
names(PA_Com_Base_SMZ_AGED)[8] <- "CO2_Workers_OTH"

###Aged Rate Per Worker Per Sector
PA_Com_Base_SMZ_AGED$CO2_Rate_RE <- PA_Com_Base_SMZ_AGED$CO2_EST_RE/PA_Com_Base_SMZ_AGED$CO2_Workers_RE
PA_Com_Base_SMZ_AGED$CO2_Rate_OFF <- PA_Com_Base_SMZ_AGED$CO2_EST_OFF/PA_Com_Base_SMZ_AGED$CO2_Workers_OFF
PA_Com_Base_SMZ_AGED$CO2_Rate_OTH <- PA_Com_Base_SMZ_AGED$CO2_EST_OTH/PA_Com_Base_SMZ_AGED$CO2_Workers_OTH

# Merge Rates and Activities for Aged Commercial Stock
PA_Com_Base_Activities_SMZ_AGED = merge(PA_Com_Base_SMZ_AGED, Base_SMZ_Activities, by="SMZ_N")

##Apply New Rates to Aged Workers
PA_Com_Base_Activities_SMZ_AGED$CO2_base_RE <- PA_Com_Base_Activities_SMZ_AGED$CO2_Rate_RE*PA_Com_Base_Activities_SMZ_AGED$RE_BASE
PA_Com_Base_Activities_SMZ_AGED$CO2_base_OFF <- PA_Com_Base_Activities_SMZ_AGED$CO2_Rate_OFF*PA_Com_Base_Activities_SMZ_AGED$OFF_BASE
PA_Com_Base_Activities_SMZ_AGED$CO2_base_OTH <- PA_Com_Base_Activities_SMZ_AGED$CO2_Rate_OTH*PA_Com_Base_Activities_SMZ_AGED$OTH_BASE

###Replace Errors with Zero					  
PA_Com_Base_Activities_SMZ_AGED[is.na(PA_Com_Base_Activities_SMZ_AGED)] <- 0
PA_Com_Base_Activities_SMZ_AGED[PA_Com_Base_Activities_SMZ_AGED==-Inf] <- 0
PA_Com_Base_Activities_SMZ_AGED[PA_Com_Base_Activities_SMZ_AGED==Inf] <- 0					  

###Adjusted CO2 Rate Per Aged Worker
PA_Com_Base_Activities_SMZ_AGED$CO2_Rate_RE_ADJ <- PA_Com_Base_Activities_SMZ_AGED$CO2_Rate_RE*CO2_Com_Rate_Adj
PA_Com_Base_Activities_SMZ_AGED$CO2_Rate_OFF_ADJ <- PA_Com_Base_Activities_SMZ_AGED$CO2_Rate_OFF*CO2_Com_Rate_Adj					  
PA_Com_Base_Activities_SMZ_AGED$CO2_Rate_OTH_ADJ <- PA_Com_Base_Activities_SMZ_AGED$CO2_Rate_OTH*CO2_Com_Rate_Adj
					  
###Apply Adjusted Aged Rates to Activities
PA_Com_Base_Activities_SMZ_AGED$CO2_base_RE_AGED <- PA_Com_Base_Activities_SMZ_AGED$CO2_Rate_RE_ADJ*PA_Com_Base_Activities_SMZ_AGED$RE_BASE
PA_Com_Base_Activities_SMZ_AGED$CO2_base_OTH_AGED <- PA_Com_Base_Activities_SMZ_AGED$CO2_Rate_OTH_ADJ*PA_Com_Base_Activities_SMZ_AGED$OTH_BASE
PA_Com_Base_Activities_SMZ_AGED$CO2_base_OFF_AGED <- PA_Com_Base_Activities_SMZ_AGED$CO2_Rate_OFF_ADJ*PA_Com_Base_Activities_SMZ_AGED$OFF_BASE
PA_Com_Base_Activities_SMZ_AGED$CO2_base_Com_AGED <- PA_Com_Base_Activities_SMZ_AGED$CO2_base_RE_AGED + PA_Com_Base_Activities_SMZ_AGED$CO2_base_OFF_AGED + PA_Com_Base_Activities_SMZ_AGED$CO2_base_OTH_AGED
###Sum Total Aged Base Emissions
PA_Com_Base_Activities_SMZ_AGED$CO2_base_Com_Total_AGED <- PA_Com_Base_Activities_SMZ_AGED$CO2_base_RE_AGED + PA_Com_Base_Activities_SMZ_AGED$CO2_base_OFF_AGED + PA_Com_Base_Activities_SMZ_AGED$CO2_base_OTH_AGED
					  
###Add Aged Base Emissions to Projected Emissions Growth
PA_Com_SMZ_SIM_RESULT_AGG$CO2_base_Com_AGED = PA_Com_Base_Activities_SMZ_AGED[match(PA_Com_SMZ_SIM_RESULT_AGG$SMZ_N, PA_Com_Base_Activities_SMZ_AGED$SMZ_N),"CO2_base_Com_AGED"]

###Add Base Emissions to Projected Emissions Growth
PA_Com_SMZ_SIM_RESULT_AGG$CO2_base_Com = PA_Com_Base_Activities_SMZ[match(PA_Com_SMZ_SIM_RESULT_AGG$SMZ_N, PA_Com_Base_Activities_SMZ$SMZ_N),"CO2_base_Com_Total"]

PA_Com_SMZ_SIM_RESULT_AGG$CO2_Proj_Com <- PA_Com_SMZ_SIM_RESULT_AGG$CO2_base_Com_AGED + PA_Com_SMZ_SIM_RESULT_AGG$CO2_EST_ADJ

###Clean Final Data Set
PA_Com_SMZ_SIM_RESULT_AGG[is.na(PA_Com_SMZ_SIM_RESULT_AGG)] <- 0

###Write Final CO2 Output
write.csv(PA_Com_SMZ_SIM_RESULT_AGG[,c("SMZ_N","CO2_base_Com","CO2_Proj_Com")], file=(paste(OUTPATH,"PA_BEM_Com_CO2.csv",sep="")),row.names=FALSE)

###Take out the trash
file.remove(paste(TEMPATH,"COM_CO2_SAMPLE.csv",sep=""))
	
####END PA_COM CO2	

#####START DC_COM CO2####

##### Estimate Commercial Emissions
DC_Com_Base$CO2_EST <- Com_ols$coef[1]+
					  (Com_ols$coef[2])*DC_Com_Base$HDD658 +
					  (Com_ols$coef[3])*DC_Com_Base$Age +
					  (Com_ols$coef[4])*DC_Com_Base$SQFTSTRC					  

###Calculate Emissions per Worker
DC_Com_Base$CO2_RE<- DC_Com_Base$CO2_EST*DC_Com_Base$RETAIL
DC_Com_Base$CO2_OFF<- DC_Com_Base$CO2_EST*DC_Com_Base$Office
DC_Com_Base$CO2_OTH<- DC_Com_Base$CO2_EST*DC_Com_Base$Other

###Calculate Number of Workers BY Sector
DC_Com_Base$Workers<-1
DC_Com_Base$Workers_RE<- DC_Com_Base$Workers*DC_Com_Base$RETAIL
DC_Com_Base$Workers_OFF<- DC_Com_Base$Workers*DC_Com_Base$Office
DC_Com_Base$Workers_OTH<- DC_Com_Base$Workers*DC_Com_Base$Other
				
####Aggregate data By SMZ by sector
DC_Com_Base_SMZ <-aggregate(cbind(DC_Com_Base$CO2_EST, DC_Com_Base$CO2_RE,DC_Com_Base$CO2_OFF, DC_Com_Base$CO2_OTH, DC_Com_Base$Workers_RE,DC_Com_Base$Workers_OFF, DC_Com_Base$Workers_OTH)~DC_Com_Base$SMZ_N, data=DC_Com_Base, sum, na.rm=TRUE)
names(DC_Com_Base_SMZ)[1] <- "SMZ_N" 
names(DC_Com_Base_SMZ)[2] <- "CO2_EST" 
names(DC_Com_Base_SMZ)[3] <- "CO2_EST_RE" 
names(DC_Com_Base_SMZ)[4] <- "CO2_EST_OFF"
names(DC_Com_Base_SMZ)[5] <- "CO2_EST_OTH"
names(DC_Com_Base_SMZ)[6] <- "CO2_Workers_RE" 
names(DC_Com_Base_SMZ)[7] <- "CO2_Workers_OFF"
names(DC_Com_Base_SMZ)[8] <- "CO2_Workers_OTH"

DC_Com_Base_SMZ_SQFT <-aggregate(cbind(DC_Com_Base$SQFTSTRC)~DC_Com_Base$SMZ_N, data=DC_Com_Base, mean, na.rm=TRUE) 
names(DC_Com_Base_SMZ_SQFT)[1] <- "SMZ_N" 
names(DC_Com_Base_SMZ_SQFT)[2] <- "SQFT" 

DC_Com_Base_SMZ$SQFT = DC_Com_Base_SMZ_SQFT[match(DC_Com_Base_SMZ$SMZ_N, DC_Com_Base_SMZ_SQFT$SMZ_N),"SQFT"] 

###Rate Per Worker Per Sector
DC_Com_Base_SMZ$CO2_Rate_RE <- DC_Com_Base_SMZ$CO2_EST_RE/DC_Com_Base_SMZ$CO2_Workers_RE
DC_Com_Base_SMZ$CO2_Rate_OFF <- DC_Com_Base_SMZ$CO2_EST_OFF/DC_Com_Base_SMZ$CO2_Workers_OFF
DC_Com_Base_SMZ$CO2_Rate_OTH <- DC_Com_Base_SMZ$CO2_EST_OTH/DC_Com_Base_SMZ$CO2_Workers_OTH

# merge Rates and Activities
DC_Com_Base_Activities_SMZ = merge(DC_Com_Base_SMZ, Base_SMZ_Activities, by="SMZ_N")

###Apply Rates to Activities
DC_Com_Base_Activities_SMZ$CO2_base_RE <- DC_Com_Base_Activities_SMZ$CO2_Rate_RE*DC_Com_Base_Activities_SMZ$RE_BASE
DC_Com_Base_Activities_SMZ$CO2_base_OFF <- DC_Com_Base_Activities_SMZ$CO2_Rate_OFF*DC_Com_Base_Activities_SMZ$OFF_BASE
DC_Com_Base_Activities_SMZ$CO2_base_OTH <- DC_Com_Base_Activities_SMZ$CO2_Rate_OTH*DC_Com_Base_Activities_SMZ$OTH_BASE

###Replace Errors with Zero
DC_Com_Base_Activities_SMZ[is.na(DC_Com_Base_Activities_SMZ)] <- 0
DC_Com_Base_Activities_SMZ[DC_Com_Base_Activities_SMZ==-Inf] <- 0
DC_Com_Base_Activities_SMZ[DC_Com_Base_Activities_SMZ==Inf] <- 0

###Emission Rate Adjustment Factor
DC_Com_Base_Activities_SMZ$Tot_SEC_CO2 <- DC_Com_Base_Activities_SMZ$CO2_base_RE + DC_Com_Base_Activities_SMZ$CO2_base_OFF + DC_Com_Base_Activities_SMZ$CO2_base_OTH
Tot_BASE_Com_CO2<- sum(DC_Com_Base_Activities_SMZ$Tot_SEC_CO2, na.rm = TRUE)*0.000001
CO2_Com_Rate_Adj<-(DC_COM_CO2con/Tot_BASE_Com_CO2)

###Adjusted Rate Per Worker Per Sector
DC_Com_Base_Activities_SMZ$CO2_Rate_RE_ADJ <- DC_Com_Base_Activities_SMZ$CO2_Rate_RE*CO2_Com_Rate_Adj
DC_Com_Base_Activities_SMZ$CO2_Rate_OFF_ADJ <- DC_Com_Base_Activities_SMZ$CO2_Rate_OFF*CO2_Com_Rate_Adj
DC_Com_Base_Activities_SMZ$CO2_Rate_OTH_ADJ <- DC_Com_Base_Activities_SMZ$CO2_Rate_OTH*CO2_Com_Rate_Adj

###Apply Adjusted Rates to Activities
DC_Com_Base_Activities_SMZ$CO2_base_RE <- DC_Com_Base_Activities_SMZ$CO2_Rate_RE_ADJ*DC_Com_Base_Activities_SMZ$RE_BASE
DC_Com_Base_Activities_SMZ$CO2_base_OFF <- DC_Com_Base_Activities_SMZ$CO2_Rate_OFF_ADJ*DC_Com_Base_Activities_SMZ$OFF_BASE
DC_Com_Base_Activities_SMZ$CO2_base_OTH <- DC_Com_Base_Activities_SMZ$CO2_Rate_OTH_ADJ*DC_Com_Base_Activities_SMZ$OTH_BASE

###Sum Total Base Emissions
DC_Com_Base_Activities_SMZ$CO2_base_Com_Total <- DC_Com_Base_Activities_SMZ$CO2_base_RE + DC_Com_Base_Activities_SMZ$CO2_base_OFF + DC_Com_Base_Activities_SMZ$CO2_base_OTH

#Merge Base Data, Rates and Projected Activities
DC_Com_Base_Activities_SMZ_PROJ = merge(DC_Com_Base_Activities_SMZ, Proj_SMZ_Activities, by="SMZ_N")

###Calculate Projected Growth
PROJ_GROWTH_RE <- data.frame(DC_Com_Base_Activities_SMZ_PROJ$SMZ_N, (DC_Com_Base_Activities_SMZ_PROJ$RE_PROJ-DC_Com_Base_Activities_SMZ_PROJ$RE_BASE))
names(PROJ_GROWTH_RE) <- c("SMZ_N","PROJ_GROWTH_RE")
PROJ_GROWTH_OFF <- data.frame(DC_Com_Base_Activities_SMZ_PROJ$SMZ_N, (DC_Com_Base_Activities_SMZ_PROJ$OFF_PROJ-DC_Com_Base_Activities_SMZ_PROJ$OFF_BASE))
names(PROJ_GROWTH_OFF) <- c("SMZ_N","PROJ_GROWTH_OFF")
PROJ_GROWTH_OTH <- data.frame(DC_Com_Base_Activities_SMZ_PROJ$SMZ_N, (DC_Com_Base_Activities_SMZ_PROJ$OTH_PROJ-DC_Com_Base_Activities_SMZ_PROJ$OTH_BASE))
names(PROJ_GROWTH_OTH) <- c("SMZ_N","PROJ_GROWTH_OTH")
PROJ_GROWTH_EMP <- data.frame(DC_Com_Base_Activities_SMZ_PROJ$SMZ_N, (PROJ_GROWTH_RE$PROJ_GROWTH_RE + PROJ_GROWTH_OFF$PROJ_GROWTH_OFF + PROJ_GROWTH_OTH$PROJ_GROWTH_OTH))
names(PROJ_GROWTH_EMP) <- c("SMZ_N","PROJ_GROWTH_EMP")

############
###Simulate New Commercial Stock
############
DC_Com_SMZ_SIM <- PROJ_GROWTH_EMP

#Attach HDD Data
DC_Com_SMZ_SIM$HDD = SMZ_HDD[match(DC_Com_SMZ_SIM$SMZ_N, SMZ_HDD$SMZ_N),"HDD"] 

#Sampling Function
sampEMP<-function(x){

sample(PROJ_GROWTH_EMP + 1, 1)

#Sim commercial Data
DC_Com_SMZ_SIM$Age <- sapply(DC_Com_SMZ_SIM$PROJ_GROWTH_EMP*TIME + 1, sample, 1) - 1
DC_Com_SMZ_SIM$SQFTSTRC <- sapply(DC_Com_SMZ_SIM$PROJ_GROWTH_EMP*DC_Com_Base_SMZ$SQFT + 1, sample, 1) - 1


##### Estimate New Stock Commercial Emissions
DC_Com_SMZ_SIM$CO2_EST <- Com_ols$coef[1]+
					  (Com_ols$coef[2])*(DC_Com_SMZ_SIM$HDD*DDCCRESULT$HDD) +
					  (Com_ols$coef[3])*(DC_Com_SMZ_SIM$Age) +
					  (Com_ols$coef[4])*DC_Com_SMZ_SIM$SQFTSTRC
					  
###Apply Emissions Adjustment to Projected New Stock
DC_Com_SMZ_SIM$CO2_EST_ADJ <- DC_Com_SMZ_SIM$CO2_EST*CO2_Com_Rate_Adj
write.table(DC_Com_SMZ_SIM[c("SMZ_N","CO2_EST_ADJ")],file=(paste(TEMPATH,"COM_CO2_SAMPLE.csv",sep="")), sep=",", row.names=FALSE, col.names=FALSE, append=TRUE)
}

replicate(n = 10000, expr = sampEMP())

DC_Com_SMZ_SIM_RESULT<- read.csv(paste(TEMPATH,"COM_CO2_SAMPLE.csv",sep="")) #Read back in simulated housing stock
names(DC_Com_SMZ_SIM_RESULT) <- c("SMZ_N","CO2_EST_ADJ")

####Aggregate CO2 data in simulated commercial stock By SMZ
DC_Com_SMZ_SIM_RESULT_AGG <-aggregate(cbind(DC_Com_SMZ_SIM_RESULT$CO2_EST_ADJ)~DC_Com_SMZ_SIM_RESULT$SMZ_N, data=DC_Com_SMZ_SIM_RESULT, mean, na.rm=FALSE)
names(DC_Com_SMZ_SIM_RESULT_AGG)[1] <- "SMZ_N" 
names(DC_Com_SMZ_SIM_RESULT_AGG)[2] <- "CO2_EST_ADJ" 

###########
###Age Base Commercial Stock
############
DC_Com_Base_AGED <- DC_Com_Base

DC_Com_Base_AGED$CO2_EST <- Com_ols$coef[1]+
					  (Com_ols$coef[2])*(DC_Com_Base$HDD*DDCCRESULT$HDD) +
					  (Com_ols$coef[3])*(DC_Com_Base$Age + TIME) +
					  (Com_ols$coef[4])*DC_Com_Base$SQFTSTRC

###Calculate Emissions BY Sector per Worker
DC_Com_Base_AGED$CO2_RE <- DC_Com_Base_AGED$CO2_EST*DC_Com_Base_AGED$RETAIL
DC_Com_Base_AGED$CO2_OFF<- DC_Com_Base_AGED$CO2_EST*DC_Com_Base_AGED$Office
DC_Com_Base_AGED$CO2_OTH<- DC_Com_Base_AGED$CO2_EST*DC_Com_Base_AGED$Other

###Calculate Number of Workers BY Sector
DC_Com_Base_AGED$Workers_RE<- DC_Com_Base_AGED$Workers*DC_Com_Base_AGED$RETAIL
DC_Com_Base_AGED$Workers_OFF<- DC_Com_Base_AGED$Workers*DC_Com_Base_AGED$Office
DC_Com_Base_AGED$Workers_OTH<- DC_Com_Base_AGED$Workers*DC_Com_Base_AGED$Other
				
####Aggregate data By SMZ by sector
DC_Com_Base_SMZ_AGED <-aggregate(cbind(DC_Com_Base_AGED$CO2_EST, DC_Com_Base_AGED$CO2_RE,DC_Com_Base_AGED$CO2_OFF, DC_Com_Base_AGED$CO2_OTH, DC_Com_Base_AGED$Workers_RE,DC_Com_Base_AGED$Workers_OFF, DC_Com_Base_AGED$Workers_OTH)~DC_Com_Base_AGED$SMZ_N, data=DC_Com_Base_AGED, sum, na.rm=TRUE)
names(DC_Com_Base_SMZ_AGED)[1] <- "SMZ_N" 
names(DC_Com_Base_SMZ_AGED)[2] <- "CO2_EST" 
names(DC_Com_Base_SMZ_AGED)[3] <- "CO2_EST_RE" 
names(DC_Com_Base_SMZ_AGED)[4] <- "CO2_EST_OFF"
names(DC_Com_Base_SMZ_AGED)[5] <- "CO2_EST_OTH"
names(DC_Com_Base_SMZ_AGED)[6] <- "CO2_Workers_RE" 
names(DC_Com_Base_SMZ_AGED)[7] <- "CO2_Workers_OFF"
names(DC_Com_Base_SMZ_AGED)[8] <- "CO2_Workers_OTH"

###Aged Rate Per Worker Per Sector
DC_Com_Base_SMZ_AGED$CO2_Rate_RE <- DC_Com_Base_SMZ_AGED$CO2_EST_RE/DC_Com_Base_SMZ_AGED$CO2_Workers_RE
DC_Com_Base_SMZ_AGED$CO2_Rate_OFF <- DC_Com_Base_SMZ_AGED$CO2_EST_OFF/DC_Com_Base_SMZ_AGED$CO2_Workers_OFF
DC_Com_Base_SMZ_AGED$CO2_Rate_OTH <- DC_Com_Base_SMZ_AGED$CO2_EST_OTH/DC_Com_Base_SMZ_AGED$CO2_Workers_OTH

# Merge Rates and Activities for Aged Commercial Stock
DC_Com_Base_Activities_SMZ_AGED = merge(DC_Com_Base_SMZ_AGED, Base_SMZ_Activities, by="SMZ_N")

##Apply New Rates to Aged Workers
DC_Com_Base_Activities_SMZ_AGED$CO2_base_RE <- DC_Com_Base_Activities_SMZ_AGED$CO2_Rate_RE*DC_Com_Base_Activities_SMZ_AGED$RE_BASE
DC_Com_Base_Activities_SMZ_AGED$CO2_base_OFF <- DC_Com_Base_Activities_SMZ_AGED$CO2_Rate_OFF*DC_Com_Base_Activities_SMZ_AGED$OFF_BASE
DC_Com_Base_Activities_SMZ_AGED$CO2_base_OTH <- DC_Com_Base_Activities_SMZ_AGED$CO2_Rate_OTH*DC_Com_Base_Activities_SMZ_AGED$OTH_BASE

###Replace Errors with Zero					  
DC_Com_Base_Activities_SMZ_AGED[is.na(DC_Com_Base_Activities_SMZ_AGED)] <- 0
DC_Com_Base_Activities_SMZ_AGED[DC_Com_Base_Activities_SMZ_AGED==-Inf] <- 0
DC_Com_Base_Activities_SMZ_AGED[DC_Com_Base_Activities_SMZ_AGED==Inf] <- 0					  

###Adjusted CO2 Rate Per Aged Worker
DC_Com_Base_Activities_SMZ_AGED$CO2_Rate_RE_ADJ <- DC_Com_Base_Activities_SMZ_AGED$CO2_Rate_RE*CO2_Com_Rate_Adj
DC_Com_Base_Activities_SMZ_AGED$CO2_Rate_OFF_ADJ <- DC_Com_Base_Activities_SMZ_AGED$CO2_Rate_OFF*CO2_Com_Rate_Adj					  
DC_Com_Base_Activities_SMZ_AGED$CO2_Rate_OTH_ADJ <- DC_Com_Base_Activities_SMZ_AGED$CO2_Rate_OTH*CO2_Com_Rate_Adj
					  
###Apply Adjusted Aged Rates to Activities
DC_Com_Base_Activities_SMZ_AGED$CO2_base_RE_AGED <- DC_Com_Base_Activities_SMZ_AGED$CO2_Rate_RE_ADJ*DC_Com_Base_Activities_SMZ_AGED$RE_BASE
DC_Com_Base_Activities_SMZ_AGED$CO2_base_OTH_AGED <- DC_Com_Base_Activities_SMZ_AGED$CO2_Rate_OTH_ADJ*DC_Com_Base_Activities_SMZ_AGED$OTH_BASE
DC_Com_Base_Activities_SMZ_AGED$CO2_base_OFF_AGED <- DC_Com_Base_Activities_SMZ_AGED$CO2_Rate_OFF_ADJ*DC_Com_Base_Activities_SMZ_AGED$OFF_BASE
DC_Com_Base_Activities_SMZ_AGED$CO2_base_Com_AGED <- DC_Com_Base_Activities_SMZ_AGED$CO2_base_RE_AGED + DC_Com_Base_Activities_SMZ_AGED$CO2_base_OFF_AGED + DC_Com_Base_Activities_SMZ_AGED$CO2_base_OTH_AGED
###Sum Total Aged Base Emissions
DC_Com_Base_Activities_SMZ_AGED$CO2_base_Com_Total_AGED <- DC_Com_Base_Activities_SMZ_AGED$CO2_base_RE_AGED + DC_Com_Base_Activities_SMZ_AGED$CO2_base_OFF_AGED + DC_Com_Base_Activities_SMZ_AGED$CO2_base_OTH_AGED
					  
###Add Aged Base Emissions to Projected Emissions Growth
DC_Com_SMZ_SIM_RESULT_AGG$CO2_base_Com_AGED = DC_Com_Base_Activities_SMZ_AGED[match(DC_Com_SMZ_SIM_RESULT_AGG$SMZ_N, DC_Com_Base_Activities_SMZ_AGED$SMZ_N),"CO2_base_Com_AGED"]

###Add Base Emissions to Projected Emissions Growth
DC_Com_SMZ_SIM_RESULT_AGG$CO2_base_Com = DC_Com_Base_Activities_SMZ[match(DC_Com_SMZ_SIM_RESULT_AGG$SMZ_N, DC_Com_Base_Activities_SMZ$SMZ_N),"CO2_base_Com_Total"]

DC_Com_SMZ_SIM_RESULT_AGG$CO2_Proj_Com <- DC_Com_SMZ_SIM_RESULT_AGG$CO2_base_Com_AGED + DC_Com_SMZ_SIM_RESULT_AGG$CO2_EST_ADJ

###Clean Final Data Set
DC_Com_SMZ_SIM_RESULT_AGG[is.na(DC_Com_SMZ_SIM_RESULT_AGG)] <- 0

###Write Final CO2 Output
write.csv(DC_Com_SMZ_SIM_RESULT_AGG[,c("SMZ_N","CO2_base_Com","CO2_Proj_Com")], file=(paste(OUTPATH,"DC_BEM_Com_CO2.csv",sep="")),row.names=FALSE)

###Take out the trash
file.remove(paste(TEMPATH,"COM_CO2_SAMPLE.csv",sep=""))
	
####END DC_COM CO2	

#####START DE_COM CO2####

##### Estimate Commercial Emissions
DE_Com_Base$CO2_EST <- Com_ols$coef[1]+
					  (Com_ols$coef[2])*DE_Com_Base$HDD658 +
					  (Com_ols$coef[3])*DE_Com_Base$Age +
					  (Com_ols$coef[4])*DE_Com_Base$SQFTSTRC					  

###Calculate Emissions per Worker
DE_Com_Base$CO2_RE<- DE_Com_Base$CO2_EST*DE_Com_Base$RETAIL
DE_Com_Base$CO2_OFF<- DE_Com_Base$CO2_EST*DE_Com_Base$Office
DE_Com_Base$CO2_OTH<- DE_Com_Base$CO2_EST*DE_Com_Base$Other

###Calculate Number of Workers BY Sector
DE_Com_Base$Workers<-1
DE_Com_Base$Workers_RE<- DE_Com_Base$Workers*DE_Com_Base$RETAIL
DE_Com_Base$Workers_OFF<- DE_Com_Base$Workers*DE_Com_Base$Office
DE_Com_Base$Workers_OTH<- DE_Com_Base$Workers*DE_Com_Base$Other
				
####Aggregate data By SMZ by sector
DE_Com_Base_SMZ <-aggregate(cbind(DE_Com_Base$CO2_EST, DE_Com_Base$CO2_RE,DE_Com_Base$CO2_OFF, DE_Com_Base$CO2_OTH, DE_Com_Base$Workers_RE,DE_Com_Base$Workers_OFF, DE_Com_Base$Workers_OTH)~DE_Com_Base$SMZ_N, data=DE_Com_Base, sum, na.rm=TRUE)
names(DE_Com_Base_SMZ)[1] <- "SMZ_N" 
names(DE_Com_Base_SMZ)[2] <- "CO2_EST" 
names(DE_Com_Base_SMZ)[3] <- "CO2_EST_RE" 
names(DE_Com_Base_SMZ)[4] <- "CO2_EST_OFF"
names(DE_Com_Base_SMZ)[5] <- "CO2_EST_OTH"
names(DE_Com_Base_SMZ)[6] <- "CO2_Workers_RE" 
names(DE_Com_Base_SMZ)[7] <- "CO2_Workers_OFF"
names(DE_Com_Base_SMZ)[8] <- "CO2_Workers_OTH"

DE_Com_Base_SMZ_SQFT <-aggregate(cbind(DE_Com_Base$SQFTSTRC)~DE_Com_Base$SMZ_N, data=DE_Com_Base, mean, na.rm=TRUE) 
names(DE_Com_Base_SMZ_SQFT)[1] <- "SMZ_N" 
names(DE_Com_Base_SMZ_SQFT)[2] <- "SQFT" 

DE_Com_Base_SMZ$SQFT = DE_Com_Base_SMZ_SQFT[match(DE_Com_Base_SMZ$SMZ_N, DE_Com_Base_SMZ_SQFT$SMZ_N),"SQFT"] 

###Rate Per Worker Per Sector
DE_Com_Base_SMZ$CO2_Rate_RE <- DE_Com_Base_SMZ$CO2_EST_RE/DE_Com_Base_SMZ$CO2_Workers_RE
DE_Com_Base_SMZ$CO2_Rate_OFF <- DE_Com_Base_SMZ$CO2_EST_OFF/DE_Com_Base_SMZ$CO2_Workers_OFF
DE_Com_Base_SMZ$CO2_Rate_OTH <- DE_Com_Base_SMZ$CO2_EST_OTH/DE_Com_Base_SMZ$CO2_Workers_OTH

# merge Rates and Activities
DE_Com_Base_Activities_SMZ = merge(DE_Com_Base_SMZ, Base_SMZ_Activities, by="SMZ_N")

###Apply Rates to Activities
DE_Com_Base_Activities_SMZ$CO2_base_RE <- DE_Com_Base_Activities_SMZ$CO2_Rate_RE*DE_Com_Base_Activities_SMZ$RE_BASE
DE_Com_Base_Activities_SMZ$CO2_base_OFF <- DE_Com_Base_Activities_SMZ$CO2_Rate_OFF*DE_Com_Base_Activities_SMZ$OFF_BASE
DE_Com_Base_Activities_SMZ$CO2_base_OTH <- DE_Com_Base_Activities_SMZ$CO2_Rate_OTH*DE_Com_Base_Activities_SMZ$OTH_BASE

###Replace Errors with Zero
DE_Com_Base_Activities_SMZ[is.na(DE_Com_Base_Activities_SMZ)] <- 0
DE_Com_Base_Activities_SMZ[DE_Com_Base_Activities_SMZ==-Inf] <- 0
DE_Com_Base_Activities_SMZ[DE_Com_Base_Activities_SMZ==Inf] <- 0

###Emission Rate Adjustment Factor
DE_Com_Base_Activities_SMZ$Tot_SEC_CO2 <- DE_Com_Base_Activities_SMZ$CO2_base_RE + DE_Com_Base_Activities_SMZ$CO2_base_OFF + DE_Com_Base_Activities_SMZ$CO2_base_OTH
Tot_BASE_Com_CO2<- sum(DE_Com_Base_Activities_SMZ$Tot_SEC_CO2, na.rm = TRUE)*0.000001
CO2_Com_Rate_Adj<-(DE_COM_CO2con/Tot_BASE_Com_CO2)

###Adjusted Rate Per Worker Per Sector
DE_Com_Base_Activities_SMZ$CO2_Rate_RE_ADJ <- DE_Com_Base_Activities_SMZ$CO2_Rate_RE*CO2_Com_Rate_Adj
DE_Com_Base_Activities_SMZ$CO2_Rate_OFF_ADJ <- DE_Com_Base_Activities_SMZ$CO2_Rate_OFF*CO2_Com_Rate_Adj
DE_Com_Base_Activities_SMZ$CO2_Rate_OTH_ADJ <- DE_Com_Base_Activities_SMZ$CO2_Rate_OTH*CO2_Com_Rate_Adj

###Apply Adjusted Rates to Activities
DE_Com_Base_Activities_SMZ$CO2_base_RE <- DE_Com_Base_Activities_SMZ$CO2_Rate_RE_ADJ*DE_Com_Base_Activities_SMZ$RE_BASE
DE_Com_Base_Activities_SMZ$CO2_base_OFF <- DE_Com_Base_Activities_SMZ$CO2_Rate_OFF_ADJ*DE_Com_Base_Activities_SMZ$OFF_BASE
DE_Com_Base_Activities_SMZ$CO2_base_OTH <- DE_Com_Base_Activities_SMZ$CO2_Rate_OTH_ADJ*DE_Com_Base_Activities_SMZ$OTH_BASE

###Sum Total Base Emissions
DE_Com_Base_Activities_SMZ$CO2_base_Com_Total <- DE_Com_Base_Activities_SMZ$CO2_base_RE + DE_Com_Base_Activities_SMZ$CO2_base_OFF + DE_Com_Base_Activities_SMZ$CO2_base_OTH

#Merge Base Data, Rates and Projected Activities
DE_Com_Base_Activities_SMZ_PROJ = merge(DE_Com_Base_Activities_SMZ, Proj_SMZ_Activities, by="SMZ_N")

###Calculate Projected Growth
PROJ_GROWTH_RE <- data.frame(DE_Com_Base_Activities_SMZ_PROJ$SMZ_N, (DE_Com_Base_Activities_SMZ_PROJ$RE_PROJ-DE_Com_Base_Activities_SMZ_PROJ$RE_BASE))
names(PROJ_GROWTH_RE) <- c("SMZ_N","PROJ_GROWTH_RE")
PROJ_GROWTH_OFF <- data.frame(DE_Com_Base_Activities_SMZ_PROJ$SMZ_N, (DE_Com_Base_Activities_SMZ_PROJ$OFF_PROJ-DE_Com_Base_Activities_SMZ_PROJ$OFF_BASE))
names(PROJ_GROWTH_OFF) <- c("SMZ_N","PROJ_GROWTH_OFF")
PROJ_GROWTH_OTH <- data.frame(DE_Com_Base_Activities_SMZ_PROJ$SMZ_N, (DE_Com_Base_Activities_SMZ_PROJ$OTH_PROJ-DE_Com_Base_Activities_SMZ_PROJ$OTH_BASE))
names(PROJ_GROWTH_OTH) <- c("SMZ_N","PROJ_GROWTH_OTH")
PROJ_GROWTH_EMP <- data.frame(DE_Com_Base_Activities_SMZ_PROJ$SMZ_N, (PROJ_GROWTH_RE$PROJ_GROWTH_RE + PROJ_GROWTH_OFF$PROJ_GROWTH_OFF + PROJ_GROWTH_OTH$PROJ_GROWTH_OTH))
names(PROJ_GROWTH_EMP) <- c("SMZ_N","PROJ_GROWTH_EMP")

############
###Simulate New Commercial Stock
############
DE_Com_SMZ_SIM <- PROJ_GROWTH_EMP

#Attach HDD Data
DE_Com_SMZ_SIM$HDD = SMZ_HDD[match(DE_Com_SMZ_SIM$SMZ_N, SMZ_HDD$SMZ_N),"HDD"] 

#Sampling Function
sampEMP<-function(x){

sample(PROJ_GROWTH_EMP + 1, 1)

#Sim commercial Data
DE_Com_SMZ_SIM$Age <- sapply(DE_Com_SMZ_SIM$PROJ_GROWTH_EMP*TIME + 1, sample, 1) - 1
DE_Com_SMZ_SIM$SQFTSTRC <- sapply(DE_Com_SMZ_SIM$PROJ_GROWTH_EMP*DE_Com_Base_SMZ$SQFT + 1, sample, 1) - 1


##### Estimate New Stock Commercial Emissions
DE_Com_SMZ_SIM$CO2_EST <- Com_ols$coef[1]+
					  (Com_ols$coef[2])*(DE_Com_SMZ_SIM$HDD*DDCCRESULT$HDD) +
					  (Com_ols$coef[3])*(DE_Com_SMZ_SIM$Age) +
					  (Com_ols$coef[4])*DE_Com_SMZ_SIM$SQFTSTRC
					  
###Apply Emissions Adjustment to Projected New Stock
DE_Com_SMZ_SIM$CO2_EST_ADJ <- DE_Com_SMZ_SIM$CO2_EST*CO2_Com_Rate_Adj
write.table(DE_Com_SMZ_SIM[c("SMZ_N","CO2_EST_ADJ")],file=(paste(TEMPATH,"COM_CO2_SAMPLE.csv",sep="")), sep=",", row.names=FALSE, col.names=FALSE, append=TRUE)
}

replicate(n = 10000, expr = sampEMP())

DE_Com_SMZ_SIM_RESULT<- read.csv(paste(TEMPATH,"COM_CO2_SAMPLE.csv",sep="")) #Read back in simulated housing stock
names(DE_Com_SMZ_SIM_RESULT) <- c("SMZ_N","CO2_EST_ADJ")

####Aggregate CO2 data in simulated commercial stock By SMZ
DE_Com_SMZ_SIM_RESULT_AGG <-aggregate(cbind(DE_Com_SMZ_SIM_RESULT$CO2_EST_ADJ)~DE_Com_SMZ_SIM_RESULT$SMZ_N, data=DE_Com_SMZ_SIM_RESULT, mean, na.rm=FALSE)
names(DE_Com_SMZ_SIM_RESULT_AGG)[1] <- "SMZ_N" 
names(DE_Com_SMZ_SIM_RESULT_AGG)[2] <- "CO2_EST_ADJ" 

###########
###Age Base Commercial Stock
############
DE_Com_Base_AGED <- DE_Com_Base

DE_Com_Base_AGED$CO2_EST <- Com_ols$coef[1]+
					  (Com_ols$coef[2])*(DE_Com_Base$HDD*DDCCRESULT$HDD) +
					  (Com_ols$coef[3])*(DE_Com_Base$Age + TIME) +
					  (Com_ols$coef[4])*DE_Com_Base$SQFTSTRC

###Calculate Emissions BY Sector per Worker
DE_Com_Base_AGED$CO2_RE <- DE_Com_Base_AGED$CO2_EST*DE_Com_Base_AGED$RETAIL
DE_Com_Base_AGED$CO2_OFF<- DE_Com_Base_AGED$CO2_EST*DE_Com_Base_AGED$Office
DE_Com_Base_AGED$CO2_OTH<- DE_Com_Base_AGED$CO2_EST*DE_Com_Base_AGED$Other

###Calculate Number of Workers BY Sector
DE_Com_Base_AGED$Workers_RE<- DE_Com_Base_AGED$Workers*DE_Com_Base_AGED$RETAIL
DE_Com_Base_AGED$Workers_OFF<- DE_Com_Base_AGED$Workers*DE_Com_Base_AGED$Office
DE_Com_Base_AGED$Workers_OTH<- DE_Com_Base_AGED$Workers*DE_Com_Base_AGED$Other
				
####Aggregate data By SMZ by sector
DE_Com_Base_SMZ_AGED <-aggregate(cbind(DE_Com_Base_AGED$CO2_EST, DE_Com_Base_AGED$CO2_RE,DE_Com_Base_AGED$CO2_OFF, DE_Com_Base_AGED$CO2_OTH, DE_Com_Base_AGED$Workers_RE,DE_Com_Base_AGED$Workers_OFF, DE_Com_Base_AGED$Workers_OTH)~DE_Com_Base_AGED$SMZ_N, data=DE_Com_Base_AGED, sum, na.rm=TRUE)
names(DE_Com_Base_SMZ_AGED)[1] <- "SMZ_N" 
names(DE_Com_Base_SMZ_AGED)[2] <- "CO2_EST" 
names(DE_Com_Base_SMZ_AGED)[3] <- "CO2_EST_RE" 
names(DE_Com_Base_SMZ_AGED)[4] <- "CO2_EST_OFF"
names(DE_Com_Base_SMZ_AGED)[5] <- "CO2_EST_OTH"
names(DE_Com_Base_SMZ_AGED)[6] <- "CO2_Workers_RE" 
names(DE_Com_Base_SMZ_AGED)[7] <- "CO2_Workers_OFF"
names(DE_Com_Base_SMZ_AGED)[8] <- "CO2_Workers_OTH"

###Aged Rate Per Worker Per Sector
DE_Com_Base_SMZ_AGED$CO2_Rate_RE <- DE_Com_Base_SMZ_AGED$CO2_EST_RE/DE_Com_Base_SMZ_AGED$CO2_Workers_RE
DE_Com_Base_SMZ_AGED$CO2_Rate_OFF <- DE_Com_Base_SMZ_AGED$CO2_EST_OFF/DE_Com_Base_SMZ_AGED$CO2_Workers_OFF
DE_Com_Base_SMZ_AGED$CO2_Rate_OTH <- DE_Com_Base_SMZ_AGED$CO2_EST_OTH/DE_Com_Base_SMZ_AGED$CO2_Workers_OTH

# Merge Rates and Activities for Aged Commercial Stock
DE_Com_Base_Activities_SMZ_AGED = merge(DE_Com_Base_SMZ_AGED, Base_SMZ_Activities, by="SMZ_N")

##Apply New Rates to Aged Workers
DE_Com_Base_Activities_SMZ_AGED$CO2_base_RE <- DE_Com_Base_Activities_SMZ_AGED$CO2_Rate_RE*DE_Com_Base_Activities_SMZ_AGED$RE_BASE
DE_Com_Base_Activities_SMZ_AGED$CO2_base_OFF <- DE_Com_Base_Activities_SMZ_AGED$CO2_Rate_OFF*DE_Com_Base_Activities_SMZ_AGED$OFF_BASE
DE_Com_Base_Activities_SMZ_AGED$CO2_base_OTH <- DE_Com_Base_Activities_SMZ_AGED$CO2_Rate_OTH*DE_Com_Base_Activities_SMZ_AGED$OTH_BASE

###Replace Errors with Zero					  
DE_Com_Base_Activities_SMZ_AGED[is.na(DE_Com_Base_Activities_SMZ_AGED)] <- 0
DE_Com_Base_Activities_SMZ_AGED[DE_Com_Base_Activities_SMZ_AGED==-Inf] <- 0
DE_Com_Base_Activities_SMZ_AGED[DE_Com_Base_Activities_SMZ_AGED==Inf] <- 0					  

###Adjusted CO2 Rate Per Aged Worker
DE_Com_Base_Activities_SMZ_AGED$CO2_Rate_RE_ADJ <- DE_Com_Base_Activities_SMZ_AGED$CO2_Rate_RE*CO2_Com_Rate_Adj
DE_Com_Base_Activities_SMZ_AGED$CO2_Rate_OFF_ADJ <- DE_Com_Base_Activities_SMZ_AGED$CO2_Rate_OFF*CO2_Com_Rate_Adj					  
DE_Com_Base_Activities_SMZ_AGED$CO2_Rate_OTH_ADJ <- DE_Com_Base_Activities_SMZ_AGED$CO2_Rate_OTH*CO2_Com_Rate_Adj
					  
###Apply Adjusted Aged Rates to Activities
DE_Com_Base_Activities_SMZ_AGED$CO2_base_RE_AGED <- DE_Com_Base_Activities_SMZ_AGED$CO2_Rate_RE_ADJ*DE_Com_Base_Activities_SMZ_AGED$RE_BASE
DE_Com_Base_Activities_SMZ_AGED$CO2_base_OTH_AGED <- DE_Com_Base_Activities_SMZ_AGED$CO2_Rate_OTH_ADJ*DE_Com_Base_Activities_SMZ_AGED$OTH_BASE
DE_Com_Base_Activities_SMZ_AGED$CO2_base_OFF_AGED <- DE_Com_Base_Activities_SMZ_AGED$CO2_Rate_OFF_ADJ*DE_Com_Base_Activities_SMZ_AGED$OFF_BASE
DE_Com_Base_Activities_SMZ_AGED$CO2_base_Com_AGED <- DE_Com_Base_Activities_SMZ_AGED$CO2_base_RE_AGED + DE_Com_Base_Activities_SMZ_AGED$CO2_base_OFF_AGED + DE_Com_Base_Activities_SMZ_AGED$CO2_base_OTH_AGED
###Sum Total Aged Base Emissions
DE_Com_Base_Activities_SMZ_AGED$CO2_base_Com_Total_AGED <- DE_Com_Base_Activities_SMZ_AGED$CO2_base_RE_AGED + DE_Com_Base_Activities_SMZ_AGED$CO2_base_OFF_AGED + DE_Com_Base_Activities_SMZ_AGED$CO2_base_OTH_AGED
					  
###Add Aged Base Emissions to Projected Emissions Growth
DE_Com_SMZ_SIM_RESULT_AGG$CO2_base_Com_AGED = DE_Com_Base_Activities_SMZ_AGED[match(DE_Com_SMZ_SIM_RESULT_AGG$SMZ_N, DE_Com_Base_Activities_SMZ_AGED$SMZ_N),"CO2_base_Com_AGED"]

###Add Base Emissions to Projected Emissions Growth
DE_Com_SMZ_SIM_RESULT_AGG$CO2_base_Com = DE_Com_Base_Activities_SMZ[match(DE_Com_SMZ_SIM_RESULT_AGG$SMZ_N, DE_Com_Base_Activities_SMZ$SMZ_N),"CO2_base_Com_Total"]

DE_Com_SMZ_SIM_RESULT_AGG$CO2_Proj_Com <- DE_Com_SMZ_SIM_RESULT_AGG$CO2_base_Com_AGED + DE_Com_SMZ_SIM_RESULT_AGG$CO2_EST_ADJ

###Clean Final Data Set
DE_Com_SMZ_SIM_RESULT_AGG[is.na(DE_Com_SMZ_SIM_RESULT_AGG)] <- 0

###Write Final CO2 Output
write.csv(DE_Com_SMZ_SIM_RESULT_AGG[,c("SMZ_N","CO2_base_Com","CO2_Proj_Com")], file=(paste(OUTPATH,"DE_BEM_Com_CO2.csv",sep="")),row.names=FALSE)

###Take out the trash
file.remove(paste(TEMPATH,"COM_CO2_SAMPLE.csv",sep=""))
	
####END DE_COM CO2	

#####START WV_COM CO2####

##### Estimate Commercial Emissions
WV_Com_Base$CO2_EST <- Com_ols$coef[1]+
					  (Com_ols$coef[2])*WV_Com_Base$HDD658 +
					  (Com_ols$coef[3])*WV_Com_Base$Age +
					  (Com_ols$coef[4])*WV_Com_Base$SQFTSTRC					  

###Calculate Emissions per Worker
WV_Com_Base$CO2_RE<- WV_Com_Base$CO2_EST*WV_Com_Base$RETAIL
WV_Com_Base$CO2_OFF<- WV_Com_Base$CO2_EST*WV_Com_Base$Office
WV_Com_Base$CO2_OTH<- WV_Com_Base$CO2_EST*WV_Com_Base$Other

###Calculate Number of Workers BY Sector
WV_Com_Base$Workers<-1
WV_Com_Base$Workers_RE<- WV_Com_Base$Workers*WV_Com_Base$RETAIL
WV_Com_Base$Workers_OFF<- WV_Com_Base$Workers*WV_Com_Base$Office
WV_Com_Base$Workers_OTH<- WV_Com_Base$Workers*WV_Com_Base$Other
				
####Aggregate data By SMZ by sector
WV_Com_Base_SMZ <-aggregate(cbind(WV_Com_Base$CO2_EST, WV_Com_Base$CO2_RE,WV_Com_Base$CO2_OFF, WV_Com_Base$CO2_OTH, WV_Com_Base$Workers_RE,WV_Com_Base$Workers_OFF, WV_Com_Base$Workers_OTH)~WV_Com_Base$SMZ_N, data=WV_Com_Base, sum, na.rm=TRUE)
names(WV_Com_Base_SMZ)[1] <- "SMZ_N" 
names(WV_Com_Base_SMZ)[2] <- "CO2_EST" 
names(WV_Com_Base_SMZ)[3] <- "CO2_EST_RE" 
names(WV_Com_Base_SMZ)[4] <- "CO2_EST_OFF"
names(WV_Com_Base_SMZ)[5] <- "CO2_EST_OTH"
names(WV_Com_Base_SMZ)[6] <- "CO2_Workers_RE" 
names(WV_Com_Base_SMZ)[7] <- "CO2_Workers_OFF"
names(WV_Com_Base_SMZ)[8] <- "CO2_Workers_OTH"

WV_Com_Base_SMZ_SQFT <-aggregate(cbind(WV_Com_Base$SQFTSTRC)~WV_Com_Base$SMZ_N, data=WV_Com_Base, mean, na.rm=TRUE) 
names(WV_Com_Base_SMZ_SQFT)[1] <- "SMZ_N" 
names(WV_Com_Base_SMZ_SQFT)[2] <- "SQFT" 

WV_Com_Base_SMZ$SQFT = WV_Com_Base_SMZ_SQFT[match(WV_Com_Base_SMZ$SMZ_N, WV_Com_Base_SMZ_SQFT$SMZ_N),"SQFT"] 

###Rate Per Worker Per Sector
WV_Com_Base_SMZ$CO2_Rate_RE <- WV_Com_Base_SMZ$CO2_EST_RE/WV_Com_Base_SMZ$CO2_Workers_RE
WV_Com_Base_SMZ$CO2_Rate_OFF <- WV_Com_Base_SMZ$CO2_EST_OFF/WV_Com_Base_SMZ$CO2_Workers_OFF
WV_Com_Base_SMZ$CO2_Rate_OTH <- WV_Com_Base_SMZ$CO2_EST_OTH/WV_Com_Base_SMZ$CO2_Workers_OTH

# merge Rates and Activities
WV_Com_Base_Activities_SMZ = merge(WV_Com_Base_SMZ, Base_SMZ_Activities, by="SMZ_N")

###Apply Rates to Activities
WV_Com_Base_Activities_SMZ$CO2_base_RE <- WV_Com_Base_Activities_SMZ$CO2_Rate_RE*WV_Com_Base_Activities_SMZ$RE_BASE
WV_Com_Base_Activities_SMZ$CO2_base_OFF <- WV_Com_Base_Activities_SMZ$CO2_Rate_OFF*WV_Com_Base_Activities_SMZ$OFF_BASE
WV_Com_Base_Activities_SMZ$CO2_base_OTH <- WV_Com_Base_Activities_SMZ$CO2_Rate_OTH*WV_Com_Base_Activities_SMZ$OTH_BASE

###Replace Errors with Zero
WV_Com_Base_Activities_SMZ[is.na(WV_Com_Base_Activities_SMZ)] <- 0
WV_Com_Base_Activities_SMZ[WV_Com_Base_Activities_SMZ==-Inf] <- 0
WV_Com_Base_Activities_SMZ[WV_Com_Base_Activities_SMZ==Inf] <- 0

###Emission Rate Adjustment Factor
WV_Com_Base_Activities_SMZ$Tot_SEC_CO2 <- WV_Com_Base_Activities_SMZ$CO2_base_RE + WV_Com_Base_Activities_SMZ$CO2_base_OFF + WV_Com_Base_Activities_SMZ$CO2_base_OTH
Tot_BASE_Com_CO2<- sum(WV_Com_Base_Activities_SMZ$Tot_SEC_CO2, na.rm = TRUE)*0.000001
CO2_Com_Rate_Adj<-(WV_COM_CO2con/Tot_BASE_Com_CO2)

###Adjusted Rate Per Worker Per Sector
WV_Com_Base_Activities_SMZ$CO2_Rate_RE_ADJ <- WV_Com_Base_Activities_SMZ$CO2_Rate_RE*CO2_Com_Rate_Adj
WV_Com_Base_Activities_SMZ$CO2_Rate_OFF_ADJ <- WV_Com_Base_Activities_SMZ$CO2_Rate_OFF*CO2_Com_Rate_Adj
WV_Com_Base_Activities_SMZ$CO2_Rate_OTH_ADJ <- WV_Com_Base_Activities_SMZ$CO2_Rate_OTH*CO2_Com_Rate_Adj

###Apply Adjusted Rates to Activities
WV_Com_Base_Activities_SMZ$CO2_base_RE <- WV_Com_Base_Activities_SMZ$CO2_Rate_RE_ADJ*WV_Com_Base_Activities_SMZ$RE_BASE
WV_Com_Base_Activities_SMZ$CO2_base_OFF <- WV_Com_Base_Activities_SMZ$CO2_Rate_OFF_ADJ*WV_Com_Base_Activities_SMZ$OFF_BASE
WV_Com_Base_Activities_SMZ$CO2_base_OTH <- WV_Com_Base_Activities_SMZ$CO2_Rate_OTH_ADJ*WV_Com_Base_Activities_SMZ$OTH_BASE

###Sum Total Base Emissions
WV_Com_Base_Activities_SMZ$CO2_base_Com_Total <- WV_Com_Base_Activities_SMZ$CO2_base_RE + WV_Com_Base_Activities_SMZ$CO2_base_OFF + WV_Com_Base_Activities_SMZ$CO2_base_OTH

#Merge Base Data, Rates and Projected Activities
WV_Com_Base_Activities_SMZ_PROJ = merge(WV_Com_Base_Activities_SMZ, Proj_SMZ_Activities, by="SMZ_N")

###Calculate Projected Growth
PROJ_GROWTH_RE <- data.frame(WV_Com_Base_Activities_SMZ_PROJ$SMZ_N, (WV_Com_Base_Activities_SMZ_PROJ$RE_PROJ-WV_Com_Base_Activities_SMZ_PROJ$RE_BASE))
names(PROJ_GROWTH_RE) <- c("SMZ_N","PROJ_GROWTH_RE")
PROJ_GROWTH_OFF <- data.frame(WV_Com_Base_Activities_SMZ_PROJ$SMZ_N, (WV_Com_Base_Activities_SMZ_PROJ$OFF_PROJ-WV_Com_Base_Activities_SMZ_PROJ$OFF_BASE))
names(PROJ_GROWTH_OFF) <- c("SMZ_N","PROJ_GROWTH_OFF")
PROJ_GROWTH_OTH <- data.frame(WV_Com_Base_Activities_SMZ_PROJ$SMZ_N, (WV_Com_Base_Activities_SMZ_PROJ$OTH_PROJ-WV_Com_Base_Activities_SMZ_PROJ$OTH_BASE))
names(PROJ_GROWTH_OTH) <- c("SMZ_N","PROJ_GROWTH_OTH")
PROJ_GROWTH_EMP <- data.frame(WV_Com_Base_Activities_SMZ_PROJ$SMZ_N, (PROJ_GROWTH_RE$PROJ_GROWTH_RE + PROJ_GROWTH_OFF$PROJ_GROWTH_OFF + PROJ_GROWTH_OTH$PROJ_GROWTH_OTH))
names(PROJ_GROWTH_EMP) <- c("SMZ_N","PROJ_GROWTH_EMP")

############
###Simulate New Commercial Stock
############
WV_Com_SMZ_SIM <- PROJ_GROWTH_EMP

#Attach HDD Data
WV_Com_SMZ_SIM$HDD = SMZ_HDD[match(WV_Com_SMZ_SIM$SMZ_N, SMZ_HDD$SMZ_N),"HDD"] 

#Sampling Function
sampEMP<-function(x){

sample(PROJ_GROWTH_EMP + 1, 1)

#Sim commercial Data
WV_Com_SMZ_SIM$Age <- sapply(WV_Com_SMZ_SIM$PROJ_GROWTH_EMP*TIME + 1, sample, 1) - 1
WV_Com_SMZ_SIM$SQFTSTRC <- sapply(WV_Com_SMZ_SIM$PROJ_GROWTH_EMP*WV_Com_Base_SMZ$SQFT + 1, sample, 1) - 1


##### Estimate New Stock Commercial Emissions
WV_Com_SMZ_SIM$CO2_EST <- Com_ols$coef[1]+
					  (Com_ols$coef[2])*(WV_Com_SMZ_SIM$HDD*DDCCRESULT$HDD) +
					  (Com_ols$coef[3])*(WV_Com_SMZ_SIM$Age) +
					  (Com_ols$coef[4])*WV_Com_SMZ_SIM$SQFTSTRC
					  
###Apply Emissions Adjustment to Projected New Stock
WV_Com_SMZ_SIM$CO2_EST_ADJ <- WV_Com_SMZ_SIM$CO2_EST*CO2_Com_Rate_Adj
write.table(WV_Com_SMZ_SIM[c("SMZ_N","CO2_EST_ADJ")],file=(paste(TEMPATH,"COM_CO2_SAMPLE.csv",sep="")), sep=",", row.names=FALSE, col.names=FALSE, append=TRUE)
}

replicate(n = 10000, expr = sampEMP())

WV_Com_SMZ_SIM_RESULT<- read.csv(paste(TEMPATH,"COM_CO2_SAMPLE.csv",sep="")) #Read back in simulated housing stock
names(WV_Com_SMZ_SIM_RESULT) <- c("SMZ_N","CO2_EST_ADJ")

####Aggregate CO2 data in simulated commercial stock By SMZ
WV_Com_SMZ_SIM_RESULT_AGG <-aggregate(cbind(WV_Com_SMZ_SIM_RESULT$CO2_EST_ADJ)~WV_Com_SMZ_SIM_RESULT$SMZ_N, data=WV_Com_SMZ_SIM_RESULT, mean, na.rm=FALSE)
names(WV_Com_SMZ_SIM_RESULT_AGG)[1] <- "SMZ_N" 
names(WV_Com_SMZ_SIM_RESULT_AGG)[2] <- "CO2_EST_ADJ" 

###########
###Age Base Commercial Stock
############
WV_Com_Base_AGED <- WV_Com_Base

WV_Com_Base_AGED$CO2_EST <- Com_ols$coef[1]+
					  (Com_ols$coef[2])*(WV_Com_Base$HDD*DDCCRESULT$HDD) +
					  (Com_ols$coef[3])*(WV_Com_Base$Age + TIME) +
					  (Com_ols$coef[4])*WV_Com_Base$SQFTSTRC

###Calculate Emissions BY Sector per Worker
WV_Com_Base_AGED$CO2_RE <- WV_Com_Base_AGED$CO2_EST*WV_Com_Base_AGED$RETAIL
WV_Com_Base_AGED$CO2_OFF<- WV_Com_Base_AGED$CO2_EST*WV_Com_Base_AGED$Office
WV_Com_Base_AGED$CO2_OTH<- WV_Com_Base_AGED$CO2_EST*WV_Com_Base_AGED$Other

###Calculate Number of Workers BY Sector
WV_Com_Base_AGED$Workers_RE<- WV_Com_Base_AGED$Workers*WV_Com_Base_AGED$RETAIL
WV_Com_Base_AGED$Workers_OFF<- WV_Com_Base_AGED$Workers*WV_Com_Base_AGED$Office
WV_Com_Base_AGED$Workers_OTH<- WV_Com_Base_AGED$Workers*WV_Com_Base_AGED$Other
				
####Aggregate data By SMZ by sector
WV_Com_Base_SMZ_AGED <-aggregate(cbind(WV_Com_Base_AGED$CO2_EST, WV_Com_Base_AGED$CO2_RE,WV_Com_Base_AGED$CO2_OFF, WV_Com_Base_AGED$CO2_OTH, WV_Com_Base_AGED$Workers_RE,WV_Com_Base_AGED$Workers_OFF, WV_Com_Base_AGED$Workers_OTH)~WV_Com_Base_AGED$SMZ_N, data=WV_Com_Base_AGED, sum, na.rm=TRUE)
names(WV_Com_Base_SMZ_AGED)[1] <- "SMZ_N" 
names(WV_Com_Base_SMZ_AGED)[2] <- "CO2_EST" 
names(WV_Com_Base_SMZ_AGED)[3] <- "CO2_EST_RE" 
names(WV_Com_Base_SMZ_AGED)[4] <- "CO2_EST_OFF"
names(WV_Com_Base_SMZ_AGED)[5] <- "CO2_EST_OTH"
names(WV_Com_Base_SMZ_AGED)[6] <- "CO2_Workers_RE" 
names(WV_Com_Base_SMZ_AGED)[7] <- "CO2_Workers_OFF"
names(WV_Com_Base_SMZ_AGED)[8] <- "CO2_Workers_OTH"

###Aged Rate Per Worker Per Sector
WV_Com_Base_SMZ_AGED$CO2_Rate_RE <- WV_Com_Base_SMZ_AGED$CO2_EST_RE/WV_Com_Base_SMZ_AGED$CO2_Workers_RE
WV_Com_Base_SMZ_AGED$CO2_Rate_OFF <- WV_Com_Base_SMZ_AGED$CO2_EST_OFF/WV_Com_Base_SMZ_AGED$CO2_Workers_OFF
WV_Com_Base_SMZ_AGED$CO2_Rate_OTH <- WV_Com_Base_SMZ_AGED$CO2_EST_OTH/WV_Com_Base_SMZ_AGED$CO2_Workers_OTH

# Merge Rates and Activities for Aged Commercial Stock
WV_Com_Base_Activities_SMZ_AGED = merge(WV_Com_Base_SMZ_AGED, Base_SMZ_Activities, by="SMZ_N")

##Apply New Rates to Aged Workers
WV_Com_Base_Activities_SMZ_AGED$CO2_base_RE <- WV_Com_Base_Activities_SMZ_AGED$CO2_Rate_RE*WV_Com_Base_Activities_SMZ_AGED$RE_BASE
WV_Com_Base_Activities_SMZ_AGED$CO2_base_OFF <- WV_Com_Base_Activities_SMZ_AGED$CO2_Rate_OFF*WV_Com_Base_Activities_SMZ_AGED$OFF_BASE
WV_Com_Base_Activities_SMZ_AGED$CO2_base_OTH <- WV_Com_Base_Activities_SMZ_AGED$CO2_Rate_OTH*WV_Com_Base_Activities_SMZ_AGED$OTH_BASE

###Replace Errors with Zero					  
WV_Com_Base_Activities_SMZ_AGED[is.na(WV_Com_Base_Activities_SMZ_AGED)] <- 0
WV_Com_Base_Activities_SMZ_AGED[WV_Com_Base_Activities_SMZ_AGED==-Inf] <- 0
WV_Com_Base_Activities_SMZ_AGED[WV_Com_Base_Activities_SMZ_AGED==Inf] <- 0					  

###Adjusted CO2 Rate Per Aged Worker
WV_Com_Base_Activities_SMZ_AGED$CO2_Rate_RE_ADJ <- WV_Com_Base_Activities_SMZ_AGED$CO2_Rate_RE*CO2_Com_Rate_Adj
WV_Com_Base_Activities_SMZ_AGED$CO2_Rate_OFF_ADJ <- WV_Com_Base_Activities_SMZ_AGED$CO2_Rate_OFF*CO2_Com_Rate_Adj					  
WV_Com_Base_Activities_SMZ_AGED$CO2_Rate_OTH_ADJ <- WV_Com_Base_Activities_SMZ_AGED$CO2_Rate_OTH*CO2_Com_Rate_Adj
					  
###Apply Adjusted Aged Rates to Activities
WV_Com_Base_Activities_SMZ_AGED$CO2_base_RE_AGED <- WV_Com_Base_Activities_SMZ_AGED$CO2_Rate_RE_ADJ*WV_Com_Base_Activities_SMZ_AGED$RE_BASE
WV_Com_Base_Activities_SMZ_AGED$CO2_base_OTH_AGED <- WV_Com_Base_Activities_SMZ_AGED$CO2_Rate_OTH_ADJ*WV_Com_Base_Activities_SMZ_AGED$OTH_BASE
WV_Com_Base_Activities_SMZ_AGED$CO2_base_OFF_AGED <- WV_Com_Base_Activities_SMZ_AGED$CO2_Rate_OFF_ADJ*WV_Com_Base_Activities_SMZ_AGED$OFF_BASE
WV_Com_Base_Activities_SMZ_AGED$CO2_base_Com_AGED <- WV_Com_Base_Activities_SMZ_AGED$CO2_base_RE_AGED + WV_Com_Base_Activities_SMZ_AGED$CO2_base_OFF_AGED + WV_Com_Base_Activities_SMZ_AGED$CO2_base_OTH_AGED
###Sum Total Aged Base Emissions
WV_Com_Base_Activities_SMZ_AGED$CO2_base_Com_Total_AGED <- WV_Com_Base_Activities_SMZ_AGED$CO2_base_RE_AGED + WV_Com_Base_Activities_SMZ_AGED$CO2_base_OFF_AGED + WV_Com_Base_Activities_SMZ_AGED$CO2_base_OTH_AGED
					  
###Add Aged Base Emissions to Projected Emissions Growth
WV_Com_SMZ_SIM_RESULT_AGG$CO2_base_Com_AGED = WV_Com_Base_Activities_SMZ_AGED[match(WV_Com_SMZ_SIM_RESULT_AGG$SMZ_N, WV_Com_Base_Activities_SMZ_AGED$SMZ_N),"CO2_base_Com_AGED"]

###Add Base Emissions to Projected Emissions Growth
WV_Com_SMZ_SIM_RESULT_AGG$CO2_base_Com = WV_Com_Base_Activities_SMZ[match(WV_Com_SMZ_SIM_RESULT_AGG$SMZ_N, WV_Com_Base_Activities_SMZ$SMZ_N),"CO2_base_Com_Total"]

WV_Com_SMZ_SIM_RESULT_AGG$CO2_Proj_Com <- WV_Com_SMZ_SIM_RESULT_AGG$CO2_base_Com_AGED + WV_Com_SMZ_SIM_RESULT_AGG$CO2_EST_ADJ

###Clean Final Data Set
WV_Com_SMZ_SIM_RESULT_AGG[is.na(WV_Com_SMZ_SIM_RESULT_AGG)] <- 0

###Write Final CO2 Output
write.csv(WV_Com_SMZ_SIM_RESULT_AGG[,c("SMZ_N","CO2_base_Com","CO2_Proj_Com")], file=(paste(OUTPATH,"WV_BEM_Com_CO2.csv",sep="")),row.names=FALSE)

###Take out the trash
file.remove(paste(TEMPATH,"COM_CO2_SAMPLE.csv",sep=""))
	
####END WV_COM CO2	

##############
###END Commercial CO2 MODEL
##############


####50% Complete!


##############
###Begin Residential CO2 MODEL
##############

####Prep Variable for CO2 Binary logistic model
RECS$CO2_PROD <- NA
RECS$CO2_PROD[RECS$TCO2>0] <- 1
RECS$CO2_PROD[RECS$TCO2==0] <- 0
table(RECS$CO2_PROD)

#####Fit Binary Logistic
wlogit <- glm(CO2_PROD ~ HD65 + AGE + BEDROOMS + SF1, data = RECS, family = "binomial")
summary(wlogit)

###Calculate Building Age
MD_Res_Base$Age <- NA
MD_Res_Base$Age <-2007-MD_Res_Base$Yearval

###Calculate Building Beds
MD_Res_Base$Beds <- NA
MD_Res_Base$Beds <-MD_Res_Base$fbth_numb+1

###Calculate Housing Type
MD_Res_Base$SF1 <- NA
MD_Res_Base$SF1[MD_Res_Base$dwll_type==1] <- 1
MD_Res_Base$SF1[MD_Res_Base$dwll_type!=1] <- 0

###START MD_Res CO2###

##Create Count Dummy Variable Households
MD_Res_Base$HH <- 1

##### Estimate Probability of Residential Emissions
MD_Res_Base$CO2_PROB <- exp(wlogit$coef[1]+
						  wlogit$coef[2]*MD_Res_Base$htdd+
						  wlogit$coef[3]*MD_Res_Base$Age+
						  wlogit$coef[4]*MD_Res_Base$Beds+
						  wlogit$coef[5]*MD_Res_Base$SF1)/
						(1+exp(wlogit$coef[1]+
						  wlogit$coef[2]*MD_Res_Base$htdd+
						  wlogit$coef[3]*MD_Res_Base$Age+
						  wlogit$coef[4]*MD_Res_Base$Beds+
						  wlogit$coef[5]*MD_Res_Base$SF1))
###Replace Errors with Zero
MD_Res_Base[is.na(MD_Res_Base)] <- 0
MD_Res_Base[MD_Res_Base==-Inf] <- 0
MD_Res_Base[MD_Res_Base==Inf] <- 0

##### Select observations based on Probability of CO2 Emissions
MD_Res_Base2 <- MD_Res_Base[which(MD_Res_Base$CO2_PROB>.99),]

##### Select observations based on CO2 Emissions Greater than zero
RECS2 <- RECS[which(RECS$TCO2>0),]

#####Fit HH_ols
HH_ols <- lm(TCO2 ~ HD65 + AGE + SF1 + TOTSQFT + WALLTYPE4 + EQUIPM10 + EQUIPM3, data = RECS2)
summary(HH_ols)

##### Estimate Residential Emissions
MD_Res_Base2$CO2_EST <- HH_ols$coef[1]+
					  HH_ols$coef[2]*MD_Res_Base2$htdd+
					  HH_ols$coef[3]*MD_Res_Base2$Age+
					  HH_ols$coef[4]*MD_Res_Base2$SF1+
					  HH_ols$coef[5]*MD_Res_Base2$sqftstrc+
					  HH_ols$coef[6]*MD_Res_Base2$Stucco+
					  HH_ols$coef[7]*MD_Res_Base2$fire+
					  HH_ols$coef[8]*MD_Res_Base2$radiator

					  
##Count CO2 Emitting Households
MD_Res_Base2$HH <- NA
MD_Res_Base2$HH[MD_Res_Base2$CO2_EST>0] <- 1
MD_Res_Base2$HH[MD_Res_Base2$CO2_EST<=0] <- 0
				
####Aggregate data By SMZ
MD_Res_Base2_SMZ <-aggregate(cbind(MD_Res_Base2$CO2_EST, MD_Res_Base2$HH)~MD_Res_Base2$SMZ_N, data=MD_Res_Base2, sum, na.rm=TRUE)
names(MD_Res_Base2_SMZ)[1] <- "SMZ_N" 
names(MD_Res_Base2_SMZ)[2] <- "CO2_EST" 
names(MD_Res_Base2_SMZ)[3] <- "HH" 

MD_Res_Base2_SMZ_SQFT <-aggregate(cbind(MD_Res_Base2$sqftstrc)~MD_Res_Base2$SMZ_N, data=MD_Res_Base2, mean, na.rm=TRUE) 
names(MD_Res_Base2_SMZ_SQFT)[1] <- "SMZ_N" 
names(MD_Res_Base2_SMZ_SQFT)[2] <- "SQFT" 

###Rate Per HH
MD_Res_Base2_SMZ$CO2_Rate_HH <- MD_Res_Base2_SMZ$CO2_EST/MD_Res_Base2_SMZ$HH

# merge Rates and Activities
MD_Res_Base2_Activities_SMZ = merge(MD_Res_Base2_SMZ, Base_SMZ_Activities, by="SMZ_N")

###Apply Rates to Households
MD_Res_Base2_Activities_SMZ$CO2_base_HH <- MD_Res_Base2_Activities_SMZ$CO2_Rate_HH*MD_Res_Base2_Activities_SMZ$HH_BASE

###Replace Errors with Zero
MD_Res_Base2_Activities_SMZ[is.na(MD_Res_Base2_Activities_SMZ)] <- 0
MD_Res_Base2_Activities_SMZ[MD_Res_Base2_Activities_SMZ==-Inf] <- 0
MD_Res_Base2_Activities_SMZ[MD_Res_Base2_Activities_SMZ==Inf] <- 0

###Emission Rate Adjustment Factor
MD_Res_Base2_Activities_SMZ$Tot_HH_CO2 <- MD_Res_Base2_Activities_SMZ$CO2_base_HH # + MD_Res_Base2_Activities_SMZ$CO2_base_OFF + MD_Res_Base2_Activities_SMZ$CO2_base_OTH
Tot_BASE_HH_CO2 <- sum(MD_Res_Base2_Activities_SMZ$Tot_HH_CO2, na.rm = TRUE)*0.000001
CO2_HH_Rate_Adj<-(MD_RES_CO2con/Tot_BASE_HH_CO2)

###Adjusted Rate Per Household
MD_Res_Base2_Activities_SMZ$CO2_Rate_HH_ADJ <- MD_Res_Base2_Activities_SMZ$CO2_Rate_HH*CO2_HH_Rate_Adj

###Apply Adjusted Rates to Activities
MD_Res_Base2_Activities_SMZ$CO2_base_HH <- MD_Res_Base2_Activities_SMZ$CO2_Rate_HH_ADJ*MD_Res_Base2_Activities_SMZ$HH_BASE

###Sum Total Base Emissions
MD_Res_Base2_Activities_SMZ$CO2_base_HH_Total <- MD_Res_Base2_Activities_SMZ$CO2_base_HH #Leave for additional household type expansion

#Merge Base Data, Rates and Projected Activities
MD_Res_Base2_Activities_SMZ_PROJ = merge(MD_Res_Base2_Activities_SMZ, Proj_SMZ_Activities, by="SMZ_N")

# Export updated dataset
#write.csv(MD_Res_Base2_Activities_SMZ_PROJ,file="D:/BEM2/OUTPUT/check.csv")


###Calculate Projected Growth
PROJ_GROWTH_HH <- data.frame(MD_Res_Base2_Activities_SMZ_PROJ$SMZ_N, (MD_Res_Base2_Activities_SMZ_PROJ$HH_PROJ-MD_Res_Base2_Activities_SMZ_PROJ$HH_BASE))
names(PROJ_GROWTH_HH) <- c("SMZ_N","PROJ_GROWTH_HH")

############
###New Housing Stock Simulator
############
MD_Res_SMZ_SIM<-PROJ_GROWTH_HH

#Attach HDD and SQFT Data
MD_Res_SMZ_SIM$HDD = SMZ_HDD[match(MD_Res_SMZ_SIM$SMZ_N, SMZ_HDD$SMZ_N),"HDD"] 
MD_Res_SMZ_SIM$SQFT = MD_Res_Base2_SMZ_SQFT[match(MD_Res_SMZ_SIM$SMZ_N, MD_Res_Base2_SMZ_SQFT$SMZ_N),"SQFT"] 

#Sampling Function
sampHH<-function(x){

sample(PROJ_GROWTH_HH + 1, 1)

#Sim Housing Data
MD_Res_SMZ_SIM$Age <- sapply(MD_Res_SMZ_SIM$PROJ_GROWTH_HH*TIME + 1, sample, 1) - 1
MD_Res_SMZ_SIM$SF1 <- sapply(MD_Res_SMZ_SIM$PROJ_GROWTH_HH + 1, sample, 1) - 1
MD_Res_SMZ_SIM$sqftstrc <- sapply(MD_Res_SMZ_SIM$PROJ_GROWTH_HH*MD_Res_SMZ_SIM$SQFT + 1, sample, 1) - 1
MD_Res_SMZ_SIM$Stucco <- sapply(MD_Res_SMZ_SIM$PROJ_GROWTH_HH + 1, sample, 1) - 1
MD_Res_SMZ_SIM$fire <- sapply(MD_Res_SMZ_SIM$PROJ_GROWTH_HH + 1, sample, 1) - 1
MD_Res_SMZ_SIM$radiator <- sapply(MD_Res_SMZ_SIM$PROJ_GROWTH_HH + 1, sample, 1) - 1

##### Estimate New Stock Residential Emissions
MD_Res_SMZ_SIM$CO2_EST <- HH_ols$coef[1]+
					  HH_ols$coef[2]*(MD_Res_SMZ_SIM$HDD*DDCCRESULT$HDD)+
					  HH_ols$coef[3]*MD_Res_SMZ_SIM$Age+
					  HH_ols$coef[4]*MD_Res_SMZ_SIM$SF1+
					  HH_ols$coef[5]*MD_Res_SMZ_SIM$sqftstrc+
					  HH_ols$coef[6]*MD_Res_SMZ_SIM$Stucco+
					  HH_ols$coef[7]*MD_Res_SMZ_SIM$fire+
					  HH_ols$coef[8]*MD_Res_SMZ_SIM$radiator

###Apply Emissions Adjustment to Projected New Stock
MD_Res_SMZ_SIM$CO2_EST_ADJ <- MD_Res_SMZ_SIM$CO2_EST*CO2_HH_Rate_Adj
write.table(MD_Res_SMZ_SIM[c("SMZ_N","CO2_EST_ADJ")],file=(paste(TEMPATH,"RES_CO2_SAMPLE.csv",sep="")), sep=",", row.names=FALSE, col.names=FALSE, append=TRUE)
}

replicate(n = 10000, expr = sampHH())

MD_Res_SMZ_SIM_RESULT<- read.csv(paste(TEMPATH,"RES_CO2_SAMPLE.csv",sep="")) #Read back in simulated housing stock
names(MD_Res_SMZ_SIM_RESULT) <- c("SMZ_N","CO2_EST_ADJ")

####Aggregate CO2 data in simulated housing stock By SMZ
MD_Res_SMZ_SIM_RESULT_AGG <-aggregate(cbind(MD_Res_SMZ_SIM_RESULT$CO2_EST_ADJ)~MD_Res_SMZ_SIM_RESULT$SMZ_N, data=MD_Res_SMZ_SIM_RESULT, mean, na.rm=FALSE)
names(MD_Res_SMZ_SIM_RESULT_AGG)[1] <- "SMZ_N" 
names(MD_Res_SMZ_SIM_RESULT_AGG)[2] <- "CO2_EST_ADJ" 

#MD_Res_SMZ_SIM_RESULT_AGG_MAX <-aggregate(cbind(MD_Res_SMZ_SIM_RESULT2$CO2_EST_ADJ)~MD_Res_SMZ_SIM_RESULT2$SMZ_N, data=MD_Res_SMZ_SIM_RESULT2, max, na.rm=TRUE)
#names(MD_Res_SMZ_SIM_RESULT_AGG_MAX)[1] <- "SMZ_N" 
#names(MD_Res_SMZ_SIM_RESULT_AGG_MAX)[2] <- "CO2_EST_ADJ_MAX"

#MD_Res_SMZ_SIM_RESULT_AGG_MIN <-aggregate(cbind(MD_Res_SMZ_SI#M_RESULT2$CO2_EST_ADJ)~MD_Res_SMZ_SIM_RESULT2$SMZ_N, data=MD_Res_SMZ_SIM_RESULT2, min, na.rm=TRUE)
#names(MD_Res_SMZ_SIM_RESULT_AGG_MIN)[1] <- "SMZ_N" 
#names(MD_Res_SMZ_SIM_RESULT_AGG_MIN)[2] <- "CO2_EST_ADJ_MIN"

###Add MAX and MIN Emissions to Average
#MD_Res_SMZ_SIM_RESULT_AGG$CO2_EST_ADJ_MAX = MD_Res_SMZ_SIM_RESULT_AGG_MAX[match(MD_Res_SMZ_SIM_RESULT_AGG$SMZ_N, MD_Res_SMZ_SIM_RESULT_AGG_MAX$SMZ_N),"CO2_EST_ADJ_MAX"]
#MD_Res_SMZ_SIM_RESULT_AGG$CO2_EST_ADJ_MIN = MD_Res_SMZ_SIM_RESULT_AGG_MIN[match(MD_Res_SMZ_SIM_RESULT_AGG$SMZ_N, MD_Res_SMZ_SIM_RESULT_AGG_MIN$SMZ_N),"CO2_EST_ADJ_MIN"]


############
###Age Base Housing Stock
############

MD_Res_Base2_AGED <- MD_Res_Base2
##### Estimate Residential Emissions With added age constant
MD_Res_Base2_AGED$CO2_EST <- HH_ols$coef[1]+
					  HH_ols$coef[2]*(MD_Res_Base2$htdd*DDCCRESULT$HDD)+
					  HH_ols$coef[3]*(MD_Res_Base2$Age+TIME)+
					  HH_ols$coef[4]*MD_Res_Base2$SF1+
					  HH_ols$coef[5]*MD_Res_Base2$sqftstrc+
					  HH_ols$coef[6]*MD_Res_Base2$Stucco+
					  HH_ols$coef[7]*MD_Res_Base2$fire+
					  HH_ols$coef[8]*MD_Res_Base2$radiator
					  
##Count CO2 Emitting Households in the Aged housing stock
MD_Res_Base2_AGED$HH <- NA
MD_Res_Base2_AGED$HH[MD_Res_Base2$CO2_EST>0] <- 1
MD_Res_Base2_AGED$HH[MD_Res_Base2$CO2_EST<=0] <- 0
				
####Aggregate CO2 data in aged housing stock By SMZ
MD_Res_Base2_SMZ_AGED <-aggregate(cbind(MD_Res_Base2_AGED$CO2_EST, MD_Res_Base2_AGED$HH)~MD_Res_Base2_AGED$SMZ_N, data=MD_Res_Base2_AGED, sum, na.rm=TRUE)
names(MD_Res_Base2_SMZ_AGED)[1] <- "SMZ_N" 
names(MD_Res_Base2_SMZ_AGED)[2] <- "CO2_EST" 
names(MD_Res_Base2_SMZ_AGED)[3] <- "HH" 

###Calculate New Rate Per HH for Aged Housing Stock
MD_Res_Base2_SMZ_AGED$CO2_Rate_HH <- MD_Res_Base2_SMZ_AGED$CO2_EST/MD_Res_Base2_SMZ_AGED$HH

# Merge Rates and Activities for Aged Housing Stock
MD_Res_Base2_Activities_SMZ_AGED = merge(MD_Res_Base2_SMZ_AGED, Base_SMZ_Activities, by="SMZ_N")

###Apply New Rates to Aged Households
MD_Res_Base2_Activities_SMZ_AGED$CO2_base_HH <- MD_Res_Base2_Activities_SMZ_AGED$CO2_Rate_HH*MD_Res_Base2_Activities_SMZ_AGED$HH_BASE

###Replace Errors with Zero
MD_Res_Base2_Activities_SMZ_AGED[is.na(MD_Res_Base2_Activities_SMZ_AGED)] <- 0
MD_Res_Base2_Activities_SMZ_AGED[MD_Res_Base2_Activities_SMZ_AGED==-Inf] <- 0
MD_Res_Base2_Activities_SMZ_AGED[MD_Res_Base2_Activities_SMZ_AGED==Inf] <- 0

###Adjusted CO2 Rate Per Aged Household
MD_Res_Base2_Activities_SMZ_AGED$CO2_Rate_HH_ADJ <- MD_Res_Base2_Activities_SMZ_AGED$CO2_Rate_HH*CO2_HH_Rate_Adj

###Apply Adjusted Aged Rates to Activities
MD_Res_Base2_Activities_SMZ_AGED$CO2_base_HH_AGED <- MD_Res_Base2_Activities_SMZ_AGED$CO2_Rate_HH_ADJ*MD_Res_Base2_Activities_SMZ_AGED$HH_BASE

###Sum Total Aged Base Emissions
MD_Res_Base2_Activities_SMZ_AGED$CO2_base_HH_Total_AGED <- MD_Res_Base2_Activities_SMZ_AGED$CO2_base_HH_AGED #+ MD_Res_Base2_Activities_SMZ$CO2_base_OFF + MD_Res_Base2_Activities_SMZ$CO2_base_OTH

###Add Aged Base Emissions to Projected Emissions Growth
MD_Res_SMZ_SIM_RESULT_AGG$CO2_base_HH_AGED = MD_Res_Base2_Activities_SMZ_AGED[match(MD_Res_SMZ_SIM_RESULT_AGG$SMZ_N, MD_Res_Base2_Activities_SMZ_AGED$SMZ_N),"CO2_base_HH_AGED"]

###Add Base Emissions to Projected Emissions Growth
MD_Res_SMZ_SIM_RESULT_AGG$CO2_base_HH = MD_Res_Base2_Activities_SMZ[match(MD_Res_SMZ_SIM_RESULT_AGG$SMZ_N, MD_Res_Base2_Activities_SMZ$SMZ_N),"CO2_base_HH"]

MD_Res_SMZ_SIM_RESULT_AGG$CO2_Proj_HH <- MD_Res_SMZ_SIM_RESULT_AGG$CO2_base_HH_AGED + MD_Res_SMZ_SIM_RESULT_AGG$CO2_EST_ADJ

###Clean Final Data Set
MD_Res_SMZ_SIM_RESULT_AGG[is.na(MD_Res_SMZ_SIM_RESULT_AGG)] <- 0

###Write Final Res CO2 Output
write.csv(MD_Res_SMZ_SIM_RESULT_AGG[,c("SMZ_N","CO2_base_HH","CO2_Proj_HH")], file=(paste(OUTPATH,"MD_BEM_RES_CO2.csv",sep="")),row.names=FALSE)

###Take out the trash
file.remove(paste(TEMPATH,"RES_CO2_SAMPLE.csv",sep=""))
					  
			
###END MD_Res CO2###

###START VA_Res CO2###

##Create Count Dummy Variable Households
VA_Res_Base$HH <- 1

##### Estimate Probability of Residential Emissions
VA_Res_Base$CO2_PROB <- exp(wlogit$coef[1]+
						  wlogit$coef[2]*VA_Res_Base$htdd+
						  wlogit$coef[3]*VA_Res_Base$Age+
						  wlogit$coef[4]*VA_Res_Base$Beds+
						  wlogit$coef[5]*VA_Res_Base$SF1)/
						(1+exp(wlogit$coef[1]+
						  wlogit$coef[2]*VA_Res_Base$htdd+
						  wlogit$coef[3]*VA_Res_Base$Age+
						  wlogit$coef[4]*VA_Res_Base$Beds+
						  wlogit$coef[5]*VA_Res_Base$SF1))
###Replace Errors with Zero
VA_Res_Base[is.na(VA_Res_Base)] <- 0
VA_Res_Base[VA_Res_Base==-Inf] <- 0
VA_Res_Base[VA_Res_Base==Inf] <- 0

##### Select observations based on Probability of CO2 Emissions
VA_Res_Base2 <- VA_Res_Base[which(VA_Res_Base$CO2_PROB>.99),]

##### Select observations based on CO2 Emissions Greater than zero
RECS2 <- RECS[which(RECS$TCO2>0),]

#####Fit HH_ols
HH_ols <- lm(TCO2 ~ HD65 + AGE + SF1 + TOTSQFT + WALLTYPE4 + EQUIPM10 + EQUIPM3, data = RECS2)
summary(HH_ols)

##### Estimate Residential Emissions
VA_Res_Base2$CO2_EST <- HH_ols$coef[1]+
					  HH_ols$coef[2]*VA_Res_Base2$htdd+
					  HH_ols$coef[3]*VA_Res_Base2$Age+
					  HH_ols$coef[4]*VA_Res_Base2$SF1+
					  HH_ols$coef[5]*VA_Res_Base2$sqftstrc+
					  HH_ols$coef[6]*VA_Res_Base2$Stucco+
					  HH_ols$coef[7]*VA_Res_Base2$fire+
					  HH_ols$coef[8]*VA_Res_Base2$radiator

					  
##Count CO2 Emitting Households
VA_Res_Base2$HH <- NA
VA_Res_Base2$HH[VA_Res_Base2$CO2_EST>0] <- 1
VA_Res_Base2$HH[VA_Res_Base2$CO2_EST<=0] <- 0
				
####Aggregate data By SMZ
VA_Res_Base2_SMZ <-aggregate(cbind(VA_Res_Base2$CO2_EST, VA_Res_Base2$HH)~VA_Res_Base2$SMZ_N, data=VA_Res_Base2, sum, na.rm=TRUE)
names(VA_Res_Base2_SMZ)[1] <- "SMZ_N" 
names(VA_Res_Base2_SMZ)[2] <- "CO2_EST" 
names(VA_Res_Base2_SMZ)[3] <- "HH" 

VA_Res_Base2_SMZ_SQFT <-aggregate(cbind(VA_Res_Base2$sqftstrc)~VA_Res_Base2$SMZ_N, data=VA_Res_Base2, mean, na.rm=TRUE) 
names(VA_Res_Base2_SMZ_SQFT)[1] <- "SMZ_N" 
names(VA_Res_Base2_SMZ_SQFT)[2] <- "SQFT" 

###Rate Per HH
VA_Res_Base2_SMZ$CO2_Rate_HH <- VA_Res_Base2_SMZ$CO2_EST/VA_Res_Base2_SMZ$HH

# merge Rates and Activities
VA_Res_Base2_Activities_SMZ = merge(VA_Res_Base2_SMZ, Base_SMZ_Activities, by="SMZ_N")

###Apply Rates to Households
VA_Res_Base2_Activities_SMZ$CO2_base_HH <- VA_Res_Base2_Activities_SMZ$CO2_Rate_HH*VA_Res_Base2_Activities_SMZ$HH_BASE

###Replace Errors with Zero
VA_Res_Base2_Activities_SMZ[is.na(VA_Res_Base2_Activities_SMZ)] <- 0
VA_Res_Base2_Activities_SMZ[VA_Res_Base2_Activities_SMZ==-Inf] <- 0
VA_Res_Base2_Activities_SMZ[VA_Res_Base2_Activities_SMZ==Inf] <- 0

###Emission Rate Adjustment Factor
VA_Res_Base2_Activities_SMZ$Tot_HH_CO2 <- VA_Res_Base2_Activities_SMZ$CO2_base_HH # + VA_Res_Base2_Activities_SMZ$CO2_base_OFF + VA_Res_Base2_Activities_SMZ$CO2_base_OTH
Tot_BASE_HH_CO2 <- sum(VA_Res_Base2_Activities_SMZ$Tot_HH_CO2, na.rm = TRUE)*0.000001
CO2_HH_Rate_Adj<-(VA_RES_CO2con/Tot_BASE_HH_CO2)

###Adjusted Rate Per Household
VA_Res_Base2_Activities_SMZ$CO2_Rate_HH_ADJ <- VA_Res_Base2_Activities_SMZ$CO2_Rate_HH*CO2_HH_Rate_Adj

###Apply Adjusted Rates to Activities
VA_Res_Base2_Activities_SMZ$CO2_base_HH <- VA_Res_Base2_Activities_SMZ$CO2_Rate_HH_ADJ*VA_Res_Base2_Activities_SMZ$HH_BASE

###Sum Total Base Emissions
VA_Res_Base2_Activities_SMZ$CO2_base_HH_Total <- VA_Res_Base2_Activities_SMZ$CO2_base_HH #Leave for additional household type expansion

#Merge Base Data, Rates and Projected Activities
VA_Res_Base2_Activities_SMZ_PROJ = merge(VA_Res_Base2_Activities_SMZ, Proj_SMZ_Activities, by="SMZ_N")

# Export updated dataset
#write.csv(VA_Res_Base2_Activities_SMZ_PROJ,file="D:/BEM2/OUTPUT/check.csv")


###Calculate Projected Growth
PROJ_GROWTH_HH <- data.frame(VA_Res_Base2_Activities_SMZ_PROJ$SMZ_N, (VA_Res_Base2_Activities_SMZ_PROJ$HH_PROJ-VA_Res_Base2_Activities_SMZ_PROJ$HH_BASE))
names(PROJ_GROWTH_HH) <- c("SMZ_N","PROJ_GROWTH_HH")

############
###New Housing Stock Simulator
############
VA_Res_SMZ_SIM<-PROJ_GROWTH_HH

#Attach HDD and SQFT Data
VA_Res_SMZ_SIM$HDD = SMZ_HDD[match(VA_Res_SMZ_SIM$SMZ_N, SMZ_HDD$SMZ_N),"HDD"] 
VA_Res_SMZ_SIM$SQFT = VA_Res_Base2_SMZ_SQFT[match(VA_Res_SMZ_SIM$SMZ_N, VA_Res_Base2_SMZ_SQFT$SMZ_N),"SQFT"] 

#Sampling Function
sampHH<-function(x){

sample(PROJ_GROWTH_HH + 1, 1)

#Sim Housing Data
VA_Res_SMZ_SIM$Age <- sapply(VA_Res_SMZ_SIM$PROJ_GROWTH_HH*TIME + 1, sample, 1) - 1
VA_Res_SMZ_SIM$SF1 <- sapply(VA_Res_SMZ_SIM$PROJ_GROWTH_HH + 1, sample, 1) - 1
VA_Res_SMZ_SIM$sqftstrc <- sapply(VA_Res_SMZ_SIM$PROJ_GROWTH_HH*VA_Res_SMZ_SIM$SQFT + 1, sample, 1) - 1
VA_Res_SMZ_SIM$Stucco <- sapply(VA_Res_SMZ_SIM$PROJ_GROWTH_HH + 1, sample, 1) - 1
VA_Res_SMZ_SIM$fire <- sapply(VA_Res_SMZ_SIM$PROJ_GROWTH_HH + 1, sample, 1) - 1
VA_Res_SMZ_SIM$radiator <- sapply(VA_Res_SMZ_SIM$PROJ_GROWTH_HH + 1, sample, 1) - 1

##### Estimate New Stock Residential Emissions
VA_Res_SMZ_SIM$CO2_EST <- HH_ols$coef[1]+
					  HH_ols$coef[2]*(VA_Res_SMZ_SIM$HDD*DDCCRESULT$HDD)+
					  HH_ols$coef[3]*VA_Res_SMZ_SIM$Age+
					  HH_ols$coef[4]*VA_Res_SMZ_SIM$SF1+
					  HH_ols$coef[5]*VA_Res_SMZ_SIM$sqftstrc+
					  HH_ols$coef[6]*VA_Res_SMZ_SIM$Stucco+
					  HH_ols$coef[7]*VA_Res_SMZ_SIM$fire+
					  HH_ols$coef[8]*VA_Res_SMZ_SIM$radiator

###Apply Emissions Adjustment to Projected New Stock
VA_Res_SMZ_SIM$CO2_EST_ADJ <- VA_Res_SMZ_SIM$CO2_EST*CO2_HH_Rate_Adj
write.table(VA_Res_SMZ_SIM[c("SMZ_N","CO2_EST_ADJ")],file=(paste(TEMPATH,"RES_CO2_SAMPLE.csv",sep="")), sep=",", row.names=FALSE, col.names=FALSE, append=TRUE)
}

replicate(n = 10000, expr = sampHH())

VA_Res_SMZ_SIM_RESULT<- read.csv(paste(TEMPATH,"RES_CO2_SAMPLE.csv",sep="")) #Read back in simulated housing stock
names(VA_Res_SMZ_SIM_RESULT) <- c("SMZ_N","CO2_EST_ADJ")

####Aggregate CO2 data in simulated housing stock By SMZ
VA_Res_SMZ_SIM_RESULT_AGG <-aggregate(cbind(VA_Res_SMZ_SIM_RESULT$CO2_EST_ADJ)~VA_Res_SMZ_SIM_RESULT$SMZ_N, data=VA_Res_SMZ_SIM_RESULT, mean, na.rm=FALSE)
names(VA_Res_SMZ_SIM_RESULT_AGG)[1] <- "SMZ_N" 
names(VA_Res_SMZ_SIM_RESULT_AGG)[2] <- "CO2_EST_ADJ" 

#VA_Res_SMZ_SIM_RESULT_AGG_MAX <-aggregate(cbind(VA_Res_SMZ_SIM_RESULT2$CO2_EST_ADJ)~VA_Res_SMZ_SIM_RESULT2$SMZ_N, data=VA_Res_SMZ_SIM_RESULT2, max, na.rm=TRUE)
#names(VA_Res_SMZ_SIM_RESULT_AGG_MAX)[1] <- "SMZ_N" 
#names(VA_Res_SMZ_SIM_RESULT_AGG_MAX)[2] <- "CO2_EST_ADJ_MAX"

#VA_Res_SMZ_SIM_RESULT_AGG_MIN <-aggregate(cbind(VA_Res_SMZ_SI#M_RESULT2$CO2_EST_ADJ)~VA_Res_SMZ_SIM_RESULT2$SMZ_N, data=VA_Res_SMZ_SIM_RESULT2, min, na.rm=TRUE)
#names(VA_Res_SMZ_SIM_RESULT_AGG_MIN)[1] <- "SMZ_N" 
#names(VA_Res_SMZ_SIM_RESULT_AGG_MIN)[2] <- "CO2_EST_ADJ_MIN"

###Add MAX and MIN Emissions to Average
#VA_Res_SMZ_SIM_RESULT_AGG$CO2_EST_ADJ_MAX = VA_Res_SMZ_SIM_RESULT_AGG_MAX[match(VA_Res_SMZ_SIM_RESULT_AGG$SMZ_N, VA_Res_SMZ_SIM_RESULT_AGG_MAX$SMZ_N),"CO2_EST_ADJ_MAX"]
#VA_Res_SMZ_SIM_RESULT_AGG$CO2_EST_ADJ_MIN = VA_Res_SMZ_SIM_RESULT_AGG_MIN[match(VA_Res_SMZ_SIM_RESULT_AGG$SMZ_N, VA_Res_SMZ_SIM_RESULT_AGG_MIN$SMZ_N),"CO2_EST_ADJ_MIN"]


############
###Age Base Housing Stock
############

VA_Res_Base2_AGED <- VA_Res_Base2
##### Estimate Residential Emissions With added age constant
VA_Res_Base2_AGED$CO2_EST <- HH_ols$coef[1]+
					  HH_ols$coef[2]*(VA_Res_Base2$htdd*DDCCRESULT$HDD)+
					  HH_ols$coef[3]*(VA_Res_Base2$Age+TIME)+
					  HH_ols$coef[4]*VA_Res_Base2$SF1+
					  HH_ols$coef[5]*VA_Res_Base2$sqftstrc+
					  HH_ols$coef[6]*VA_Res_Base2$Stucco+
					  HH_ols$coef[7]*VA_Res_Base2$fire+
					  HH_ols$coef[8]*VA_Res_Base2$radiator
					  
##Count CO2 Emitting Households in the Aged housing stock
VA_Res_Base2_AGED$HH <- NA
VA_Res_Base2_AGED$HH[VA_Res_Base2$CO2_EST>0] <- 1
VA_Res_Base2_AGED$HH[VA_Res_Base2$CO2_EST<=0] <- 0
				
####Aggregate CO2 data in aged housing stock By SMZ
VA_Res_Base2_SMZ_AGED <-aggregate(cbind(VA_Res_Base2_AGED$CO2_EST, VA_Res_Base2_AGED$HH)~VA_Res_Base2_AGED$SMZ_N, data=VA_Res_Base2_AGED, sum, na.rm=TRUE)
names(VA_Res_Base2_SMZ_AGED)[1] <- "SMZ_N" 
names(VA_Res_Base2_SMZ_AGED)[2] <- "CO2_EST" 
names(VA_Res_Base2_SMZ_AGED)[3] <- "HH" 

###Calculate New Rate Per HH for Aged Housing Stock
VA_Res_Base2_SMZ_AGED$CO2_Rate_HH <- VA_Res_Base2_SMZ_AGED$CO2_EST/VA_Res_Base2_SMZ_AGED$HH

# Merge Rates and Activities for Aged Housing Stock
VA_Res_Base2_Activities_SMZ_AGED = merge(VA_Res_Base2_SMZ_AGED, Base_SMZ_Activities, by="SMZ_N")

###Apply New Rates to Aged Households
VA_Res_Base2_Activities_SMZ_AGED$CO2_base_HH <- VA_Res_Base2_Activities_SMZ_AGED$CO2_Rate_HH*VA_Res_Base2_Activities_SMZ_AGED$HH_BASE

###Replace Errors with Zero
VA_Res_Base2_Activities_SMZ_AGED[is.na(VA_Res_Base2_Activities_SMZ_AGED)] <- 0
VA_Res_Base2_Activities_SMZ_AGED[VA_Res_Base2_Activities_SMZ_AGED==-Inf] <- 0
VA_Res_Base2_Activities_SMZ_AGED[VA_Res_Base2_Activities_SMZ_AGED==Inf] <- 0

###Adjusted CO2 Rate Per Aged Household
VA_Res_Base2_Activities_SMZ_AGED$CO2_Rate_HH_ADJ <- VA_Res_Base2_Activities_SMZ_AGED$CO2_Rate_HH*CO2_HH_Rate_Adj

###Apply Adjusted Aged Rates to Activities
VA_Res_Base2_Activities_SMZ_AGED$CO2_base_HH_AGED <- VA_Res_Base2_Activities_SMZ_AGED$CO2_Rate_HH_ADJ*VA_Res_Base2_Activities_SMZ_AGED$HH_BASE

###Sum Total Aged Base Emissions
VA_Res_Base2_Activities_SMZ_AGED$CO2_base_HH_Total_AGED <- VA_Res_Base2_Activities_SMZ_AGED$CO2_base_HH_AGED #+ VA_Res_Base2_Activities_SMZ$CO2_base_OFF + VA_Res_Base2_Activities_SMZ$CO2_base_OTH

###Add Aged Base Emissions to Projected Emissions Growth
VA_Res_SMZ_SIM_RESULT_AGG$CO2_base_HH_AGED = VA_Res_Base2_Activities_SMZ_AGED[match(VA_Res_SMZ_SIM_RESULT_AGG$SMZ_N, VA_Res_Base2_Activities_SMZ_AGED$SMZ_N),"CO2_base_HH_AGED"]

###Add Base Emissions to Projected Emissions Growth
VA_Res_SMZ_SIM_RESULT_AGG$CO2_base_HH = VA_Res_Base2_Activities_SMZ[match(VA_Res_SMZ_SIM_RESULT_AGG$SMZ_N, VA_Res_Base2_Activities_SMZ$SMZ_N),"CO2_base_HH"]

VA_Res_SMZ_SIM_RESULT_AGG$CO2_Proj_HH <- VA_Res_SMZ_SIM_RESULT_AGG$CO2_base_HH_AGED + VA_Res_SMZ_SIM_RESULT_AGG$CO2_EST_ADJ

###Clean Final Data Set
VA_Res_SMZ_SIM_RESULT_AGG[is.na(VA_Res_SMZ_SIM_RESULT_AGG)] <- 0

###Write Final Res CO2 Output
write.csv(VA_Res_SMZ_SIM_RESULT_AGG[,c("SMZ_N","CO2_base_HH","CO2_Proj_HH")], file=(paste(OUTPATH,"VA_BEM_RES_CO2.csv",sep="")),row.names=FALSE)

###Take out the trash
file.remove(paste(TEMPATH,"RES_CO2_SAMPLE.csv",sep=""))
					  
			
###END VA_Res CO2###

###START PA_Res CO2###

##Create Count Dummy Variable Households
PA_Res_Base$HH <- 1

##### Estimate Probability of Residential Emissions
PA_Res_Base$CO2_PROB <- exp(wlogit$coef[1]+
						  wlogit$coef[2]*PA_Res_Base$htdd+
						  wlogit$coef[3]*PA_Res_Base$Age+
						  wlogit$coef[4]*PA_Res_Base$Beds+
						  wlogit$coef[5]*PA_Res_Base$SF1)/
						(1+exp(wlogit$coef[1]+
						  wlogit$coef[2]*PA_Res_Base$htdd+
						  wlogit$coef[3]*PA_Res_Base$Age+
						  wlogit$coef[4]*PA_Res_Base$Beds+
						  wlogit$coef[5]*PA_Res_Base$SF1))
###Replace Errors with Zero
PA_Res_Base[is.na(PA_Res_Base)] <- 0
PA_Res_Base[PA_Res_Base==-Inf] <- 0
PA_Res_Base[PA_Res_Base==Inf] <- 0

##### Select observations based on Probability of CO2 Emissions
PA_Res_Base2 <- PA_Res_Base[which(PA_Res_Base$CO2_PROB>.99),]

##### Select observations based on CO2 Emissions Greater than zero
RECS2 <- RECS[which(RECS$TCO2>0),]

#####Fit HH_ols
HH_ols <- lm(TCO2 ~ HD65 + AGE + SF1 + TOTSQFT + WALLTYPE4 + EQUIPM10 + EQUIPM3, data = RECS2)
summary(HH_ols)

##### Estimate Residential Emissions
PA_Res_Base2$CO2_EST <- HH_ols$coef[1]+
					  HH_ols$coef[2]*PA_Res_Base2$htdd+
					  HH_ols$coef[3]*PA_Res_Base2$Age+
					  HH_ols$coef[4]*PA_Res_Base2$SF1+
					  HH_ols$coef[5]*PA_Res_Base2$sqftstrc+
					  HH_ols$coef[6]*PA_Res_Base2$Stucco+
					  HH_ols$coef[7]*PA_Res_Base2$fire+
					  HH_ols$coef[8]*PA_Res_Base2$radiator

					  
##Count CO2 Emitting Households
PA_Res_Base2$HH <- NA
PA_Res_Base2$HH[PA_Res_Base2$CO2_EST>0] <- 1
PA_Res_Base2$HH[PA_Res_Base2$CO2_EST<=0] <- 0
				
####Aggregate data By SMZ
PA_Res_Base2_SMZ <-aggregate(cbind(PA_Res_Base2$CO2_EST, PA_Res_Base2$HH)~PA_Res_Base2$SMZ_N, data=PA_Res_Base2, sum, na.rm=TRUE)
names(PA_Res_Base2_SMZ)[1] <- "SMZ_N" 
names(PA_Res_Base2_SMZ)[2] <- "CO2_EST" 
names(PA_Res_Base2_SMZ)[3] <- "HH" 

PA_Res_Base2_SMZ_SQFT <-aggregate(cbind(PA_Res_Base2$sqftstrc)~PA_Res_Base2$SMZ_N, data=PA_Res_Base2, mean, na.rm=TRUE) 
names(PA_Res_Base2_SMZ_SQFT)[1] <- "SMZ_N" 
names(PA_Res_Base2_SMZ_SQFT)[2] <- "SQFT" 

###Rate Per HH
PA_Res_Base2_SMZ$CO2_Rate_HH <- PA_Res_Base2_SMZ$CO2_EST/PA_Res_Base2_SMZ$HH

# merge Rates and Activities
PA_Res_Base2_Activities_SMZ = merge(PA_Res_Base2_SMZ, Base_SMZ_Activities, by="SMZ_N")

###Apply Rates to Households
PA_Res_Base2_Activities_SMZ$CO2_base_HH <- PA_Res_Base2_Activities_SMZ$CO2_Rate_HH*PA_Res_Base2_Activities_SMZ$HH_BASE

###Replace Errors with Zero
PA_Res_Base2_Activities_SMZ[is.na(PA_Res_Base2_Activities_SMZ)] <- 0
PA_Res_Base2_Activities_SMZ[PA_Res_Base2_Activities_SMZ==-Inf] <- 0
PA_Res_Base2_Activities_SMZ[PA_Res_Base2_Activities_SMZ==Inf] <- 0

###Emission Rate Adjustment Factor
PA_Res_Base2_Activities_SMZ$Tot_HH_CO2 <- PA_Res_Base2_Activities_SMZ$CO2_base_HH # + PA_Res_Base2_Activities_SMZ$CO2_base_OFF + PA_Res_Base2_Activities_SMZ$CO2_base_OTH
Tot_BASE_HH_CO2 <- sum(PA_Res_Base2_Activities_SMZ$Tot_HH_CO2, na.rm = TRUE)*0.000001
CO2_HH_Rate_Adj<-(PA_RES_CO2con/Tot_BASE_HH_CO2)

###Adjusted Rate Per Household
PA_Res_Base2_Activities_SMZ$CO2_Rate_HH_ADJ <- PA_Res_Base2_Activities_SMZ$CO2_Rate_HH*CO2_HH_Rate_Adj

###Apply Adjusted Rates to Activities
PA_Res_Base2_Activities_SMZ$CO2_base_HH <- PA_Res_Base2_Activities_SMZ$CO2_Rate_HH_ADJ*PA_Res_Base2_Activities_SMZ$HH_BASE

###Sum Total Base Emissions
PA_Res_Base2_Activities_SMZ$CO2_base_HH_Total <- PA_Res_Base2_Activities_SMZ$CO2_base_HH #Leave for additional household type expansion

#Merge Base Data, Rates and Projected Activities
PA_Res_Base2_Activities_SMZ_PROJ = merge(PA_Res_Base2_Activities_SMZ, Proj_SMZ_Activities, by="SMZ_N")

# Export updated dataset
#write.csv(PA_Res_Base2_Activities_SMZ_PROJ,file="D:/BEM2/OUTPUT/check.csv")


###Calculate Projected Growth
PROJ_GROWTH_HH <- data.frame(PA_Res_Base2_Activities_SMZ_PROJ$SMZ_N, (PA_Res_Base2_Activities_SMZ_PROJ$HH_PROJ-PA_Res_Base2_Activities_SMZ_PROJ$HH_BASE))
names(PROJ_GROWTH_HH) <- c("SMZ_N","PROJ_GROWTH_HH")

############
###New Housing Stock Simulator
############
PA_Res_SMZ_SIM<-PROJ_GROWTH_HH

#Attach HDD and SQFT Data
PA_Res_SMZ_SIM$HDD = SMZ_HDD[match(PA_Res_SMZ_SIM$SMZ_N, SMZ_HDD$SMZ_N),"HDD"] 
PA_Res_SMZ_SIM$SQFT = PA_Res_Base2_SMZ_SQFT[match(PA_Res_SMZ_SIM$SMZ_N, PA_Res_Base2_SMZ_SQFT$SMZ_N),"SQFT"] 

#Sampling Function
sampHH<-function(x){

sample(PROJ_GROWTH_HH + 1, 1)

#Sim Housing Data
PA_Res_SMZ_SIM$Age <- sapply(PA_Res_SMZ_SIM$PROJ_GROWTH_HH*TIME + 1, sample, 1) - 1
PA_Res_SMZ_SIM$SF1 <- sapply(PA_Res_SMZ_SIM$PROJ_GROWTH_HH + 1, sample, 1) - 1
PA_Res_SMZ_SIM$sqftstrc <- sapply(PA_Res_SMZ_SIM$PROJ_GROWTH_HH*PA_Res_SMZ_SIM$SQFT + 1, sample, 1) - 1
PA_Res_SMZ_SIM$Stucco <- sapply(PA_Res_SMZ_SIM$PROJ_GROWTH_HH + 1, sample, 1) - 1
PA_Res_SMZ_SIM$fire <- sapply(PA_Res_SMZ_SIM$PROJ_GROWTH_HH + 1, sample, 1) - 1
PA_Res_SMZ_SIM$radiator <- sapply(PA_Res_SMZ_SIM$PROJ_GROWTH_HH + 1, sample, 1) - 1

##### Estimate New Stock Residential Emissions
PA_Res_SMZ_SIM$CO2_EST <- HH_ols$coef[1]+
					  HH_ols$coef[2]*(PA_Res_SMZ_SIM$HDD*DDCCRESULT$HDD)+
					  HH_ols$coef[3]*PA_Res_SMZ_SIM$Age+
					  HH_ols$coef[4]*PA_Res_SMZ_SIM$SF1+
					  HH_ols$coef[5]*PA_Res_SMZ_SIM$sqftstrc+
					  HH_ols$coef[6]*PA_Res_SMZ_SIM$Stucco+
					  HH_ols$coef[7]*PA_Res_SMZ_SIM$fire+
					  HH_ols$coef[8]*PA_Res_SMZ_SIM$radiator

###Apply Emissions Adjustment to Projected New Stock
PA_Res_SMZ_SIM$CO2_EST_ADJ <- PA_Res_SMZ_SIM$CO2_EST*CO2_HH_Rate_Adj
write.table(PA_Res_SMZ_SIM[c("SMZ_N","CO2_EST_ADJ")],file=(paste(TEMPATH,"RES_CO2_SAMPLE.csv",sep="")), sep=",", row.names=FALSE, col.names=FALSE, append=TRUE)
}

replicate(n = 10000, expr = sampHH())

PA_Res_SMZ_SIM_RESULT<- read.csv(paste(TEMPATH,"RES_CO2_SAMPLE.csv",sep="")) #Read back in simulated housing stock
names(PA_Res_SMZ_SIM_RESULT) <- c("SMZ_N","CO2_EST_ADJ")

####Aggregate CO2 data in simulated housing stock By SMZ
PA_Res_SMZ_SIM_RESULT_AGG <-aggregate(cbind(PA_Res_SMZ_SIM_RESULT$CO2_EST_ADJ)~PA_Res_SMZ_SIM_RESULT$SMZ_N, data=PA_Res_SMZ_SIM_RESULT, mean, na.rm=FALSE)
names(PA_Res_SMZ_SIM_RESULT_AGG)[1] <- "SMZ_N" 
names(PA_Res_SMZ_SIM_RESULT_AGG)[2] <- "CO2_EST_ADJ" 

#PA_Res_SMZ_SIM_RESULT_AGG_MAX <-aggregate(cbind(PA_Res_SMZ_SIM_RESULT2$CO2_EST_ADJ)~PA_Res_SMZ_SIM_RESULT2$SMZ_N, data=PA_Res_SMZ_SIM_RESULT2, max, na.rm=TRUE)
#names(PA_Res_SMZ_SIM_RESULT_AGG_MAX)[1] <- "SMZ_N" 
#names(PA_Res_SMZ_SIM_RESULT_AGG_MAX)[2] <- "CO2_EST_ADJ_MAX"

#PA_Res_SMZ_SIM_RESULT_AGG_MIN <-aggregate(cbind(PA_Res_SMZ_SI#M_RESULT2$CO2_EST_ADJ)~PA_Res_SMZ_SIM_RESULT2$SMZ_N, data=PA_Res_SMZ_SIM_RESULT2, min, na.rm=TRUE)
#names(PA_Res_SMZ_SIM_RESULT_AGG_MIN)[1] <- "SMZ_N" 
#names(PA_Res_SMZ_SIM_RESULT_AGG_MIN)[2] <- "CO2_EST_ADJ_MIN"

###Add MAX and MIN Emissions to Average
#PA_Res_SMZ_SIM_RESULT_AGG$CO2_EST_ADJ_MAX = PA_Res_SMZ_SIM_RESULT_AGG_MAX[match(PA_Res_SMZ_SIM_RESULT_AGG$SMZ_N, PA_Res_SMZ_SIM_RESULT_AGG_MAX$SMZ_N),"CO2_EST_ADJ_MAX"]
#PA_Res_SMZ_SIM_RESULT_AGG$CO2_EST_ADJ_MIN = PA_Res_SMZ_SIM_RESULT_AGG_MIN[match(PA_Res_SMZ_SIM_RESULT_AGG$SMZ_N, PA_Res_SMZ_SIM_RESULT_AGG_MIN$SMZ_N),"CO2_EST_ADJ_MIN"]


############
###Age Base Housing Stock
############

PA_Res_Base2_AGED <- PA_Res_Base2
##### Estimate Residential Emissions With added age constant
PA_Res_Base2_AGED$CO2_EST <- HH_ols$coef[1]+
					  HH_ols$coef[2]*(PA_Res_Base2$htdd*DDCCRESULT$HDD)+
					  HH_ols$coef[3]*(PA_Res_Base2$Age+TIME)+
					  HH_ols$coef[4]*PA_Res_Base2$SF1+
					  HH_ols$coef[5]*PA_Res_Base2$sqftstrc+
					  HH_ols$coef[6]*PA_Res_Base2$Stucco+
					  HH_ols$coef[7]*PA_Res_Base2$fire+
					  HH_ols$coef[8]*PA_Res_Base2$radiator
					  
##Count CO2 Emitting Households in the Aged housing stock
PA_Res_Base2_AGED$HH <- NA
PA_Res_Base2_AGED$HH[PA_Res_Base2$CO2_EST>0] <- 1
PA_Res_Base2_AGED$HH[PA_Res_Base2$CO2_EST<=0] <- 0
				
####Aggregate CO2 data in aged housing stock By SMZ
PA_Res_Base2_SMZ_AGED <-aggregate(cbind(PA_Res_Base2_AGED$CO2_EST, PA_Res_Base2_AGED$HH)~PA_Res_Base2_AGED$SMZ_N, data=PA_Res_Base2_AGED, sum, na.rm=TRUE)
names(PA_Res_Base2_SMZ_AGED)[1] <- "SMZ_N" 
names(PA_Res_Base2_SMZ_AGED)[2] <- "CO2_EST" 
names(PA_Res_Base2_SMZ_AGED)[3] <- "HH" 

###Calculate New Rate Per HH for Aged Housing Stock
PA_Res_Base2_SMZ_AGED$CO2_Rate_HH <- PA_Res_Base2_SMZ_AGED$CO2_EST/PA_Res_Base2_SMZ_AGED$HH

# Merge Rates and Activities for Aged Housing Stock
PA_Res_Base2_Activities_SMZ_AGED = merge(PA_Res_Base2_SMZ_AGED, Base_SMZ_Activities, by="SMZ_N")

###Apply New Rates to Aged Households
PA_Res_Base2_Activities_SMZ_AGED$CO2_base_HH <- PA_Res_Base2_Activities_SMZ_AGED$CO2_Rate_HH*PA_Res_Base2_Activities_SMZ_AGED$HH_BASE

###Replace Errors with Zero
PA_Res_Base2_Activities_SMZ_AGED[is.na(PA_Res_Base2_Activities_SMZ_AGED)] <- 0
PA_Res_Base2_Activities_SMZ_AGED[PA_Res_Base2_Activities_SMZ_AGED==-Inf] <- 0
PA_Res_Base2_Activities_SMZ_AGED[PA_Res_Base2_Activities_SMZ_AGED==Inf] <- 0

###Adjusted CO2 Rate Per Aged Household
PA_Res_Base2_Activities_SMZ_AGED$CO2_Rate_HH_ADJ <- PA_Res_Base2_Activities_SMZ_AGED$CO2_Rate_HH*CO2_HH_Rate_Adj

###Apply Adjusted Aged Rates to Activities
PA_Res_Base2_Activities_SMZ_AGED$CO2_base_HH_AGED <- PA_Res_Base2_Activities_SMZ_AGED$CO2_Rate_HH_ADJ*PA_Res_Base2_Activities_SMZ_AGED$HH_BASE

###Sum Total Aged Base Emissions
PA_Res_Base2_Activities_SMZ_AGED$CO2_base_HH_Total_AGED <- PA_Res_Base2_Activities_SMZ_AGED$CO2_base_HH_AGED #+ PA_Res_Base2_Activities_SMZ$CO2_base_OFF + PA_Res_Base2_Activities_SMZ$CO2_base_OTH

###Add Aged Base Emissions to Projected Emissions Growth
PA_Res_SMZ_SIM_RESULT_AGG$CO2_base_HH_AGED = PA_Res_Base2_Activities_SMZ_AGED[match(PA_Res_SMZ_SIM_RESULT_AGG$SMZ_N, PA_Res_Base2_Activities_SMZ_AGED$SMZ_N),"CO2_base_HH_AGED"]

###Add Base Emissions to Projected Emissions Growth
PA_Res_SMZ_SIM_RESULT_AGG$CO2_base_HH = PA_Res_Base2_Activities_SMZ[match(PA_Res_SMZ_SIM_RESULT_AGG$SMZ_N, PA_Res_Base2_Activities_SMZ$SMZ_N),"CO2_base_HH"]

PA_Res_SMZ_SIM_RESULT_AGG$CO2_Proj_HH <- PA_Res_SMZ_SIM_RESULT_AGG$CO2_base_HH_AGED + PA_Res_SMZ_SIM_RESULT_AGG$CO2_EST_ADJ

###Clean Final Data Set
PA_Res_SMZ_SIM_RESULT_AGG[is.na(PA_Res_SMZ_SIM_RESULT_AGG)] <- 0

###Write Final Res CO2 Output
write.csv(PA_Res_SMZ_SIM_RESULT_AGG[,c("SMZ_N","CO2_base_HH","CO2_Proj_HH")], file=(paste(OUTPATH,"PA_BEM_RES_CO2.csv",sep="")),row.names=FALSE)

###Take out the trash
file.remove(paste(TEMPATH,"RES_CO2_SAMPLE.csv",sep=""))
					  
			
###END PA_Res CO2###

###START WV_Res CO2###

##Create Count Dummy Variable Households
WV_Res_Base$HH <- 1

##### Estimate Probability of Residential Emissions
WV_Res_Base$CO2_PROB <- exp(wlogit$coef[1]+
						  wlogit$coef[2]*WV_Res_Base$htdd+
						  wlogit$coef[3]*WV_Res_Base$Age+
						  wlogit$coef[4]*WV_Res_Base$Beds+
						  wlogit$coef[5]*WV_Res_Base$SF1)/
						(1+exp(wlogit$coef[1]+
						  wlogit$coef[2]*WV_Res_Base$htdd+
						  wlogit$coef[3]*WV_Res_Base$Age+
						  wlogit$coef[4]*WV_Res_Base$Beds+
						  wlogit$coef[5]*WV_Res_Base$SF1))
###Replace Errors with Zero
WV_Res_Base[is.na(WV_Res_Base)] <- 0
WV_Res_Base[WV_Res_Base==-Inf] <- 0
WV_Res_Base[WV_Res_Base==Inf] <- 0

##### Select observations based on Probability of CO2 Emissions
WV_Res_Base2 <- WV_Res_Base[which(WV_Res_Base$CO2_PROB>.99),]

##### Select observations based on CO2 Emissions Greater than zero
RECS2 <- RECS[which(RECS$TCO2>0),]

#####Fit HH_ols
HH_ols <- lm(TCO2 ~ HD65 + AGE + SF1 + TOTSQFT + WALLTYPE4 + EQUIPM10 + EQUIPM3, data = RECS2)
summary(HH_ols)

##### Estimate Residential Emissions
WV_Res_Base2$CO2_EST <- HH_ols$coef[1]+
					  HH_ols$coef[2]*WV_Res_Base2$htdd+
					  HH_ols$coef[3]*WV_Res_Base2$Age+
					  HH_ols$coef[4]*WV_Res_Base2$SF1+
					  HH_ols$coef[5]*WV_Res_Base2$sqftstrc+
					  HH_ols$coef[6]*WV_Res_Base2$Stucco+
					  HH_ols$coef[7]*WV_Res_Base2$fire+
					  HH_ols$coef[8]*WV_Res_Base2$radiator

					  
##Count CO2 Emitting Households
WV_Res_Base2$HH <- NA
WV_Res_Base2$HH[WV_Res_Base2$CO2_EST>0] <- 1
WV_Res_Base2$HH[WV_Res_Base2$CO2_EST<=0] <- 0
				
####Aggregate data By SMZ
WV_Res_Base2_SMZ <-aggregate(cbind(WV_Res_Base2$CO2_EST, WV_Res_Base2$HH)~WV_Res_Base2$SMZ_N, data=WV_Res_Base2, sum, na.rm=TRUE)
names(WV_Res_Base2_SMZ)[1] <- "SMZ_N" 
names(WV_Res_Base2_SMZ)[2] <- "CO2_EST" 
names(WV_Res_Base2_SMZ)[3] <- "HH" 

WV_Res_Base2_SMZ_SQFT <-aggregate(cbind(WV_Res_Base2$sqftstrc)~WV_Res_Base2$SMZ_N, data=WV_Res_Base2, mean, na.rm=TRUE) 
names(WV_Res_Base2_SMZ_SQFT)[1] <- "SMZ_N" 
names(WV_Res_Base2_SMZ_SQFT)[2] <- "SQFT" 

###Rate Per HH
WV_Res_Base2_SMZ$CO2_Rate_HH <- WV_Res_Base2_SMZ$CO2_EST/WV_Res_Base2_SMZ$HH

# merge Rates and Activities
WV_Res_Base2_Activities_SMZ = merge(WV_Res_Base2_SMZ, Base_SMZ_Activities, by="SMZ_N")

###Apply Rates to Households
WV_Res_Base2_Activities_SMZ$CO2_base_HH <- WV_Res_Base2_Activities_SMZ$CO2_Rate_HH*WV_Res_Base2_Activities_SMZ$HH_BASE

###Replace Errors with Zero
WV_Res_Base2_Activities_SMZ[is.na(WV_Res_Base2_Activities_SMZ)] <- 0
WV_Res_Base2_Activities_SMZ[WV_Res_Base2_Activities_SMZ==-Inf] <- 0
WV_Res_Base2_Activities_SMZ[WV_Res_Base2_Activities_SMZ==Inf] <- 0

###Emission Rate Adjustment Factor
WV_Res_Base2_Activities_SMZ$Tot_HH_CO2 <- WV_Res_Base2_Activities_SMZ$CO2_base_HH # + WV_Res_Base2_Activities_SMZ$CO2_base_OFF + WV_Res_Base2_Activities_SMZ$CO2_base_OTH
Tot_BASE_HH_CO2 <- sum(WV_Res_Base2_Activities_SMZ$Tot_HH_CO2, na.rm = TRUE)*0.000001
CO2_HH_Rate_Adj<-(WV_RES_CO2con/Tot_BASE_HH_CO2)

###Adjusted Rate Per Household
WV_Res_Base2_Activities_SMZ$CO2_Rate_HH_ADJ <- WV_Res_Base2_Activities_SMZ$CO2_Rate_HH*CO2_HH_Rate_Adj

###Apply Adjusted Rates to Activities
WV_Res_Base2_Activities_SMZ$CO2_base_HH <- WV_Res_Base2_Activities_SMZ$CO2_Rate_HH_ADJ*WV_Res_Base2_Activities_SMZ$HH_BASE

###Sum Total Base Emissions
WV_Res_Base2_Activities_SMZ$CO2_base_HH_Total <- WV_Res_Base2_Activities_SMZ$CO2_base_HH #Leave for additional household type expansion

#Merge Base Data, Rates and Projected Activities
WV_Res_Base2_Activities_SMZ_PROJ = merge(WV_Res_Base2_Activities_SMZ, Proj_SMZ_Activities, by="SMZ_N")

# Export updated dataset
#write.csv(WV_Res_Base2_Activities_SMZ_PROJ,file="D:/BEM2/OUTPUT/check.csv")


###Calculate Projected Growth
PROJ_GROWTH_HH <- data.frame(WV_Res_Base2_Activities_SMZ_PROJ$SMZ_N, (WV_Res_Base2_Activities_SMZ_PROJ$HH_PROJ-WV_Res_Base2_Activities_SMZ_PROJ$HH_BASE))
names(PROJ_GROWTH_HH) <- c("SMZ_N","PROJ_GROWTH_HH")

############
###New Housing Stock Simulator
############
WV_Res_SMZ_SIM<-PROJ_GROWTH_HH

#Attach HDD and SQFT Data
WV_Res_SMZ_SIM$HDD = SMZ_HDD[match(WV_Res_SMZ_SIM$SMZ_N, SMZ_HDD$SMZ_N),"HDD"] 
WV_Res_SMZ_SIM$SQFT = WV_Res_Base2_SMZ_SQFT[match(WV_Res_SMZ_SIM$SMZ_N, WV_Res_Base2_SMZ_SQFT$SMZ_N),"SQFT"] 

#Sampling Function
sampHH<-function(x){

sample(PROJ_GROWTH_HH + 1, 1)

#Sim Housing Data
WV_Res_SMZ_SIM$Age <- sapply(WV_Res_SMZ_SIM$PROJ_GROWTH_HH*TIME + 1, sample, 1) - 1
WV_Res_SMZ_SIM$SF1 <- sapply(WV_Res_SMZ_SIM$PROJ_GROWTH_HH + 1, sample, 1) - 1
WV_Res_SMZ_SIM$sqftstrc <- sapply(WV_Res_SMZ_SIM$PROJ_GROWTH_HH*WV_Res_SMZ_SIM$SQFT + 1, sample, 1) - 1
WV_Res_SMZ_SIM$Stucco <- sapply(WV_Res_SMZ_SIM$PROJ_GROWTH_HH + 1, sample, 1) - 1
WV_Res_SMZ_SIM$fire <- sapply(WV_Res_SMZ_SIM$PROJ_GROWTH_HH + 1, sample, 1) - 1
WV_Res_SMZ_SIM$radiator <- sapply(WV_Res_SMZ_SIM$PROJ_GROWTH_HH + 1, sample, 1) - 1

##### Estimate New Stock Residential Emissions
WV_Res_SMZ_SIM$CO2_EST <- HH_ols$coef[1]+
					  HH_ols$coef[2]*(WV_Res_SMZ_SIM$HDD*DDCCRESULT$HDD)+
					  HH_ols$coef[3]*WV_Res_SMZ_SIM$Age+
					  HH_ols$coef[4]*WV_Res_SMZ_SIM$SF1+
					  HH_ols$coef[5]*WV_Res_SMZ_SIM$sqftstrc+
					  HH_ols$coef[6]*WV_Res_SMZ_SIM$Stucco+
					  HH_ols$coef[7]*WV_Res_SMZ_SIM$fire+
					  HH_ols$coef[8]*WV_Res_SMZ_SIM$radiator

###Apply Emissions Adjustment to Projected New Stock
WV_Res_SMZ_SIM$CO2_EST_ADJ <- WV_Res_SMZ_SIM$CO2_EST*CO2_HH_Rate_Adj
write.table(WV_Res_SMZ_SIM[c("SMZ_N","CO2_EST_ADJ")],file=(paste(TEMPATH,"RES_CO2_SAMPLE.csv",sep="")), sep=",", row.names=FALSE, col.names=FALSE, append=TRUE)
}

replicate(n = 10000, expr = sampHH())

WV_Res_SMZ_SIM_RESULT<- read.csv(paste(TEMPATH,"RES_CO2_SAMPLE.csv",sep="")) #Read back in simulated housing stock
names(WV_Res_SMZ_SIM_RESULT) <- c("SMZ_N","CO2_EST_ADJ")

####Aggregate CO2 data in simulated housing stock By SMZ
WV_Res_SMZ_SIM_RESULT_AGG <-aggregate(cbind(WV_Res_SMZ_SIM_RESULT$CO2_EST_ADJ)~WV_Res_SMZ_SIM_RESULT$SMZ_N, data=WV_Res_SMZ_SIM_RESULT, mean, na.rm=FALSE)
names(WV_Res_SMZ_SIM_RESULT_AGG)[1] <- "SMZ_N" 
names(WV_Res_SMZ_SIM_RESULT_AGG)[2] <- "CO2_EST_ADJ" 

#WV_Res_SMZ_SIM_RESULT_AGG_MAX <-aggregate(cbind(WV_Res_SMZ_SIM_RESULT2$CO2_EST_ADJ)~WV_Res_SMZ_SIM_RESULT2$SMZ_N, data=WV_Res_SMZ_SIM_RESULT2, max, na.rm=TRUE)
#names(WV_Res_SMZ_SIM_RESULT_AGG_MAX)[1] <- "SMZ_N" 
#names(WV_Res_SMZ_SIM_RESULT_AGG_MAX)[2] <- "CO2_EST_ADJ_MAX"

#WV_Res_SMZ_SIM_RESULT_AGG_MIN <-aggregate(cbind(WV_Res_SMZ_SI#M_RESULT2$CO2_EST_ADJ)~WV_Res_SMZ_SIM_RESULT2$SMZ_N, data=WV_Res_SMZ_SIM_RESULT2, min, na.rm=TRUE)
#names(WV_Res_SMZ_SIM_RESULT_AGG_MIN)[1] <- "SMZ_N" 
#names(WV_Res_SMZ_SIM_RESULT_AGG_MIN)[2] <- "CO2_EST_ADJ_MIN"

###Add MAX and MIN Emissions to Average
#WV_Res_SMZ_SIM_RESULT_AGG$CO2_EST_ADJ_MAX = WV_Res_SMZ_SIM_RESULT_AGG_MAX[match(WV_Res_SMZ_SIM_RESULT_AGG$SMZ_N, WV_Res_SMZ_SIM_RESULT_AGG_MAX$SMZ_N),"CO2_EST_ADJ_MAX"]
#WV_Res_SMZ_SIM_RESULT_AGG$CO2_EST_ADJ_MIN = WV_Res_SMZ_SIM_RESULT_AGG_MIN[match(WV_Res_SMZ_SIM_RESULT_AGG$SMZ_N, WV_Res_SMZ_SIM_RESULT_AGG_MIN$SMZ_N),"CO2_EST_ADJ_MIN"]


############
###Age Base Housing Stock
############

WV_Res_Base2_AGED <- WV_Res_Base2
##### Estimate Residential Emissions With added age constant
WV_Res_Base2_AGED$CO2_EST <- HH_ols$coef[1]+
					  HH_ols$coef[2]*(WV_Res_Base2$htdd*DDCCRESULT$HDD)+
					  HH_ols$coef[3]*(WV_Res_Base2$Age+TIME)+
					  HH_ols$coef[4]*WV_Res_Base2$SF1+
					  HH_ols$coef[5]*WV_Res_Base2$sqftstrc+
					  HH_ols$coef[6]*WV_Res_Base2$Stucco+
					  HH_ols$coef[7]*WV_Res_Base2$fire+
					  HH_ols$coef[8]*WV_Res_Base2$radiator
					  
##Count CO2 Emitting Households in the Aged housing stock
WV_Res_Base2_AGED$HH <- NA
WV_Res_Base2_AGED$HH[WV_Res_Base2$CO2_EST>0] <- 1
WV_Res_Base2_AGED$HH[WV_Res_Base2$CO2_EST<=0] <- 0
				
####Aggregate CO2 data in aged housing stock By SMZ
WV_Res_Base2_SMZ_AGED <-aggregate(cbind(WV_Res_Base2_AGED$CO2_EST, WV_Res_Base2_AGED$HH)~WV_Res_Base2_AGED$SMZ_N, data=WV_Res_Base2_AGED, sum, na.rm=TRUE)
names(WV_Res_Base2_SMZ_AGED)[1] <- "SMZ_N" 
names(WV_Res_Base2_SMZ_AGED)[2] <- "CO2_EST" 
names(WV_Res_Base2_SMZ_AGED)[3] <- "HH" 

###Calculate New Rate Per HH for Aged Housing Stock
WV_Res_Base2_SMZ_AGED$CO2_Rate_HH <- WV_Res_Base2_SMZ_AGED$CO2_EST/WV_Res_Base2_SMZ_AGED$HH

# Merge Rates and Activities for Aged Housing Stock
WV_Res_Base2_Activities_SMZ_AGED = merge(WV_Res_Base2_SMZ_AGED, Base_SMZ_Activities, by="SMZ_N")

###Apply New Rates to Aged Households
WV_Res_Base2_Activities_SMZ_AGED$CO2_base_HH <- WV_Res_Base2_Activities_SMZ_AGED$CO2_Rate_HH*WV_Res_Base2_Activities_SMZ_AGED$HH_BASE

###Replace Errors with Zero
WV_Res_Base2_Activities_SMZ_AGED[is.na(WV_Res_Base2_Activities_SMZ_AGED)] <- 0
WV_Res_Base2_Activities_SMZ_AGED[WV_Res_Base2_Activities_SMZ_AGED==-Inf] <- 0
WV_Res_Base2_Activities_SMZ_AGED[WV_Res_Base2_Activities_SMZ_AGED==Inf] <- 0

###Adjusted CO2 Rate Per Aged Household
WV_Res_Base2_Activities_SMZ_AGED$CO2_Rate_HH_ADJ <- WV_Res_Base2_Activities_SMZ_AGED$CO2_Rate_HH*CO2_HH_Rate_Adj

###Apply Adjusted Aged Rates to Activities
WV_Res_Base2_Activities_SMZ_AGED$CO2_base_HH_AGED <- WV_Res_Base2_Activities_SMZ_AGED$CO2_Rate_HH_ADJ*WV_Res_Base2_Activities_SMZ_AGED$HH_BASE

###Sum Total Aged Base Emissions
WV_Res_Base2_Activities_SMZ_AGED$CO2_base_HH_Total_AGED <- WV_Res_Base2_Activities_SMZ_AGED$CO2_base_HH_AGED #+ WV_Res_Base2_Activities_SMZ$CO2_base_OFF + WV_Res_Base2_Activities_SMZ$CO2_base_OTH

###Add Aged Base Emissions to Projected Emissions Growth
WV_Res_SMZ_SIM_RESULT_AGG$CO2_base_HH_AGED = WV_Res_Base2_Activities_SMZ_AGED[match(WV_Res_SMZ_SIM_RESULT_AGG$SMZ_N, WV_Res_Base2_Activities_SMZ_AGED$SMZ_N),"CO2_base_HH_AGED"]

###Add Base Emissions to Projected Emissions Growth
WV_Res_SMZ_SIM_RESULT_AGG$CO2_base_HH = WV_Res_Base2_Activities_SMZ[match(WV_Res_SMZ_SIM_RESULT_AGG$SMZ_N, WV_Res_Base2_Activities_SMZ$SMZ_N),"CO2_base_HH"]

WV_Res_SMZ_SIM_RESULT_AGG$CO2_Proj_HH <- WV_Res_SMZ_SIM_RESULT_AGG$CO2_base_HH_AGED + WV_Res_SMZ_SIM_RESULT_AGG$CO2_EST_ADJ

###Clean Final Data Set
WV_Res_SMZ_SIM_RESULT_AGG[is.na(WV_Res_SMZ_SIM_RESULT_AGG)] <- 0

###Write Final Res CO2 Output
write.csv(WV_Res_SMZ_SIM_RESULT_AGG[,c("SMZ_N","CO2_base_HH","CO2_Proj_HH")], file=(paste(OUTPATH,"WV_BEM_RES_CO2.csv",sep="")),row.names=FALSE)

###Take out the trash
file.remove(paste(TEMPATH,"RES_CO2_SAMPLE.csv",sep=""))
					  
			
###END WV_Res CO2###

###START DE_Res CO2###

##Create Count Dummy Variable Households
DE_Res_Base$HH <- 1

##### Estimate Probability of Residential Emissions
DE_Res_Base$CO2_PROB <- exp(wlogit$coef[1]+
						  wlogit$coef[2]*DE_Res_Base$htdd+
						  wlogit$coef[3]*DE_Res_Base$Age+
						  wlogit$coef[4]*DE_Res_Base$Beds+
						  wlogit$coef[5]*DE_Res_Base$SF1)/
						(1+exp(wlogit$coef[1]+
						  wlogit$coef[2]*DE_Res_Base$htdd+
						  wlogit$coef[3]*DE_Res_Base$Age+
						  wlogit$coef[4]*DE_Res_Base$Beds+
						  wlogit$coef[5]*DE_Res_Base$SF1))
###Replace Errors with Zero
DE_Res_Base[is.na(DE_Res_Base)] <- 0
DE_Res_Base[DE_Res_Base==-Inf] <- 0
DE_Res_Base[DE_Res_Base==Inf] <- 0

##### Select observations based on Probability of CO2 Emissions
DE_Res_Base2 <- DE_Res_Base[which(DE_Res_Base$CO2_PROB>.99),]

##### Select observations based on CO2 Emissions Greater than zero
RECS2 <- RECS[which(RECS$TCO2>0),]

#####Fit HH_ols
HH_ols <- lm(TCO2 ~ HD65 + AGE + SF1 + TOTSQFT + WALLTYPE4 + EQUIPM10 + EQUIPM3, data = RECS2)
summary(HH_ols)

##### Estimate Residential Emissions
DE_Res_Base2$CO2_EST <- HH_ols$coef[1]+
					  HH_ols$coef[2]*DE_Res_Base2$htdd+
					  HH_ols$coef[3]*DE_Res_Base2$Age+
					  HH_ols$coef[4]*DE_Res_Base2$SF1+
					  HH_ols$coef[5]*DE_Res_Base2$sqftstrc+
					  HH_ols$coef[6]*DE_Res_Base2$Stucco+
					  HH_ols$coef[7]*DE_Res_Base2$fire+
					  HH_ols$coef[8]*DE_Res_Base2$radiator

					  
##Count CO2 Emitting Households
DE_Res_Base2$HH <- NA
DE_Res_Base2$HH[DE_Res_Base2$CO2_EST>0] <- 1
DE_Res_Base2$HH[DE_Res_Base2$CO2_EST<=0] <- 0
				
####Aggregate data By SMZ
DE_Res_Base2_SMZ <-aggregate(cbind(DE_Res_Base2$CO2_EST, DE_Res_Base2$HH)~DE_Res_Base2$SMZ_N, data=DE_Res_Base2, sum, na.rm=TRUE)
names(DE_Res_Base2_SMZ)[1] <- "SMZ_N" 
names(DE_Res_Base2_SMZ)[2] <- "CO2_EST" 
names(DE_Res_Base2_SMZ)[3] <- "HH" 

DE_Res_Base2_SMZ_SQFT <-aggregate(cbind(DE_Res_Base2$sqftstrc)~DE_Res_Base2$SMZ_N, data=DE_Res_Base2, mean, na.rm=TRUE) 
names(DE_Res_Base2_SMZ_SQFT)[1] <- "SMZ_N" 
names(DE_Res_Base2_SMZ_SQFT)[2] <- "SQFT" 

###Rate Per HH
DE_Res_Base2_SMZ$CO2_Rate_HH <- DE_Res_Base2_SMZ$CO2_EST/DE_Res_Base2_SMZ$HH

# merge Rates and Activities
DE_Res_Base2_Activities_SMZ = merge(DE_Res_Base2_SMZ, Base_SMZ_Activities, by="SMZ_N")

###Apply Rates to Households
DE_Res_Base2_Activities_SMZ$CO2_base_HH <- DE_Res_Base2_Activities_SMZ$CO2_Rate_HH*DE_Res_Base2_Activities_SMZ$HH_BASE

###Replace Errors with Zero
DE_Res_Base2_Activities_SMZ[is.na(DE_Res_Base2_Activities_SMZ)] <- 0
DE_Res_Base2_Activities_SMZ[DE_Res_Base2_Activities_SMZ==-Inf] <- 0
DE_Res_Base2_Activities_SMZ[DE_Res_Base2_Activities_SMZ==Inf] <- 0

###Emission Rate Adjustment Factor
DE_Res_Base2_Activities_SMZ$Tot_HH_CO2 <- DE_Res_Base2_Activities_SMZ$CO2_base_HH # + DE_Res_Base2_Activities_SMZ$CO2_base_OFF + DE_Res_Base2_Activities_SMZ$CO2_base_OTH
Tot_BASE_HH_CO2 <- sum(DE_Res_Base2_Activities_SMZ$Tot_HH_CO2, na.rm = TRUE)*0.000001
CO2_HH_Rate_Adj<-(DE_RES_CO2con/Tot_BASE_HH_CO2)

###Adjusted Rate Per Household
DE_Res_Base2_Activities_SMZ$CO2_Rate_HH_ADJ <- DE_Res_Base2_Activities_SMZ$CO2_Rate_HH*CO2_HH_Rate_Adj

###Apply Adjusted Rates to Activities
DE_Res_Base2_Activities_SMZ$CO2_base_HH <- DE_Res_Base2_Activities_SMZ$CO2_Rate_HH_ADJ*DE_Res_Base2_Activities_SMZ$HH_BASE

###Sum Total Base Emissions
DE_Res_Base2_Activities_SMZ$CO2_base_HH_Total <- DE_Res_Base2_Activities_SMZ$CO2_base_HH #Leave for additional household type expansion

#Merge Base Data, Rates and Projected Activities
DE_Res_Base2_Activities_SMZ_PROJ = merge(DE_Res_Base2_Activities_SMZ, Proj_SMZ_Activities, by="SMZ_N")

# Export updated dataset
#write.csv(DE_Res_Base2_Activities_SMZ_PROJ,file="D:/BEM2/OUTPUT/check.csv")


###Calculate Projected Growth
PROJ_GROWTH_HH <- data.frame(DE_Res_Base2_Activities_SMZ_PROJ$SMZ_N, (DE_Res_Base2_Activities_SMZ_PROJ$HH_PROJ-DE_Res_Base2_Activities_SMZ_PROJ$HH_BASE))
names(PROJ_GROWTH_HH) <- c("SMZ_N","PROJ_GROWTH_HH")

############
###New Housing Stock Simulator
############
DE_Res_SMZ_SIM<-PROJ_GROWTH_HH

#Attach HDD and SQFT Data
DE_Res_SMZ_SIM$HDD = SMZ_HDD[match(DE_Res_SMZ_SIM$SMZ_N, SMZ_HDD$SMZ_N),"HDD"] 
DE_Res_SMZ_SIM$SQFT = DE_Res_Base2_SMZ_SQFT[match(DE_Res_SMZ_SIM$SMZ_N, DE_Res_Base2_SMZ_SQFT$SMZ_N),"SQFT"] 

#Sampling Function
sampHH<-function(x){

sample(PROJ_GROWTH_HH + 1, 1)

#Sim Housing Data
DE_Res_SMZ_SIM$Age <- sapply(DE_Res_SMZ_SIM$PROJ_GROWTH_HH*TIME + 1, sample, 1) - 1
DE_Res_SMZ_SIM$SF1 <- sapply(DE_Res_SMZ_SIM$PROJ_GROWTH_HH + 1, sample, 1) - 1
DE_Res_SMZ_SIM$sqftstrc <- sapply(DE_Res_SMZ_SIM$PROJ_GROWTH_HH*DE_Res_SMZ_SIM$SQFT + 1, sample, 1) - 1
DE_Res_SMZ_SIM$Stucco <- sapply(DE_Res_SMZ_SIM$PROJ_GROWTH_HH + 1, sample, 1) - 1
DE_Res_SMZ_SIM$fire <- sapply(DE_Res_SMZ_SIM$PROJ_GROWTH_HH + 1, sample, 1) - 1
DE_Res_SMZ_SIM$radiator <- sapply(DE_Res_SMZ_SIM$PROJ_GROWTH_HH + 1, sample, 1) - 1

##### Estimate New Stock Residential Emissions
DE_Res_SMZ_SIM$CO2_EST <- HH_ols$coef[1]+
					  HH_ols$coef[2]*(DE_Res_SMZ_SIM$HDD*DDCCRESULT$HDD)+
					  HH_ols$coef[3]*DE_Res_SMZ_SIM$Age+
					  HH_ols$coef[4]*DE_Res_SMZ_SIM$SF1+
					  HH_ols$coef[5]*DE_Res_SMZ_SIM$sqftstrc+
					  HH_ols$coef[6]*DE_Res_SMZ_SIM$Stucco+
					  HH_ols$coef[7]*DE_Res_SMZ_SIM$fire+
					  HH_ols$coef[8]*DE_Res_SMZ_SIM$radiator

###Apply Emissions Adjustment to Projected New Stock
DE_Res_SMZ_SIM$CO2_EST_ADJ <- DE_Res_SMZ_SIM$CO2_EST*CO2_HH_Rate_Adj
write.table(DE_Res_SMZ_SIM[c("SMZ_N","CO2_EST_ADJ")],file=(paste(TEMPATH,"RES_CO2_SAMPLE.csv",sep="")), sep=",", row.names=FALSE, col.names=FALSE, append=TRUE)
}

replicate(n = 10000, expr = sampHH())

DE_Res_SMZ_SIM_RESULT<- read.csv(paste(TEMPATH,"RES_CO2_SAMPLE.csv",sep="")) #Read back in simulated housing stock
names(DE_Res_SMZ_SIM_RESULT) <- c("SMZ_N","CO2_EST_ADJ")

####Aggregate CO2 data in simulated housing stock By SMZ
DE_Res_SMZ_SIM_RESULT_AGG <-aggregate(cbind(DE_Res_SMZ_SIM_RESULT$CO2_EST_ADJ)~DE_Res_SMZ_SIM_RESULT$SMZ_N, data=DE_Res_SMZ_SIM_RESULT, mean, na.rm=FALSE)
names(DE_Res_SMZ_SIM_RESULT_AGG)[1] <- "SMZ_N" 
names(DE_Res_SMZ_SIM_RESULT_AGG)[2] <- "CO2_EST_ADJ" 

#DE_Res_SMZ_SIM_RESULT_AGG_MAX <-aggregate(cbind(DE_Res_SMZ_SIM_RESULT2$CO2_EST_ADJ)~DE_Res_SMZ_SIM_RESULT2$SMZ_N, data=DE_Res_SMZ_SIM_RESULT2, max, na.rm=TRUE)
#names(DE_Res_SMZ_SIM_RESULT_AGG_MAX)[1] <- "SMZ_N" 
#names(DE_Res_SMZ_SIM_RESULT_AGG_MAX)[2] <- "CO2_EST_ADJ_MAX"

#DE_Res_SMZ_SIM_RESULT_AGG_MIN <-aggregate(cbind(DE_Res_SMZ_SI#M_RESULT2$CO2_EST_ADJ)~DE_Res_SMZ_SIM_RESULT2$SMZ_N, data=DE_Res_SMZ_SIM_RESULT2, min, na.rm=TRUE)
#names(DE_Res_SMZ_SIM_RESULT_AGG_MIN)[1] <- "SMZ_N" 
#names(DE_Res_SMZ_SIM_RESULT_AGG_MIN)[2] <- "CO2_EST_ADJ_MIN"

###Add MAX and MIN Emissions to Average
#DE_Res_SMZ_SIM_RESULT_AGG$CO2_EST_ADJ_MAX = DE_Res_SMZ_SIM_RESULT_AGG_MAX[match(DE_Res_SMZ_SIM_RESULT_AGG$SMZ_N, DE_Res_SMZ_SIM_RESULT_AGG_MAX$SMZ_N),"CO2_EST_ADJ_MAX"]
#DE_Res_SMZ_SIM_RESULT_AGG$CO2_EST_ADJ_MIN = DE_Res_SMZ_SIM_RESULT_AGG_MIN[match(DE_Res_SMZ_SIM_RESULT_AGG$SMZ_N, DE_Res_SMZ_SIM_RESULT_AGG_MIN$SMZ_N),"CO2_EST_ADJ_MIN"]


############
###Age Base Housing Stock
############

DE_Res_Base2_AGED <- DE_Res_Base2
##### Estimate Residential Emissions With added age constant
DE_Res_Base2_AGED$CO2_EST <- HH_ols$coef[1]+
					  HH_ols$coef[2]*(DE_Res_Base2$htdd*DDCCRESULT$HDD)+
					  HH_ols$coef[3]*(DE_Res_Base2$Age+TIME)+
					  HH_ols$coef[4]*DE_Res_Base2$SF1+
					  HH_ols$coef[5]*DE_Res_Base2$sqftstrc+
					  HH_ols$coef[6]*DE_Res_Base2$Stucco+
					  HH_ols$coef[7]*DE_Res_Base2$fire+
					  HH_ols$coef[8]*DE_Res_Base2$radiator
					  
##Count CO2 Emitting Households in the Aged housing stock
DE_Res_Base2_AGED$HH <- NA
DE_Res_Base2_AGED$HH[DE_Res_Base2$CO2_EST>0] <- 1
DE_Res_Base2_AGED$HH[DE_Res_Base2$CO2_EST<=0] <- 0
				
####Aggregate CO2 data in aged housing stock By SMZ
DE_Res_Base2_SMZ_AGED <-aggregate(cbind(DE_Res_Base2_AGED$CO2_EST, DE_Res_Base2_AGED$HH)~DE_Res_Base2_AGED$SMZ_N, data=DE_Res_Base2_AGED, sum, na.rm=TRUE)
names(DE_Res_Base2_SMZ_AGED)[1] <- "SMZ_N" 
names(DE_Res_Base2_SMZ_AGED)[2] <- "CO2_EST" 
names(DE_Res_Base2_SMZ_AGED)[3] <- "HH" 

###Calculate New Rate Per HH for Aged Housing Stock
DE_Res_Base2_SMZ_AGED$CO2_Rate_HH <- DE_Res_Base2_SMZ_AGED$CO2_EST/DE_Res_Base2_SMZ_AGED$HH

# Merge Rates and Activities for Aged Housing Stock
DE_Res_Base2_Activities_SMZ_AGED = merge(DE_Res_Base2_SMZ_AGED, Base_SMZ_Activities, by="SMZ_N")

###Apply New Rates to Aged Households
DE_Res_Base2_Activities_SMZ_AGED$CO2_base_HH <- DE_Res_Base2_Activities_SMZ_AGED$CO2_Rate_HH*DE_Res_Base2_Activities_SMZ_AGED$HH_BASE

###Replace Errors with Zero
DE_Res_Base2_Activities_SMZ_AGED[is.na(DE_Res_Base2_Activities_SMZ_AGED)] <- 0
DE_Res_Base2_Activities_SMZ_AGED[DE_Res_Base2_Activities_SMZ_AGED==-Inf] <- 0
DE_Res_Base2_Activities_SMZ_AGED[DE_Res_Base2_Activities_SMZ_AGED==Inf] <- 0

###Adjusted CO2 Rate Per Aged Household
DE_Res_Base2_Activities_SMZ_AGED$CO2_Rate_HH_ADJ <- DE_Res_Base2_Activities_SMZ_AGED$CO2_Rate_HH*CO2_HH_Rate_Adj

###Apply Adjusted Aged Rates to Activities
DE_Res_Base2_Activities_SMZ_AGED$CO2_base_HH_AGED <- DE_Res_Base2_Activities_SMZ_AGED$CO2_Rate_HH_ADJ*DE_Res_Base2_Activities_SMZ_AGED$HH_BASE

###Sum Total Aged Base Emissions
DE_Res_Base2_Activities_SMZ_AGED$CO2_base_HH_Total_AGED <- DE_Res_Base2_Activities_SMZ_AGED$CO2_base_HH_AGED #+ DE_Res_Base2_Activities_SMZ$CO2_base_OFF + DE_Res_Base2_Activities_SMZ$CO2_base_OTH

###Add Aged Base Emissions to Projected Emissions Growth
DE_Res_SMZ_SIM_RESULT_AGG$CO2_base_HH_AGED = DE_Res_Base2_Activities_SMZ_AGED[match(DE_Res_SMZ_SIM_RESULT_AGG$SMZ_N, DE_Res_Base2_Activities_SMZ_AGED$SMZ_N),"CO2_base_HH_AGED"]

###Add Base Emissions to Projected Emissions Growth
DE_Res_SMZ_SIM_RESULT_AGG$CO2_base_HH = DE_Res_Base2_Activities_SMZ[match(DE_Res_SMZ_SIM_RESULT_AGG$SMZ_N, DE_Res_Base2_Activities_SMZ$SMZ_N),"CO2_base_HH"]

DE_Res_SMZ_SIM_RESULT_AGG$CO2_Proj_HH <- DE_Res_SMZ_SIM_RESULT_AGG$CO2_base_HH_AGED + DE_Res_SMZ_SIM_RESULT_AGG$CO2_EST_ADJ

###Clean Final Data Set
DE_Res_SMZ_SIM_RESULT_AGG[is.na(DE_Res_SMZ_SIM_RESULT_AGG)] <- 0

###Write Final Res CO2 Output
write.csv(DE_Res_SMZ_SIM_RESULT_AGG[,c("SMZ_N","CO2_base_HH","CO2_Proj_HH")], file=(paste(OUTPATH,"DE_BEM_RES_CO2.csv",sep="")),row.names=FALSE)

###Take out the trash
file.remove(paste(TEMPATH,"RES_CO2_SAMPLE.csv",sep=""))
					  
			
###END DE_Res CO2###


###START DC_Res CO2###

##Create Count Dummy Variable Households
DC_Res_Base$HH <- 1

##### Estimate Probability of Residential Emissions
DC_Res_Base$CO2_PROB <- exp(wlogit$coef[1]+
						  wlogit$coef[2]*DC_Res_Base$htdd+
						  wlogit$coef[3]*DC_Res_Base$Age+
						  wlogit$coef[4]*DC_Res_Base$Beds+
						  wlogit$coef[5]*DC_Res_Base$SF1)/
						(1+exp(wlogit$coef[1]+
						  wlogit$coef[2]*DC_Res_Base$htdd+
						  wlogit$coef[3]*DC_Res_Base$Age+
						  wlogit$coef[4]*DC_Res_Base$Beds+
						  wlogit$coef[5]*DC_Res_Base$SF1))
###Replace Errors with Zero
DC_Res_Base[is.na(DC_Res_Base)] <- 0
DC_Res_Base[DC_Res_Base==-Inf] <- 0
DC_Res_Base[DC_Res_Base==Inf] <- 0

##### Select observations based on Probability of CO2 Emissions
DC_Res_Base2 <- DC_Res_Base[which(DC_Res_Base$CO2_PROB>.99),]

##### Select observations based on CO2 Emissions Greater than zero
RECS2 <- RECS[which(RECS$TCO2>0),]

#####Fit HH_ols
HH_ols <- lm(TCO2 ~ HD65 + AGE + SF1 + TOTSQFT + WALLTYPE4 + EQUIPM10 + EQUIPM3, data = RECS2)
summary(HH_ols)

##### Estimate Residential Emissions
DC_Res_Base2$CO2_EST <- HH_ols$coef[1]+
					  HH_ols$coef[2]*DC_Res_Base2$htdd+
					  HH_ols$coef[3]*DC_Res_Base2$Age+
					  HH_ols$coef[4]*DC_Res_Base2$SF1+
					  HH_ols$coef[5]*DC_Res_Base2$sqftstrc+
					  HH_ols$coef[6]*DC_Res_Base2$Stucco+
					  HH_ols$coef[7]*DC_Res_Base2$fire+
					  HH_ols$coef[8]*DC_Res_Base2$radiator

					  
##Count CO2 Emitting Households
DC_Res_Base2$HH <- NA
DC_Res_Base2$HH[DC_Res_Base2$CO2_EST>0] <- 1
DC_Res_Base2$HH[DC_Res_Base2$CO2_EST<=0] <- 0
				
####Aggregate data By SMZ
DC_Res_Base2_SMZ <-aggregate(cbind(DC_Res_Base2$CO2_EST, DC_Res_Base2$HH)~DC_Res_Base2$SMZ_N, data=DC_Res_Base2, sum, na.rm=TRUE)
names(DC_Res_Base2_SMZ)[1] <- "SMZ_N" 
names(DC_Res_Base2_SMZ)[2] <- "CO2_EST" 
names(DC_Res_Base2_SMZ)[3] <- "HH" 

DC_Res_Base2_SMZ_SQFT <-aggregate(cbind(DC_Res_Base2$sqftstrc)~DC_Res_Base2$SMZ_N, data=DC_Res_Base2, mean, na.rm=TRUE) 
names(DC_Res_Base2_SMZ_SQFT)[1] <- "SMZ_N" 
names(DC_Res_Base2_SMZ_SQFT)[2] <- "SQFT" 

###Rate Per HH
DC_Res_Base2_SMZ$CO2_Rate_HH <- DC_Res_Base2_SMZ$CO2_EST/DC_Res_Base2_SMZ$HH

# merge Rates and Activities
DC_Res_Base2_Activities_SMZ = merge(DC_Res_Base2_SMZ, Base_SMZ_Activities, by="SMZ_N")

###Apply Rates to Households
DC_Res_Base2_Activities_SMZ$CO2_base_HH <- DC_Res_Base2_Activities_SMZ$CO2_Rate_HH*DC_Res_Base2_Activities_SMZ$HH_BASE

###Replace Errors with Zero
DC_Res_Base2_Activities_SMZ[is.na(DC_Res_Base2_Activities_SMZ)] <- 0
DC_Res_Base2_Activities_SMZ[DC_Res_Base2_Activities_SMZ==-Inf] <- 0
DC_Res_Base2_Activities_SMZ[DC_Res_Base2_Activities_SMZ==Inf] <- 0

###Emission Rate Adjustment Factor
DC_Res_Base2_Activities_SMZ$Tot_HH_CO2 <- DC_Res_Base2_Activities_SMZ$CO2_base_HH # + DC_Res_Base2_Activities_SMZ$CO2_base_OFF + DC_Res_Base2_Activities_SMZ$CO2_base_OTH
Tot_BASE_HH_CO2 <- sum(DC_Res_Base2_Activities_SMZ$Tot_HH_CO2, na.rm = TRUE)*0.000001
CO2_HH_Rate_Adj<-(DC_RES_CO2con/Tot_BASE_HH_CO2)

###Adjusted Rate Per Household
DC_Res_Base2_Activities_SMZ$CO2_Rate_HH_ADJ <- DC_Res_Base2_Activities_SMZ$CO2_Rate_HH*CO2_HH_Rate_Adj

###Apply Adjusted Rates to Activities
DC_Res_Base2_Activities_SMZ$CO2_base_HH <- DC_Res_Base2_Activities_SMZ$CO2_Rate_HH_ADJ*DC_Res_Base2_Activities_SMZ$HH_BASE

###Sum Total Base Emissions
DC_Res_Base2_Activities_SMZ$CO2_base_HH_Total <- DC_Res_Base2_Activities_SMZ$CO2_base_HH #Leave for additional household type expansion

#Merge Base Data, Rates and Projected Activities
DC_Res_Base2_Activities_SMZ_PROJ = merge(DC_Res_Base2_Activities_SMZ, Proj_SMZ_Activities, by="SMZ_N")

# Export updated dataset
#write.csv(DC_Res_Base2_Activities_SMZ_PROJ,file="D:/BEM2/OUTPUT/check.csv")


###Calculate Projected Growth
PROJ_GROWTH_HH <- data.frame(DC_Res_Base2_Activities_SMZ_PROJ$SMZ_N, (DC_Res_Base2_Activities_SMZ_PROJ$HH_PROJ-DC_Res_Base2_Activities_SMZ_PROJ$HH_BASE))
names(PROJ_GROWTH_HH) <- c("SMZ_N","PROJ_GROWTH_HH")

############
###New Housing Stock Simulator
############
DC_Res_SMZ_SIM<-PROJ_GROWTH_HH

#Attach HDD and SQFT Data
DC_Res_SMZ_SIM$HDD = SMZ_HDD[match(DC_Res_SMZ_SIM$SMZ_N, SMZ_HDD$SMZ_N),"HDD"] 
DC_Res_SMZ_SIM$SQFT = DC_Res_Base2_SMZ_SQFT[match(DC_Res_SMZ_SIM$SMZ_N, DC_Res_Base2_SMZ_SQFT$SMZ_N),"SQFT"] 

#Sampling Function
sampHH<-function(x){

sample(PROJ_GROWTH_HH + 1, 1)

#Sim Housing Data
DC_Res_SMZ_SIM$Age <- sapply(DC_Res_SMZ_SIM$PROJ_GROWTH_HH*TIME + 1, sample, 1) - 1
DC_Res_SMZ_SIM$SF1 <- sapply(DC_Res_SMZ_SIM$PROJ_GROWTH_HH + 1, sample, 1) - 1
DC_Res_SMZ_SIM$sqftstrc <- sapply(DC_Res_SMZ_SIM$PROJ_GROWTH_HH*DC_Res_SMZ_SIM$SQFT + 1, sample, 1) - 1
DC_Res_SMZ_SIM$Stucco <- sapply(DC_Res_SMZ_SIM$PROJ_GROWTH_HH + 1, sample, 1) - 1
DC_Res_SMZ_SIM$fire <- sapply(DC_Res_SMZ_SIM$PROJ_GROWTH_HH + 1, sample, 1) - 1
DC_Res_SMZ_SIM$radiator <- sapply(DC_Res_SMZ_SIM$PROJ_GROWTH_HH + 1, sample, 1) - 1

##### Estimate New Stock Residential Emissions
DC_Res_SMZ_SIM$CO2_EST <- HH_ols$coef[1]+
					  HH_ols$coef[2]*(DC_Res_SMZ_SIM$HDD*DDCCRESULT$HDD)+
					  HH_ols$coef[3]*DC_Res_SMZ_SIM$Age+
					  HH_ols$coef[4]*DC_Res_SMZ_SIM$SF1+
					  HH_ols$coef[5]*DC_Res_SMZ_SIM$sqftstrc+
					  HH_ols$coef[6]*DC_Res_SMZ_SIM$Stucco+
					  HH_ols$coef[7]*DC_Res_SMZ_SIM$fire+
					  HH_ols$coef[8]*DC_Res_SMZ_SIM$radiator

###Apply Emissions Adjustment to Projected New Stock
DC_Res_SMZ_SIM$CO2_EST_ADJ <- DC_Res_SMZ_SIM$CO2_EST*CO2_HH_Rate_Adj
write.table(DC_Res_SMZ_SIM[c("SMZ_N","CO2_EST_ADJ")],file=(paste(TEMPATH,"RES_CO2_SAMPLE.csv",sep="")), sep=",", row.names=FALSE, col.names=FALSE, append=TRUE)
}

replicate(n = 10000, expr = sampHH())

DC_Res_SMZ_SIM_RESULT<- read.csv(paste(TEMPATH,"RES_CO2_SAMPLE.csv",sep="")) #Read back in simulated housing stock
names(DC_Res_SMZ_SIM_RESULT) <- c("SMZ_N","CO2_EST_ADJ")

####Aggregate CO2 data in simulated housing stock By SMZ
DC_Res_SMZ_SIM_RESULT_AGG <-aggregate(cbind(DC_Res_SMZ_SIM_RESULT$CO2_EST_ADJ)~DC_Res_SMZ_SIM_RESULT$SMZ_N, data=DC_Res_SMZ_SIM_RESULT, mean, na.rm=FALSE)
names(DC_Res_SMZ_SIM_RESULT_AGG)[1] <- "SMZ_N" 
names(DC_Res_SMZ_SIM_RESULT_AGG)[2] <- "CO2_EST_ADJ" 

#DC_Res_SMZ_SIM_RESULT_AGG_MAX <-aggregate(cbind(DC_Res_SMZ_SIM_RESULT2$CO2_EST_ADJ)~DC_Res_SMZ_SIM_RESULT2$SMZ_N, data=DC_Res_SMZ_SIM_RESULT2, max, na.rm=TRUE)
#names(DC_Res_SMZ_SIM_RESULT_AGG_MAX)[1] <- "SMZ_N" 
#names(DC_Res_SMZ_SIM_RESULT_AGG_MAX)[2] <- "CO2_EST_ADJ_MAX"

#DC_Res_SMZ_SIM_RESULT_AGG_MIN <-aggregate(cbind(DC_Res_SMZ_SI#M_RESULT2$CO2_EST_ADJ)~DC_Res_SMZ_SIM_RESULT2$SMZ_N, data=DC_Res_SMZ_SIM_RESULT2, min, na.rm=TRUE)
#names(DC_Res_SMZ_SIM_RESULT_AGG_MIN)[1] <- "SMZ_N" 
#names(DC_Res_SMZ_SIM_RESULT_AGG_MIN)[2] <- "CO2_EST_ADJ_MIN"

###Add MAX and MIN Emissions to Average
#DC_Res_SMZ_SIM_RESULT_AGG$CO2_EST_ADJ_MAX = DC_Res_SMZ_SIM_RESULT_AGG_MAX[match(DC_Res_SMZ_SIM_RESULT_AGG$SMZ_N, DC_Res_SMZ_SIM_RESULT_AGG_MAX$SMZ_N),"CO2_EST_ADJ_MAX"]
#DC_Res_SMZ_SIM_RESULT_AGG$CO2_EST_ADJ_MIN = DC_Res_SMZ_SIM_RESULT_AGG_MIN[match(DC_Res_SMZ_SIM_RESULT_AGG$SMZ_N, DC_Res_SMZ_SIM_RESULT_AGG_MIN$SMZ_N),"CO2_EST_ADJ_MIN"]


############
###Age Base Housing Stock
############

DC_Res_Base2_AGED <- DC_Res_Base2
##### Estimate Residential Emissions With added age constant
DC_Res_Base2_AGED$CO2_EST <- HH_ols$coef[1]+
					  HH_ols$coef[2]*(DC_Res_Base2$htdd*DDCCRESULT$HDD)+
					  HH_ols$coef[3]*(DC_Res_Base2$Age+TIME)+
					  HH_ols$coef[4]*DC_Res_Base2$SF1+
					  HH_ols$coef[5]*DC_Res_Base2$sqftstrc+
					  HH_ols$coef[6]*DC_Res_Base2$Stucco+
					  HH_ols$coef[7]*DC_Res_Base2$fire+
					  HH_ols$coef[8]*DC_Res_Base2$radiator
					  
##Count CO2 Emitting Households in the Aged housing stock
DC_Res_Base2_AGED$HH <- NA
DC_Res_Base2_AGED$HH[DC_Res_Base2$CO2_EST>0] <- 1
DC_Res_Base2_AGED$HH[DC_Res_Base2$CO2_EST<=0] <- 0
				
####Aggregate CO2 data in aged housing stock By SMZ
DC_Res_Base2_SMZ_AGED <-aggregate(cbind(DC_Res_Base2_AGED$CO2_EST, DC_Res_Base2_AGED$HH)~DC_Res_Base2_AGED$SMZ_N, data=DC_Res_Base2_AGED, sum, na.rm=TRUE)
names(DC_Res_Base2_SMZ_AGED)[1] <- "SMZ_N" 
names(DC_Res_Base2_SMZ_AGED)[2] <- "CO2_EST" 
names(DC_Res_Base2_SMZ_AGED)[3] <- "HH" 

###Calculate New Rate Per HH for Aged Housing Stock
DC_Res_Base2_SMZ_AGED$CO2_Rate_HH <- DC_Res_Base2_SMZ_AGED$CO2_EST/DC_Res_Base2_SMZ_AGED$HH

# Merge Rates and Activities for Aged Housing Stock
DC_Res_Base2_Activities_SMZ_AGED = merge(DC_Res_Base2_SMZ_AGED, Base_SMZ_Activities, by="SMZ_N")

###Apply New Rates to Aged Households
DC_Res_Base2_Activities_SMZ_AGED$CO2_base_HH <- DC_Res_Base2_Activities_SMZ_AGED$CO2_Rate_HH*DC_Res_Base2_Activities_SMZ_AGED$HH_BASE

###Replace Errors with Zero
DC_Res_Base2_Activities_SMZ_AGED[is.na(DC_Res_Base2_Activities_SMZ_AGED)] <- 0
DC_Res_Base2_Activities_SMZ_AGED[DC_Res_Base2_Activities_SMZ_AGED==-Inf] <- 0
DC_Res_Base2_Activities_SMZ_AGED[DC_Res_Base2_Activities_SMZ_AGED==Inf] <- 0

###Adjusted CO2 Rate Per Aged Household
DC_Res_Base2_Activities_SMZ_AGED$CO2_Rate_HH_ADJ <- DC_Res_Base2_Activities_SMZ_AGED$CO2_Rate_HH*CO2_HH_Rate_Adj

###Apply Adjusted Aged Rates to Activities
DC_Res_Base2_Activities_SMZ_AGED$CO2_base_HH_AGED <- DC_Res_Base2_Activities_SMZ_AGED$CO2_Rate_HH_ADJ*DC_Res_Base2_Activities_SMZ_AGED$HH_BASE

###Sum Total Aged Base Emissions
DC_Res_Base2_Activities_SMZ_AGED$CO2_base_HH_Total_AGED <- DC_Res_Base2_Activities_SMZ_AGED$CO2_base_HH_AGED #+ DC_Res_Base2_Activities_SMZ$CO2_base_OFF + DC_Res_Base2_Activities_SMZ$CO2_base_OTH

###Add Aged Base Emissions to Projected Emissions Growth
DC_Res_SMZ_SIM_RESULT_AGG$CO2_base_HH_AGED = DC_Res_Base2_Activities_SMZ_AGED[match(DC_Res_SMZ_SIM_RESULT_AGG$SMZ_N, DC_Res_Base2_Activities_SMZ_AGED$SMZ_N),"CO2_base_HH_AGED"]

###Add Base Emissions to Projected Emissions Growth
DC_Res_SMZ_SIM_RESULT_AGG$CO2_base_HH = DC_Res_Base2_Activities_SMZ[match(DC_Res_SMZ_SIM_RESULT_AGG$SMZ_N, DC_Res_Base2_Activities_SMZ$SMZ_N),"CO2_base_HH"]

DC_Res_SMZ_SIM_RESULT_AGG$CO2_Proj_HH <- DC_Res_SMZ_SIM_RESULT_AGG$CO2_base_HH_AGED + DC_Res_SMZ_SIM_RESULT_AGG$CO2_EST_ADJ

###Clean Final Data Set
DC_Res_SMZ_SIM_RESULT_AGG[is.na(DC_Res_SMZ_SIM_RESULT_AGG)] <- 0

###Write Final Res CO2 Output
write.csv(DC_Res_SMZ_SIM_RESULT_AGG[,c("SMZ_N","CO2_base_HH","CO2_Proj_HH")], file=(paste(OUTPATH,"DC_BEM_RES_CO2.csv",sep="")),row.names=FALSE)

###Take out the trash
file.remove(paste(TEMPATH,"RES_CO2_SAMPLE.csv",sep=""))
					  
			
###END DC_Res CO2###

			
##############
###END Residential CO2 MODEL
##############

############
#Format Final CO2 Output
############
MD_Com_CO2 <- read.csv(paste(OUTPATH,"MD_BEM_Com_CO2.csv",sep="")) #Final Commercial CO2 File
VA_Com_CO2 <- read.csv(paste(OUTPATH,"VA_BEM_Com_CO2.csv",sep="")) #Final Commercial CO2 File
PA_Com_CO2 <- read.csv(paste(OUTPATH,"PA_BEM_Com_CO2.csv",sep="")) #Final Commercial CO2 File
WV_Com_CO2 <- read.csv(paste(OUTPATH,"WV_BEM_Com_CO2.csv",sep="")) #Final Commercial CO2 File
DC_Com_CO2 <- read.csv(paste(OUTPATH,"DC_BEM_Com_CO2.csv",sep="")) #Final Commercial CO2 File
DE_Com_CO2 <- read.csv(paste(OUTPATH,"DE_BEM_Com_CO2.csv",sep="")) #Final Commercial CO2 File

ALL_Com_CO2 <- rbind(MD_Com_CO2,VA_Com_CO2,PA_Com_CO2,WV_Com_CO2,DC_Com_CO2,DE_Com_CO2)

MD_Res_CO2 <- read.csv(paste(OUTPATH,"MD_BEM_Res_CO2.csv",sep="")) #Final Resmercial CO2 File
VA_Res_CO2 <- read.csv(paste(OUTPATH,"VA_BEM_Res_CO2.csv",sep="")) #Final Resmercial CO2 File
PA_Res_CO2 <- read.csv(paste(OUTPATH,"PA_BEM_Res_CO2.csv",sep="")) #Final Resmercial CO2 File
WV_Res_CO2 <- read.csv(paste(OUTPATH,"WV_BEM_Res_CO2.csv",sep="")) #Final Resmercial CO2 File
DC_Res_CO2 <- read.csv(paste(OUTPATH,"DC_BEM_Res_CO2.csv",sep="")) #Final Resmercial CO2 File
DE_Res_CO2 <- read.csv(paste(OUTPATH,"DE_BEM_Res_CO2.csv",sep="")) #Final Resmercial CO2 File

ALL_Res_CO2 <- rbind(MD_Res_CO2,VA_Res_CO2,PA_Res_CO2,WV_Res_CO2,DC_Res_CO2,DE_Res_CO2)

BlankSMZ[is.na(BlankSMZ)] <- 0

#Merge Final Data
BlankSMZ$COM_BASE_CO2 = ALL_Com_CO2[match(BlankSMZ$SMZ_N, ALL_Com_CO2$SMZ_N),"CO2_base_Com"]
BlankSMZ$RES_BASE_CO2 = ALL_Res_CO2[match(BlankSMZ$SMZ_N, ALL_Res_CO2$SMZ_N),"CO2_base_HH"]
BlankSMZ$COM_PROJ_CO2 = ALL_Com_CO2[match(BlankSMZ$SMZ_N, ALL_Com_CO2$SMZ_N),"CO2_Proj_Com"]
BlankSMZ$RES_PROJ_CO2 = ALL_Res_CO2[match(BlankSMZ$SMZ_N, ALL_Res_CO2$SMZ_N),"CO2_Proj_HH"]

#Clean-up
BlankSMZ[is.na(BlankSMZ)] <- 0
BlankSMZ[BlankSMZ<0] <- 0

#Sum CO2 Results
BlankSMZ$BASE_TOTAL_CO2 <- BlankSMZ$COM_BASE_CO2 + BlankSMZ$RES_BASE_CO2
BlankSMZ$PROJ_TOTAL_CO2 <- BlankSMZ$COM_PROJ_CO2 + BlankSMZ$RES_PROJ_CO2

###Write Final  CO2 Output
OUTHEADBASE<-paste(BASEYEAR,"_TOTCO2",sep="")
OUTHEADPROJ<-paste(PROJYEAR,"_TOTCO2",sep="")

if(Parameters$TempratureChange==0) {
   write.table(BlankSMZ[,c("SMZ_N","COM_BASE_CO2","RES_BASE_CO2","COM_PROJ_CO2","RES_PROJ_CO2","BASE_TOTAL_CO2","PROJ_TOTAL_CO2")], file=(paste(OUTPATH,sprintf("BEM_CO2_FINAL_%s", SCENARIO),sprintf("_%s", BASEYEAR),sprintf("-%s.csv", PROJYEAR),sep="")),row.names=FALSE, sep=",")

} else {
   write.table(BlankSMZ[,c("SMZ_N","COM_BASE_CO2","RES_BASE_CO2","COM_PROJ_CO2","RES_PROJ_CO2","BASE_TOTAL_CO2","PROJ_TOTAL_CO2")], file=(paste(OUTPATH,sprintf("BEM_CO2_FINAL_CLIMATE-CHANGE_%s", SCENARIO),sprintf("_%s", BASEYEAR),sprintf("-%s.csv", PROJYEAR),sep="")),row.names=FALSE, sep=",")

}

##########################
###MODEL RUN COMPLETE!###
